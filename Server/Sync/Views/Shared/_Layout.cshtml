<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Style.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.tablesorter.min.js")" type="text/javascript"></script>
    <link rel="Stylesheet" type="text/css" href="@Url.Content("~/Content/jquery-ui-1.8.11.css")" />
    <link rel="shortcut icon" href="~/Content/Images/new_icon_01.ICO" type="image/png">
</head>
<body>
    <div class="page">
        <div id="header">
            <div id="title">
                <h1>Сервер сети ТОНУС-КЛУБ</h1>
            </div>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            <div id="menucontainer">
                <ul id="menu">
                    <li>@Html.ActionLink("Синхронизация и лицензирование", "Index", "Home")</li>
                    <li>@Html.ActionLink("Баг-трекер", "Index", "Bugs")</li>
                    <li>@Html.ActionLink("О системе", "About", "Home")</li>
                </ul>
            </div>
        </div>
        <div id="main">
            @RenderBody()
        </div>
        <div id="footer">
        </div>
    </div>
</body>
</html>
