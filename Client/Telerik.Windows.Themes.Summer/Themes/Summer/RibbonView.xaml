	<Style x:Key="ApplicationRibbonButton" TargetType="telerikRibbonView:RadRibbonButton">
		<Setter Property="Padding" Value="5" />
		<Setter Property="BorderThickness" Value="1.5" />
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="HorizontalAlignment" Value="Stretch" />
		<Setter Property="HorizontalContentAlignment" Value="Left" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Size" Value="Large" />
		<!--<Setter Property="SplitText" Value="False" />-->
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:RadRibbonButton">
					<Grid>
						
						<Border x:Name="buttonSurface" BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}"
								CornerRadius="1">
							<Border x:Name="buttonSurfaceInner" BorderBrush="Transparent" BorderThickness="1" />
						</Border>

						<Grid Margin="{TemplateBinding Padding}">
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
							</Grid.ColumnDefinitions>

							<ContentPresenter Content="" Grid.ColumnSpan="2" />

							<Image Name="Image" />

							<TextBlock Name="textRow1" Grid.Column="1" Margin="4,0,1,0" Text="{TemplateBinding Text}"
									TextAlignment="Left" VerticalAlignment="Center" />
						</Grid>

						<Rectangle x:Name="DisabledIndicator" Visibility="Collapsed" Fill="#99FFFFFF" />
					</Grid>

					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="true">
							<Setter Property="BorderBrush" TargetName="buttonSurface"
									Value="{StaticResource ApplicationRibbonButtonOverBorder}" />
							<Setter Property="Background" TargetName="buttonSurface"
									Value="{StaticResource ApplicationRibbonButtonOverBackground}" />
							<Setter Property="BorderBrush" TargetName="buttonSurfaceInner"
									Value="{StaticResource ApplicationRibbonButtonOverBorderInner}" />
						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter Property="BorderBrush" TargetName="buttonSurface"
									Value="{StaticResource ApplicationRibbonButtonOverBorder}" />
							<Setter Property="Background" TargetName="buttonSurface"
									Value="{StaticResource ApplicationRibbonButtonOverBackground}" />
							<Setter Property="BorderBrush" TargetName="buttonSurfaceInner"
									Value="{StaticResource ApplicationRibbonButtonOverBorderInner}" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
							<Setter TargetName="DisabledIndicator" Property="Visibility" Value="Visible" />
						</Trigger>
						<!--<Trigger Property="TextAlignmentState" Value="TextHide">
							<Setter TargetName="textRow1" Property="Visibility" Value="Collapsed" />
						</Trigger>-->
					</ControlTemplate.Triggers>
					

				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style x:Key="ApplicationRibbonDropDownButton" TargetType="telerikRibbonView:RadRibbonDropDownButton">
		<Setter Property="Padding" Value="5" />
		<Setter Property="BorderThickness" Value="1.5" />
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="HorizontalAlignment" Value="Stretch" />
		<Setter Property="HorizontalContentAlignment" Value="Left" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Size" Value="Large" />
		<!--<Setter Property="SplitText" Value="False" />-->
		<Setter Property="AutoOpenDelay" Value="0:0:0.3" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:RadRibbonDropDownButton">
					<Grid SnapsToDevicePixels="True"
						 >
						
						<Border x:Name="buttonSurface" BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}"
								CornerRadius="1">
							<Border x:Name="buttonSurfaceInner" BorderBrush="Transparent"
									BorderThickness="{TemplateBinding BorderThickness}" />
						</Border>

						<Border x:Name="isOpenIndicator" Visibility="Collapsed"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{StaticResource ApplicationRibbonButtonOverBorder}"
								Background="{StaticResource ApplicationRibbonButtonOverBackground}" CornerRadius="1">
							<Border BorderBrush="{StaticResource ApplicationRibbonButtonOverBorderInner}"
									BorderThickness="{TemplateBinding BorderThickness}" />
						</Border>

						<Border x:Name="mouseOverIndicator" CornerRadius="1"
								BorderThickness="{TemplateBinding BorderThickness}" />

						<Grid Margin="{TemplateBinding Padding}">
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="22" />
							</Grid.ColumnDefinitions>

							<ContentPresenter Grid.ColumnSpan="3"
									HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
									VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
									Content="{TemplateBinding Content}"
									ContentTemplate="{TemplateBinding ContentTemplate}" />

							<Image Name="Image" />

							<TextBlock Name="textRow1" Grid.Column="1" Margin="4,0,1,0" Text="{TemplateBinding Text}"
									TextAlignment="Left" VerticalAlignment="Center" />

							<Border Name="DropDownPanel" Grid.Column="2" VerticalAlignment="Stretch">
								<Path Name="DropDownIndicator" VerticalAlignment="Center" HorizontalAlignment="Center"
										Data="M 0,0 L 3.5,3.5 L 0,7" Margin="2,0,0,0"
										Visibility="{TemplateBinding DropDownIndicatorVisibility}"
										Fill="{StaticResource RibbonButtonDropDownArrow}"
										Stroke="{StaticResource RibbonButtonDropDownArrowBorder}" />
							</Border>
						</Grid>
						
						<Popup x:Name="DropDownPopup" HorizontalOffset="2" VerticalOffset="-1" AllowsTransparency="True"
								StaysOpen="False" Focusable="false" Placement="Bottom"
								IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
							
							
								<!--PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}">-->
								<Grid>

									<Border x:Name="DropDownPopupBorder"
											BorderBrush="{StaticResource DropDownPopupBorderBrush}"
											Background="{StaticResource DropDownPopupBackgroundBrush}"
											BorderThickness="1" MinWidth="3" Width="{TemplateBinding DropDownWidth}"
											MinHeight="3" Height="{TemplateBinding DropDownHeight}">
										<ContentControl x:Name="DropDownPopupContent"
												Content="{TemplateBinding DropDownContent}"
												HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
									</Border>
								</Grid>
								
						</Popup>
						
						

						<Rectangle x:Name="DisabledIndicator" Visibility="Collapsed" Fill="#99FFFFFF" />
					</Grid>

					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="true">
							<Setter Property="BorderBrush" TargetName="buttonSurface"
									Value="{StaticResource ApplicationRibbonButtonOverBorder}" />
							<Setter Property="Background" TargetName="buttonSurface"
									Value="{StaticResource ApplicationRibbonButtonOverBackground}" />
							<Setter Property="BorderBrush" TargetName="buttonSurfaceInner"
									Value="{StaticResource ApplicationRibbonButtonOverBorderInner}" />
						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter Property="BorderBrush" TargetName="buttonSurface"
									Value="{StaticResource ApplicationRibbonButtonOverBorder}" />
							<Setter Property="Background" TargetName="buttonSurface"
									Value="{StaticResource ApplicationRibbonButtonOverBackground}" />
							<Setter Property="BorderBrush" TargetName="buttonSurfaceInner"
									Value="{StaticResource ApplicationRibbonButtonOverBorderInner}" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
							<Setter TargetName="DisabledIndicator" Property="Visibility" Value="Visible" />
						</Trigger>
						<!--<Trigger Property="TextAlignmentState" Value="TextHide">
							<Setter TargetName="textRow1" Property="Visibility" Value="Collapsed" />
						</Trigger>-->
						<Trigger Property="IsOpen" Value="True">
							<Setter TargetName="isOpenIndicator" Property="Visibility" Value="Visible" />
						</Trigger>
					</ControlTemplate.Triggers>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<ControlTemplate TargetType="telerik:RadButton" x:Key="AppMenuRadSplitButton_ButtonStyle">
		<Grid>
			

			<Border x:Name="backgroundDecorator" BorderThickness="{TemplateBinding BorderThickness}"
					BorderBrush="{TemplateBinding BorderBrush}" Background="{x:Null}" CornerRadius="2">
				<Border x:Name="backgroundDecoratorInner" Margin="0,0,0,0"
						BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="Transparent"
						Background="{TemplateBinding Background}" CornerRadius="1">
					<ContentPresenter x:Name="Content" Content="{TemplateBinding Content}"
							ContentTemplate="{TemplateBinding ContentTemplate}"
							VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
							HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
							Margin="{TemplateBinding Padding}"
							RecognizesAccessKey="True"
						 />
				</Border>
			</Border>
		</Grid>

		
		<ControlTemplate.Triggers>
			<Trigger Property="IsEnabled" Value="False" />
			<Trigger Property="IsDefault" Value="True" />
			<Trigger Property="IsMouseOver" Value="True">
				<Setter Property="BorderBrush" TargetName="backgroundDecorator"
						Value="{StaticResource RadRibbonBarButtonOverBorder}" />
				<Setter Property="BorderBrush" TargetName="backgroundDecoratorInner"
						Value="{StaticResource RadRibbonBarButtonOverBorderInner}" />
				<Setter Property="Background" TargetName="backgroundDecoratorInner"
						Value="{StaticResource RadRibbonBarButtonOverBackground}" />
			</Trigger>
			<Trigger Property="IsPressed" Value="True">
				<Setter Property="BorderBrush" TargetName="backgroundDecorator"
						Value="{StaticResource RadRibbonBarButtonOverBorder}" />
				<Setter Property="BorderBrush" TargetName="backgroundDecoratorInner"
						Value="{StaticResource RadRibbonBarButtonOverBorderInner}" />
				<Setter Property="Background" TargetName="backgroundDecoratorInner"
						Value="{StaticResource RadRibbonBarButtonOverBackground}" />
			</Trigger>
		</ControlTemplate.Triggers>
		
	</ControlTemplate>

	<ControlTemplate TargetType="telerik:RadToggleButton" x:Key="AppMenuRadSplitButton_DropDownStyle">
		<Grid>
			

			<Border x:Name="backgroundDecorator" BorderThickness="{TemplateBinding BorderThickness}"
					BorderBrush="{TemplateBinding BorderBrush}" Background="{x:Null}" CornerRadius="2">
				<Border x:Name="backgroundDecoratorInner" Margin="0,0,0,0"
						BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="Transparent"
						Background="{TemplateBinding Background}" CornerRadius="1">
				</Border>
			</Border>

			<Border Opacity="0" x:Name="mouseOverDecorator" Background="{x:Null}"
					BorderThickness="{TemplateBinding BorderThickness}"
					BorderBrush="{StaticResource RadRibbonBarButtonOverBorder}" CornerRadius="2">
				<Border x:Name="mouseOverDecoratorInner" Margin="0,0,0,0" CornerRadius="1"
						BorderThickness="{TemplateBinding BorderThickness}"
						BorderBrush="{StaticResource RadRibbonBarButtonOverBorderInner}"
						Background="{StaticResource RadRibbonBarButtonOverBackground}">
				</Border>
			</Border>
			<Border BorderThickness="{TemplateBinding BorderThickness}">
				<ContentPresenter x:Name="contentPresenter" Content="{TemplateBinding Content}"
						ContentTemplate="{TemplateBinding ContentTemplate}"
						VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
						HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
						Margin="{TemplateBinding Padding}"
					RecognizesAccessKey="True"
					 />
			</Border>
		</Grid>

		
		<ControlTemplate.Triggers>
			<Trigger Property="IsEnabled" Value="False" />
			<Trigger Property="IsFocused" Value="True" />
			<Trigger Property="IsMouseOver" Value="True">
				<Setter Property="Opacity" TargetName="mouseOverDecorator" Value="1" />
			</Trigger>
			<Trigger Property="IsPressed" Value="True">
				<Setter Property="Opacity" TargetName="mouseOverDecorator" Value="1" />
				<!--<Setter Property="BorderBrush" TargetName="mouseOverDecorator" Value="{StaticResource RadRibbonBarButtonPressedBorder}"/>
				<Setter Property="BorderBrush" TargetName="mouseOverDecoratorInner" Value="{StaticResource RadRibbonBarButtonPressedBorderInner}"/>
				<Setter Property="Background" TargetName="mouseOverDecoratorInner" Value="{StaticResource RadRibbonBarButtonPressedBackground}"/>-->
			</Trigger>
			<Trigger Property="IsChecked" Value="True">
				<Setter Property="Opacity" TargetName="mouseOverDecorator" Value="1" />
				<!--<Setter Property="BorderBrush" TargetName="backgroundDecorator" Value="{StaticResource RadRibbonBarButtonPressedBorder}"/>
				<Setter Property="BorderBrush" TargetName="backgroundDecoratorInner" Value="{StaticResource RadRibbonBarButtonPressedBorderInner}"/>
				<Setter Property="Background" TargetName="backgroundDecoratorInner" Value="{StaticResource RadRibbonBarButtonPressedBackground}"/>-->
			</Trigger>
		</ControlTemplate.Triggers>
		
	</ControlTemplate>

	<Style x:Key="ApplicationRibbonSplitButton" TargetType="telerikRibbonView:RadRibbonSplitButton">
		<Setter Property="IsTabStop" Value="false" />
		<Setter Property="IsOpen" Value="false" />
		<Setter Property="Padding" Value="5" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="HorizontalAlignment" Value="Left" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		
		
		<Setter Property="DropDownPlacement" Value="Relative" />
		
		<Setter Property="Size" Value="Large" />
		<!--<Setter Property="SplitText" Value="False" />-->
		<Setter Property="AutoOpenDelay" Value="0:0:0.3" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:RadRibbonSplitButton">
					<Grid SnapsToDevicePixels="True"
						 >
						

						<Border x:Name="backgroundDecorator" BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}" Background="{x:Null}" CornerRadius="1">
							<Border x:Name="backgroundDecoratorInner"
									BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="Transparent"
									Background="{TemplateBinding Background}" />
						</Border>

						<Border x:Name="mouseOverDecorator" Opacity="0"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{StaticResource ApplicationRibbonButtonOverBorder}" Background="{x:Null}"
								CornerRadius="1">
							<Border x:Name="mouseOverDecoratorInner" BorderThickness="{TemplateBinding BorderThickness}"
									BorderBrush="{StaticResource ApplicationRibbonButtonOverBorderInner}"
									Background="{StaticResource ApplicationRibbonButtonOverBackground}" />
						</Border>

						<Grid Margin="1">
							<Grid.RowDefinitions>
								<RowDefinition Height="*" />
								<RowDefinition Height="Auto" MaxHeight="28" />
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="Auto" />
							</Grid.ColumnDefinitions>

							<telerik:RadButton x:Name="ButtonPart" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2"
									BorderThickness="0" Background="Transparent" Content="{TemplateBinding Content}"
									ContentTemplate="{TemplateBinding ContentTemplate}"
									HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
									VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
									Template="{StaticResource AppMenuRadSplitButton_ButtonStyle}"
									Padding="{TemplateBinding Padding}" />
							<telerik:RadToggleButton x:Name="DropDownPart" Grid.Row="0" Grid.Column="2"
									BorderThickness="0" Margin="0" Background="Transparent"
									Visibility="{TemplateBinding DropDownIndicatorVisibility}"
									Template="{StaticResource AppMenuRadSplitButton_DropDownStyle}" IsTabStop="false"
								
								
								IsChecked="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
								 />
								<Rectangle x:Name="SeparatorPart" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2"
										MinWidth="1" MinHeight="1" Visibility="Collapsed" IsHitTestVisible="False"
										HorizontalAlignment="Right" VerticalAlignment="Stretch"
										Fill="{StaticResource RadRibbonBarDropDownButtonPressedBorder}" />

								<Image Name="Image" IsHitTestVisible="False" Margin="4" />

								<TextBlock Name="textRow1" Grid.Row="0" Grid.Column="1" Margin="0,0,2,0"
										IsHitTestVisible="False" Text="{TemplateBinding Text}" TextAlignment="Left"
										VerticalAlignment="Center" />

								<Grid Name="DropDownPanel" Grid.Row="0" Grid.Column="2" Margin="0" Width="22"
										IsHitTestVisible="False">
									<Path Name="DropDownIndicator" VerticalAlignment="Center"
											HorizontalAlignment="Center" Data="M 0,0 L 3.5,3.5 L 0,7" Margin="0"
											IsHitTestVisible="False"
											Visibility="{TemplateBinding DropDownIndicatorVisibility}"
											Fill="{StaticResource RibbonButtonDropDownArrow}"
											Stroke="{StaticResource RibbonButtonDropDownArrowBorder}" />
								</Grid>
						</Grid>
						
						<Popup x:Name="DropDownPopup" HorizontalOffset="2" VerticalOffset="-1" AllowsTransparency="True"
								StaysOpen="False" Focusable="false" Placement="Bottom"
								IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
							
							
								<!--PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}">-->
								<Grid>
									<!--<telerikRibbonViewPrimitives:DropShadow telerik:StyleManager.Theme="{StaticResource Theme}" Margin="6,6,4,4" />-->
									<Border x:Name="DropDownPopupBorder"
											BorderBrush="{StaticResource DropDownPopupBorderBrush}"
											Background="{StaticResource DropDownPopupBackgroundBrush}"
											BorderThickness="1" MinWidth="3" Width="{TemplateBinding DropDownWidth}"
											MinHeight="3" Height="{TemplateBinding DropDownHeight}">
										<ContentControl x:Name="DropDownPopupContent"
												Content="{TemplateBinding DropDownContent}"
												HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
									</Border>
								</Grid>
								
						</Popup>
						
						

						<Rectangle x:Name="disabledDecorator" Visibility="Collapsed" Opacity="0" Fill="#FFFFFFFF" />
					</Grid>

					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="true">
							<Setter TargetName="mouseOverDecorator" Property="Opacity" Value="1" />
							<Setter TargetName="SeparatorPart" Property="Visibility" Value="Visible" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
							<Setter TargetName="disabledDecorator" Property="Opacity" Value="0.55" />
							<Setter TargetName="disabledDecorator" Property="Visibility" Value="Visible" />
						</Trigger>
						<Trigger Property="IsFocused" Value="True" />
						<Trigger Property="IsOpen" Value="True">
							<Setter TargetName="mouseOverDecorator" Property="Opacity" Value="1" />
							<Setter TargetName="SeparatorPart" Property="Visibility" Value="Visible" />
						</Trigger>
						<Trigger Property="DropDownButtonPosition" Value="Top" />
						<Trigger Property="DropDownButtonPosition" Value="Left" />
						<Trigger Property="DropDownButtonPosition" Value="Bottom" />
						<Trigger Property="DropDownButtonPosition" Value="Right" />
						<Trigger Property="CurrentSize" Value="Large" />
						<Trigger Property="CurrentSize" Value="Medium" />
						<Trigger Property="CurrentSize" Value="Small" />
						<Trigger Property="DropDownPlacement" Value="Top" />
						<Trigger Property="DropDownPlacement" Value="Left" />
						<Trigger Property="DropDownPlacement" Value="Right" />
						<Trigger Property="DropDownPlacement" Value="Bottom" />
						<Trigger Property="DropDownPlacement" Value="Center" />
						<Trigger Property="DropDownPlacement" Value="Relative">
							<Setter TargetName="DropDownPopup" Property="Placement" Value="Relative" />
						</Trigger>
					</ControlTemplate.Triggers>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="telerikRibbonView:Separator" x:Key="ApplicationSeparatorStyle">
		<Setter Property="Background" Value="#f5f5f5" />
		<Setter Property="BorderThickness" Value="0,1,0,0" />
		<Setter Property="BorderBrush" Value="#c5c5c5" />
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Height" Value="2" />
		<Setter Property="Margin" Value="44,0,0,0" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:Separator">
					<Border BorderThickness="{TemplateBinding BorderThickness}"
							BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" />
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="telerikRibbonView:ApplicationMenu"
           x:Key="{telerik:ThemeResourceKey ThemeType=
		external:SummerThemeExternal , ElementType=telerikRibbonView:ApplicationMenu}"
		 >
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Background" Value="{StaticResource RibbonPopupBackgroundBrush}" />
		<Setter Property="BorderBrush" Value="{StaticResource ApplicationMenuOuterBorderBrush}" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="Foreground" Value="{StaticResource RibbonPopupForegroundBrush}" />
		<Setter Property="ItemContainerStyleSelector">
			<Setter.Value>
				<telerikRibbonView:CustomContainerStyleSelector>
					<telerikRibbonView:CustomContainerStyle TypeName="RadRibbonButton"
							ContainerStyle="{StaticResource ApplicationRibbonButton}" />
					<telerikRibbonView:CustomContainerStyle TypeName="RadRibbonDropDownButton"
							ContainerStyle="{StaticResource ApplicationRibbonDropDownButton}" />
					<telerikRibbonView:CustomContainerStyle TypeName="RadRibbonSplitButton"
							ContainerStyle="{StaticResource ApplicationRibbonSplitButton}" />
					<telerikRibbonView:CustomContainerStyle TypeName="Separator"
							ContainerStyle="{StaticResource ApplicationSeparatorStyle}" />
				</telerikRibbonView:CustomContainerStyleSelector>
			</Setter.Value>
		</Setter>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:ApplicationMenu">
					<Grid x:Name="rootGrid" SnapsToDevicePixels="True"
						 >
						<Border Name="PART_OuterBorder" CornerRadius="1" BorderThickness="1"
								BorderBrush="{TemplateBinding BorderBrush}" Background="Transparent">
							<Grid>
								<Grid.RowDefinitions>
									<RowDefinition Height="Auto" />
									<RowDefinition Height="*" />
									<RowDefinition Height="Auto" />
								</Grid.RowDefinitions>
								<Rectangle x:Name="clippingRectangle" Margin="45 0 0 0" Grid.Row="0" Height="21"
										Fill="{StaticResource ApplicationMenuTopBorderBrush}" />
								
								<Rectangle x:Name="buttonRectangle" Grid.Row="0" Height="21" Width="44"
										HorizontalAlignment="Left" Fill="Transparent" />
								
								<Border Grid.Row="1" Background="{StaticResource ApplicationMenuMiddleBorderBrush}">
									<Border Margin="2,0,2,0" BorderThickness="1"
											BorderBrush="{StaticResource ApplicationMenuContentOuterBorderBrush}">
										<Border BorderThickness="1"
												BorderBrush="{StaticResource ApplicationMenuContentInnerBorderBrush}">
											<Grid>
												<Grid.ColumnDefinitions>
													<ColumnDefinition></ColumnDefinition>
													<ColumnDefinition Width="Auto"></ColumnDefinition>
												</Grid.ColumnDefinitions>
												<Border Name="LeftPane" BorderBrush="#c5c5c5"
														Background="{TemplateBinding Background}"
														BorderThickness="0,0,1,0">
													<ItemsPresenter />
												</Border>
												<Border Name="RightPane"
														Visibility="{TemplateBinding RightPaneVisibility}"
														Grid.Column="1"
														BorderBrush="{StaticResource ApplicationMenuDividerLightBrush}"
														Background="{StaticResource ApplicationMenuContentBackgroundBrush}"
														BorderThickness="1,0,0,0">
													<ContentPresenter Content="{TemplateBinding Content}"
															Name="ContentPresenter" MinWidth="100" />
												</Border>
											</Grid>
										</Border>
									</Border>
								</Border>
								<Border Grid.Row="2" Background="{StaticResource ApplicationMenuFooterBorderBrush}">
									<ContentPresenter Content="{TemplateBinding FooterContent}"
											HorizontalAlignment="Right" VerticalAlignment="Center" />
								</Border>
							</Grid>
						</Border>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
	

	<Style TargetType="telerikRibbonViewPrimitives:DropShadow"
