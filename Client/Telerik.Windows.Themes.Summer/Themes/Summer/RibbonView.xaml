    <Style TargetType="telerikRibbonView:RadRibbonDropDownButton"
		
		x:Key="{telerik:ThemeResourceKey ThemeType=external:SummerThemeExternal, ElementType=telerikRibbonView:RadRibbonDropDownButton}"
		
		>
		<Setter Property="Padding" Value="3" />
        <Setter Property="IsTabStop"
                Value="False" />
        <Setter Property="BorderThickness" Value="1" />
		<Setter Property="HorizontalAlignment" Value="Left" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="DropDownButtonPosition" Value="Bottom" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:RadRibbonDropDownButton">
					<Grid>
						

						<Border x:Name="backgroundDecorator"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}"
								Background="{TemplateBinding Background}"
								CornerRadius="2">
							<Border x:Name="backgroundDecoratorInner"
									BorderBrush="Transparent"
									BorderThickness="1"/>
						</Border>

						<Border x:Name="PressedDecorator" Opacity="0"
								BorderThickness="1"
								BorderBrush="{StaticResource RadRibbonBarDropDownButtonPressedBorder}"
								Background="{StaticResource RadRibbonBarDropDownButtonPressedBackground}"
								CornerRadius="2">
							<Border x:Name="PressedDecoratorInner"
									BorderBrush="{StaticResource RadRibbonBarDropDownButtonPressedBorderInner}"
									BorderThickness="1"
									/>
						</Border>

                        <Grid Margin="{TemplateBinding Padding}"  x:Name="ContentGrid">
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto" />
								<RowDefinition Height="*"  />
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="Auto" />
							</Grid.ColumnDefinitions>

							<ContentPresenter
								Name="ContentPresenter"
								Grid.ColumnSpan="2" Grid.RowSpan="2"
								Content="{TemplateBinding Content}"
								ContentTemplate="{TemplateBinding ContentTemplate}"
								VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
								HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
								 RecognizesAccessKey="True"  />

							<Image Name="Image" />

							<TextBlock Name="textRow1" Grid.Row="1" Margin="3,2,2,0"
									Text="{TemplateBinding TextRow1}" TextAlignment="Center" />

                             <StackPanel Name="DropDownPanel" Grid.Row="1" Margin="2,14,0,0"
									HorizontalAlignment="Center" Orientation="Horizontal"
									MinHeight="14" MaxHeight="14" Height="14">
								<TextBlock Name="textRow2" Text="{TemplateBinding TextRow2}" />
								<Path
									Name="DropDownIndicator"
									IsHitTestVisible="False"
									VerticalAlignment="Center"
									Visibility="{TemplateBinding DropDownIndicatorVisibility}"
									Fill="{StaticResource RibbonButtonDropDownArrow}"
									Stroke="{StaticResource RibbonButtonDropDownArrowBorder}"
									Data="M 0,0 L 3.5,3.5 L 7,0"
									Margin="2,0,2,0"
									 SnapsToDevicePixels="True"  />
							</StackPanel>
						</Grid>

						
						<Popup x:Name="DropDownPopup" 
							AllowsTransparency="True" StaysOpen="False" Focusable="false" Placement="Bottom"
							IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
						
						

							<Grid>
								<!--<telerikRibbonViewPrimitives:DropShadow Margin="6,6,4,4" />-->
								<Border x:Name="DropDownPopupBorder"
										BorderBrush="{StaticResource DropDownPopupBorderBrush}"
										Background="{StaticResource DropDownPopupBackgroundBrush}"
										BorderThickness="1" Padding="1" Margin="0"
										MinWidth="3"
										MinHeight="3"
										Width="{TemplateBinding DropDownWidth}"
										Height="{TemplateBinding DropDownHeight}"
										MaxWidth="{TemplateBinding DropDownMaxWidth}"
										MaxHeight="{TemplateBinding DropDownMaxHeight}">
									<ContentPresenter
										x:Name="DropDownPopupContent"
										Content="{TemplateBinding DropDownContent}"
										ContentTemplate="{TemplateBinding DropDownContentTemplate}"
										HorizontalAlignment="Stretch"
										VerticalAlignment="Stretch"
										DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}"/>
								</Border>
							</Grid>
						
						</Popup>
						
						

					</Grid>

					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="true">
							<Setter Property="BorderBrush" TargetName="backgroundDecorator" Value="{StaticResource RadRibbonBarButtonOverBorder}"/>
							<Setter Property="Background" TargetName="backgroundDecorator" Value="{StaticResource RadRibbonBarButtonOverBackground}"/>
							<Setter Property="BorderBrush" TargetName="backgroundDecoratorInner" Value="{StaticResource RadRibbonBarButtonOverBorderInner}"/>
						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter Property="Opacity" TargetName="PressedDecorator" Value="1"/>
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="ContentGrid" Property="Opacity" Value="0.5" />
                        </Trigger>
						<Trigger Property="IsFocused" Value="True" />
						<Trigger Property="IsOpen" Value="True">
							<Setter Property="Opacity" TargetName="PressedDecorator" Value="1"/>
						</Trigger>
						<Trigger Property="DropDownButtonPosition" Value="Top" />
						<Trigger Property="DropDownButtonPosition" Value="Left" />
						<Trigger Property="DropDownButtonPosition" Value="Bottom" />
						<Trigger Property="DropDownButtonPosition" Value="Right">
							<Setter TargetName="DropDownPanel" Property="Grid.Column" Value="2"/>
							<Setter TargetName="DropDownPanel" Property="Grid.RowSpan" Value="2"/>
                            <Setter TargetName="DropDownPanel" Property="Margin" Value="2,0,0,0"/>
                            <Setter TargetName="textRow1" Property="Grid.RowSpan" Value="2"/>
                            <Setter TargetName="Image" Property="Grid.RowSpan" Value="2"/>
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="CurrentSize" Value="Large" />
						<Trigger Property="CurrentSize" Value="Medium">
							<Setter TargetName="textRow1" Property="Grid.Row" Value="0" />
							<Setter TargetName="textRow1" Property="Grid.Column" Value="1" />
							<Setter TargetName="textRow1" Property="Margin" Value="4,0,3,0" />
							<Setter TargetName="textRow1" Property="VerticalAlignment" Value="Center" />
							<Setter TargetName="textRow1" Property="TextAlignment" Value="Left" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="CurrentSize" Value="Small">
							<Setter TargetName="textRow1" Property="Visibility" Value="Collapsed" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="DropDownPlacement" Value="Absolute" />
						<Trigger Property="DropDownPlacement" Value="Bottom" />
						<Trigger Property="DropDownPlacement" Value="Center" />
						<Trigger Property="DropDownPlacement" Value="Top" />
						<Trigger Property="DropDownPlacement" Value="Left" />
						<Trigger Property="DropDownPlacement" Value="Right" />
					</ControlTemplate.Triggers>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
    
	

    <Style TargetType="telerikRibbonView:RadRibbonGallery"
        x:Key="{telerik:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikRibbonView:RadRibbonGallery}"
         >
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="BorderBrush" Value="{StaticResource RibbonGalleryBorderBrush}" />
        <Setter Property="Background" Value="{StaticResource RibbonGalleryBackgroundBrush}" />
        <Setter Property="ViewportWidth" Value="217" />
        <Setter Property="ViewportHeight" Value="58" />
        
        
        <Setter Property="MoreButtonTitle" Value="More" />
        <Setter Property="UpDownButtonTitle" Value="Row " />
        
        <Setter Property="ItemWidth" Value="20" />
        <Setter Property="ItemHeight" Value="20" />
        <Setter Property="PopupLaunchButtonVisibility" Value="Visible" />
        <!--<Setter Property="PopupViewportHeight" Value="170" />-->
        <Setter Property="PopupViewportWidth" Value="237" />
        <Setter Property="Icon" Value="/Telerik.Windows.Controls.RibbonView;component/RibbonGalleryCollapsedIcon.png" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerik:RadWrapPanel 
                        
                        ItemWidth="{Binding RelativeSource={RelativeSource
FindAncestor, AncestorType={x:Type telerikRibbonView:RadRibbonGallery}},
Path=ItemWidth}" ItemHeight="{Binding RelativeSource={RelativeSource
FindAncestor, AncestorType={x:Type telerikRibbonView:RadRibbonGallery}},
Path=ItemHeight}"
                         />

                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonView:RadRibbonGallery">
                    <Grid>
                        
                        <Border x:Name="ExpandedBorder" BorderThickness="{TemplateBinding BorderThickness}"
                                BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}"
                                CornerRadius="0,1,1,0">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="14" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <ScrollViewer x:Name="ScrollViewer" Grid.RowSpan="3"
                                        VerticalScrollBarVisibility="Hidden" IsTabStop="False" BorderThickness="0"
                                        Height="{TemplateBinding ViewportHeight}" Padding="0,1,0,1">
                                    <Border x:Name="InRibbonHost" Width="{TemplateBinding ViewportWidth}"
                                            HorizontalAlignment="Left">
                                        <ItemsPresenter x:Name="ItemsPresenter" />
                                    </Border>
                                </ScrollViewer>

                                <telerikRibbonView:RadRibbonButton telerik:StyleManager.Theme="{StaticResource Theme}"
                                        telerikRibbonView:ScreenTip.Title="{TemplateBinding UpDownButtonTitle}"
                                        HorizontalAlignment="Stretch" x:Name="ScrollUpButton" Grid.Column="1"
                                        Grid.Row="0" IsTabStop="False" BorderBrush="{TemplateBinding BorderBrush}"
                                        Margin="0,-1,-1,0"
                                        Background="{StaticResource RibbonGalleryItemBackgroundBrush}" Padding="0">
                                    <Path Margin="4 1 0 0" Data="M 0,3.5 L 3.5,0 L 7,3.5"
                                            Fill="{StaticResource RibbonGroupButtonArrow}"
                                            Stroke="{StaticResource RibbonGroupButtonArrowHighlight}" />
                                </telerikRibbonView:RadRibbonButton>
                                <telerikRibbonView:RadRibbonButton telerik:StyleManager.Theme="{StaticResource Theme}"
                                        telerikRibbonView:ScreenTip.Title="{TemplateBinding UpDownButtonTitle}"
                                        HorizontalAlignment="Stretch" x:Name="ScrollDownButton" Grid.Column="1"
                                        Grid.Row="1" IsTabStop="False" BorderBrush="{TemplateBinding BorderBrush}"
                                        Margin="0,-1,-1,-1"
                                        Background="{StaticResource RibbonGalleryItemBackgroundBrush}" Padding="0">
                                    <Path Margin="4 2 0 0" HorizontalAlignment="Center" VerticalAlignment="Center"
                                            Data="M 0,0 L 3.5,3.5 L 7,0" Fill="{StaticResource RibbonGroupButtonArrow}"
                                            Stroke="{StaticResource RibbonGroupButtonArrowHighlight}" />
                                </telerikRibbonView:RadRibbonButton>
                                <telerikRibbonView:RadRibbonDropDownButton
                                        telerik:StyleManager.Theme="{StaticResource Theme}"
                                        telerikRibbonView:ScreenTip.Title="{TemplateBinding MoreButtonTitle}"
                                        HorizontalAlignment="Stretch" DropDownIndicatorVisibility="Collapsed"
                                        Visibility="{TemplateBinding PopupLaunchButtonVisibility}" x:Name="PopupButton"
                                        Grid.Column="1" Grid.Row="2" BorderBrush="{TemplateBinding BorderBrush}"
                                        Margin="0,0,-1,-1"
                                        Background="{StaticResource RibbonGalleryItemBackgroundBrush}"
                                        Padding="0,0,-1,0">
                                    <Path HorizontalAlignment="Center" VerticalAlignment="Center" Margin="2,4,0,0"
                                            Data="M 0,-2.5 L 0,-1.5 L 7,-1.5 L 7,-2.5 M 0,0 L 3.5,3.5 L 7,0"
                                            Fill="{StaticResource RibbonGroupButtonArrow}"
                                            Stroke="{StaticResource RibbonGroupButtonArrowHighlight}" />
                                </telerikRibbonView:RadRibbonDropDownButton>
                            </Grid>
                        </Border>

                        <telerikRibbonView:RadRibbonDropDownButton telerik:StyleManager.Theme="{StaticResource Theme}"
                                x:Name="CollapsedButton" Size="Large" LargeImage="{TemplateBinding Icon}"
                                Text="{TemplateBinding Title}" Visibility="Collapsed">
                            <telerikRibbonView:RadRibbonDropDownButton.DropDownContent>
                                <Grid Width="{TemplateBinding PopupViewportWidth}">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <ScrollViewer telerik:StyleManager.Theme="{StaticResource Theme}" Grid.Row="0"
                                            IsTabStop="False" BorderThickness="0" Margin="0"
                                            Height="{TemplateBinding PopupViewportHeight}" Padding="0"
                                            VerticalScrollBarVisibility="Visible">
                                        <Border x:Name="PopupHost" HorizontalAlignment="Left" Margin="0" Width="{TemplateBinding PopupViewportWidth}"
                                             >
                                        </Border>
                                    </ScrollViewer>

                                    <telerikNavigation:RadContextMenu
                                            telerik:StyleManager.Theme="{StaticResource Theme}"
                                            HorizontalContentAlignment="Stretch" x:Name="menu" BorderThickness="0,1,0,0"
                                            Grid.Row="1" ItemsSource="{TemplateBinding PopupMenuItems}" />

                                </Grid>
                            </telerikRibbonView:RadRibbonDropDownButton.DropDownContent>
                        </telerikRibbonView:RadRibbonDropDownButton>

                    </Grid>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="ExpandedBorder" Property="Background"
                                    Value="{StaticResource RibbonGalleryOverBackgroundBrush}" />
                        </Trigger>
                        <Trigger Property="IsCompressed" Value="true">
                            <Setter TargetName="ExpandedBorder" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="CollapsedButton" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <!--<Trigger Property="IsCompressed" Value="false">
                            <Setter TargetName="ExpandedBorder" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="CollapsedButton" Property="Visibility" Value="Collapsed" />
                        </Trigger>-->
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
	

	<Style TargetType="telerikRibbonView:RadRibbonDropDownButton" x:Key="DropDownButtonGroup">
		<Setter Property="Padding" Value="3" />
		<Setter Property="BorderThickness" Value="1" />
        <Setter Property="IsTabStop"
                Value="False" />
        <Setter Property="Background" Value="{StaticResource RadRibbonGroupNormalBackground}" />
		<Setter Property="BorderBrush" Value="{StaticResource RadRibbonGroupNormalBorder}" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="VerticalAlignment" Value="Stretch" />
		<Setter Property="MaxHeight" Value="999" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:RadRibbonDropDownButton">
                    <Grid SnapsToDevicePixels="True">
                       
                        <Border x:Name="buttonSurface"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}"
								Background="{TemplateBinding Background}"
								Margin="0,0,0,1"
								CornerRadius="3">
							<Border x:Name="buttonSurfaceInner"
									BorderBrush="{StaticResource RadRibbonGroupNormalBorderInner}"
									BorderThickness="1 0 0 1" CornerRadius="2"/>
						</Border>

						<Border x:Name="mouseOverIndicator" Opacity="0"
								BorderThickness="1"
								BorderBrush="{StaticResource RadRibbonGroupPressedBorder}"
								Background="{StaticResource RadRibbonGroupPressedBackground}"
								CornerRadius="3">
							<Border x:Name="mouseOverIndicatorInner"
									BorderBrush="{StaticResource RadRibbonGroupPressedBorderInner}"
									BorderThickness="1 0 0 1"
									Background="Transparent"
									CornerRadius="2" />

						</Border>


                        <Grid Margin="{TemplateBinding Padding}" x:Name="ContentGrid">
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto" />
								<RowDefinition Height="*" MaxHeight="28" />
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="Auto" />
							</Grid.ColumnDefinitions>

							<ContentPresenter Grid.ColumnSpan="3" Grid.RowSpan="2"
                                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
									VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
									Content="{TemplateBinding Content}"
									ContentTemplate="{TemplateBinding ContentTemplate}"/>

							<Image Name="Image" />

							<TextBlock Name="textRow1" Grid.Row="1" Margin="0,2,0,0"
									Text="{TemplateBinding TextRow1}" TextAlignment="Center" />

							<StackPanel Name="DropDownPanel" Grid.Row="1" Margin="0,14,0,0"
									HorizontalAlignment="Center" Orientation="Horizontal"
									MinHeight="14" MaxHeight="14" Height="14">
								<TextBlock Name="textRow2" Text="{TemplateBinding TextRow2}" />
								<Path Name="DropDownIndicator" VerticalAlignment="Center"
										Data="M 0,0 L 3.5,3.5 L 7,0" Margin="2,0,0,0"
										Visibility="{TemplateBinding DropDownIndicatorVisibility}"
										Fill="{StaticResource RibbonButtonDropDownArrow}"
										Stroke="{StaticResource RibbonButtonDropDownArrowBorder}" />
							</StackPanel>
						</Grid>

						
						<Popup x:Name="DropDownPopup" 
							AllowsTransparency="True" StaysOpen="False" Focusable="false" Placement="Bottom"
							IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
						

						
                 
							<Grid>
								<Border x:Name="DropDownPopupBorder"
									BorderBrush="Transparent"
									Background="Transparent"
									MinWidth="33" MinHeight="33">
									<ContentControl x:Name="DropDownPopupContent"
										Content="{TemplateBinding DropDownContent}"
										HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
                                                    IsTabStop="False"/>
								</Border>
                                <Border BorderBrush="{StaticResource RadRibbonGroupNormalBorder_Popup}" CornerRadius="3"
								    BorderThickness="1" Margin="0 -1 0 -1">
                                        <Border BorderBrush="{StaticResource RadRibbonGroupNormalBorderInner_Popup}" CornerRadius="2"
										    BorderThickness="1" />
                                </Border>
                            </Grid>
                                
						
						</Popup>
						
                            
						

						<Rectangle x:Name="DisabledIndicator" Visibility="Collapsed" Fill="#99FFFFFF" />
					</Grid>
						
					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="True">
							<Setter Property="BorderBrush" TargetName="buttonSurface"
									Value="{StaticResource RadRibbonGroupOverBorder}" />
							<Setter Property="Background" TargetName="buttonSurface" 
									Value="{StaticResource RadRibbonGroupOverBackground}" />
							<Setter Property="BorderBrush" TargetName="buttonSurfaceInner" 
									Value="{StaticResource RadRibbonGroupOverBorderInner}" />
						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter Property="Opacity" TargetName="mouseOverIndicator" Value="1" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="ContentGrid" Property="Opacity" Value="0.5" />
                        </Trigger>
						
						<Trigger Property="IsFocused" Value="True" />
						<Trigger Property="IsOpen" Value="True">
							<Setter Property="Opacity" TargetName="mouseOverIndicator" Value="1"/>
						</Trigger>
						<Trigger Property="DropDownButtonPosition" Value="Top" />
						<Trigger Property="DropDownButtonPosition" Value="Left" />
						<Trigger Property="DropDownButtonPosition" Value="Bottom" />
						<Trigger Property="DropDownButtonPosition" Value="Right">
							<Setter TargetName="DropDownPanel" Property="Grid.Column" Value="2"/>
							<Setter TargetName="DropDownPanel" Property="Grid.Row" Value="0"/>
							<Setter TargetName="DropDownPanel" Property="Margin" Value="0,0,0,0"/>
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="CurrentSize" Value="Large" />
						<Trigger Property="CurrentSize" Value="Medium">
							<Setter TargetName="textRow1" Property="Grid.Row" Value="0" />
							<Setter TargetName="textRow1" Property="Grid.Column" Value="1" />
							<Setter TargetName="textRow1" Property="Margin" Value="4,0,1,0" />
							<Setter TargetName="textRow1" Property="VerticalAlignment" Value="Center" />
							<Setter TargetName="textRow1" Property="TextAlignment" Value="Left" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="CurrentSize" Value="Small">
							<Setter TargetName="textRow1" Property="Visibility" Value="Collapsed" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</Trigger>
					</ControlTemplate.Triggers>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style x:Key="SeparatorStyle"  TargetType="telerikRibbonView:Separator">
		<Setter Property="Background" Value="{StaticResource RibbonGroupSeparatorLightBrush}" />
		<Setter Property="BorderThickness" Value="0,0,1,0" />
		<Setter Property="BorderBrush" Value="{StaticResource RibbonGroupSeparatorDarkBrush}" />
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Width" Value="2" />
		<Setter Property="Margin" Value="0,5,0,6" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:Separator">
					<Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" />
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<ItemsPanelTemplate x:Key="RadRibbonGroupPanelTemplate">
		<telerikRibbonViewPrimitives:RibbonButtonsPanel />
	</ItemsPanelTemplate>
	
	<Style TargetType="telerikRibbonView:RadRibbonGroup"
            x:Key="{telerik:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikRibbonView:RadRibbonGroup}"
        >
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Foreground" Value="{StaticResource RibbonForegroundBrush}" />
		<Setter Property="SeparatorStyle" Value="{StaticResource SeparatorStyle}" />
		<Setter Property="ItemsPanel" Value="{StaticResource RadRibbonGroupPanelTemplate}" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:RadRibbonGroup">
					<Grid SnapsToDevicePixels="True"  >
                        
                        <telerikRibbonViewPrimitives:GroupChrome telerik:StyleManager.Theme="{StaticResource Theme}" x:Name="ExpandedChrome" Foreground="{TemplateBinding Foreground}"  Header="{TemplateBinding Header}" HeaderTemplate="{TemplateBinding HeaderTemplate}" DialogLauncherVisibility="{TemplateBinding DialogLauncherVisibility}"
                                                                IsTabStop="False">
							<ItemsPresenter Name="ItemsPresenter" />
						</telerikRibbonViewPrimitives:GroupChrome>
                        
                        <telerikRibbonView:RadRibbonDropDownButton 							
							Visibility="Collapsed"
							Style="{StaticResource DropDownButtonGroup}" DropDownIndicatorVisibility="Collapsed" x:Name="CollapsedButton" DropDownButtonPosition="Right" HorizontalAlignment="Stretch">
							<telerikRibbonView:RadRibbonDropDownButton.DropDownContent>
								<Border CornerRadius="2" Background="{StaticResource RibbonTabBackgroundBrush}">
									<telerikRibbonViewPrimitives:GroupChrome telerik:StyleManager.Theme="{StaticResource Theme}" x:Name="CollapsedChrome"/>
								</Border>
							</telerikRibbonView:RadRibbonDropDownButton.DropDownContent>
						</telerikRibbonView:RadRibbonDropDownButton>
                       
                        
                        <Grid Name="CollapsedDisplay"						
							Visibility="Collapsed"
							Margin="2" VerticalAlignment="Top" IsHitTestVisible="False">
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto" />
								<RowDefinition Height="Auto" />
								<RowDefinition Height="Auto" />
							</Grid.RowDefinitions>
							<Border x:Name="GroupImage" BorderBrush="{StaticResource RibbonGroupButtonBorderBrush}" BorderThickness="1" CornerRadius="4" Width="31" Height="31" Background="{StaticResource RibbonGroupButtonBackgroundBrush}" Margin="3">
								<Grid>
									<Border VerticalAlignment="Bottom" Height="7" CornerRadius="0,0,3,3" Background="{StaticResource RibbonGroupButtonBottomBrush}" />
									<Image VerticalAlignment="Center" HorizontalAlignment="Center" Margin="0,0,0,7" Source="{TemplateBinding Icon}" Width="16" Height="16" />
								</Grid>
							</Border>
							<ContentControl Content="{TemplateBinding Header}"
                                            ContentTemplate="{TemplateBinding HeaderTemplate}"
                                            Foreground="{TemplateBinding Foreground}" 
                                            Grid.Row="1" 
                                            Margin="1,2,1,3" 
                                            HorizontalContentAlignment="Center" 
                                            IsTabStop="False"/>
							<Path Grid.Row="2" VerticalAlignment="Top" HorizontalAlignment="Center" Fill="{StaticResource RibbonGroupButtonArrow}" Stroke="{StaticResource RibbonGroupButtonArrowHighlight}" Data="M 0,0 L 3.5,3.5 L 7,0" />
						</Grid>
					</Grid>
                    
                    <ControlTemplate.Triggers>
                       <Trigger Property="Variant" Value="Collapsed">
                            <Setter Property="Visibility" TargetName="CollapsedDisplay" Value="Visible" />
                            <Setter Property="Visibility" TargetName="CollapsedButton" Value="Visible" />
                            <Setter Property="Visibility" TargetName="ExpandedChrome" Value="Collapsed" />
                        </Trigger>
                        <Trigger Property="IsPushed" Value="True">
                            <Setter Property="Background" TargetName="GroupImage" Value="{StaticResource RibbonGroupPushedButtonBackgroundBrush}" />
                        </Trigger>
                        <Trigger Property="IsPushed" Value="False">
                            <Setter Property="Background" TargetName="GroupImage" Value="{StaticResource RibbonGroupButtonBackgroundBrush}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
	

    <Style TargetType="telerikRibbonView:RadRibbonRadioButton"
		
        x:Key="{telerik:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikRibbonView:RadRibbonRadioButton}"
        
        >
        <Setter Property="Padding" Value="3" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="IsTabStop"
                Value="False" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonView:RadRibbonRadioButton">
                    <Grid>
                        

                        <Border x:Name="buttonSurface"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}"
								Background="{TemplateBinding Background}"
								CornerRadius="2">
                            <Border x:Name="buttonSurfaceInner"
									BorderBrush="Transparent"
									BorderThickness="1"/>
                        </Border>
                        <Border x:Name="mouseOverIndicator" CornerRadius="2" Opacity="0"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{StaticResource RadRibbonBarButtonSelectedBorder}"
								Background="{StaticResource RadRibbonBarButtonSelectedBackground}">
                            <Border x:Name="mouseOverIndicatorInner"
									BorderBrush="{StaticResource RadRibbonBarButtonSelectedBorderInner}"
									BorderThickness="1"/>
                        </Border>

                        <Grid Margin="{TemplateBinding Padding}">
                            <ContentPresenter
								Name="ContentPresenter"
								Content="{TemplateBinding Content}"
								ContentTemplate="{TemplateBinding ContentTemplate}"
								VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
								HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
								RecognizesAccessKey="True"
                                 />

                                <Grid x:Name="ContentGrid">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" MaxHeight="28" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>

                                    <Image Name="Image" />

                                    <TextBlock Name="textRow1" Grid.Row="1" Margin="0,2,0,0"
										Text="{TemplateBinding TextRow1}" TextAlignment="Center" />
                                    <TextBlock Name="textRow2" Grid.Row="1" Margin="0,14,0,0"
										Text="{TemplateBinding TextRow2}" TextAlignment="Center" />
                                </Grid>
                        </Grid>

                    </Grid>

                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="BorderBrush" TargetName="buttonSurface" Value="{StaticResource RadRibbonBarButtonOverBorder}"/>
                            <Setter Property="Background" TargetName="buttonSurface" Value="{StaticResource RadRibbonBarButtonOverBackground}"/>
                            <Setter Property="BorderBrush" TargetName="buttonSurfaceInner" Value="{StaticResource RadRibbonBarButtonOverBorderInner}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="BorderBrush" TargetName="buttonSurface" Value="{StaticResource RadRibbonBarButtonPressedBorder}"/>
                            <Setter Property="Background" TargetName="buttonSurface" Value="{StaticResource RadRibbonBarButtonPressedBackground}"/>
                            <Setter Property="BorderBrush" TargetName="buttonSurfaceInner" Value="{StaticResource RadRibbonBarButtonPressedBorderInner}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="ContentGrid" Property="Opacity" Value="0.5" />
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter Property="Opacity" TargetName="mouseOverIndicator" Value="1"/>
                        </Trigger>
                        <Trigger Property="IsFocused" Value="True" />

                        <Trigger Property="CurrentSize" Value="Large" />
                        <Trigger Property="CurrentSize" Value="Medium">
                            <Setter TargetName="textRow1" Property="Grid.Row" Value="0" />
                            <Setter TargetName="textRow1" Property="Grid.Column" Value="1" />
                            <Setter TargetName="textRow1" Property="Margin" Value="4,0,1,0" />
                            <Setter TargetName="textRow1" Property="VerticalAlignment" Value="Center" />
                            <Setter TargetName="textRow1" Property="TextAlignment" Value="Left" />
                            <Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <Trigger Property="CurrentSize" Value="Small">
                            <Setter TargetName="textRow1" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    
	

	<ControlTemplate TargetType="telerik:RadButton" x:Key="RadSplitButton_ButtonStyle">
		<Grid>
			

			<Border x:Name="backgroundDecorator"
					BorderThickness="{TemplateBinding BorderThickness}"
					BorderBrush="{TemplateBinding BorderBrush}"
					Background="{x:Null}"
					CornerRadius="2">
				<Border x:Name="backgroundDecoratorInner" Margin="0,0,0,0"
						BorderThickness="{TemplateBinding BorderThickness}"
						BorderBrush="Transparent"
						Background="{TemplateBinding Background}"
						CornerRadius="1">
					<ContentPresenter x:Name="Content"
							Content="{TemplateBinding Content}"
							ContentTemplate="{TemplateBinding ContentTemplate}"
							VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
							HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
							Margin="{TemplateBinding Padding}"
							RecognizesAccessKey="True" />
				</Border>
			</Border>
		</Grid>

		
		<ControlTemplate.Triggers>
			<Trigger Property="IsEnabled" Value="False" />
			<Trigger Property="IsDefault" Value="True" />
			<Trigger Property="IsMouseOver" Value="True">
				<Setter Property="BorderBrush" TargetName="backgroundDecorator"
						Value="{StaticResource RadRibbonBarButtonOverBorder}" />
				<Setter Property="BorderBrush" TargetName="backgroundDecoratorInner"
						Value="{StaticResource RadRibbonBarButtonOverBorderInner}" />
				<Setter Property="Background" TargetName="backgroundDecoratorInner"
						Value="{StaticResource RadRibbonBarButtonOverBackground}" />
			</Trigger>
			<Trigger Property="IsPressed" Value="True">
				<Setter Property="BorderBrush" TargetName="backgroundDecorator"
						Value="{StaticResource RadRibbonBarButtonPressedBorder}" />
				<Setter Property="BorderBrush" TargetName="backgroundDecoratorInner"
						Value="{StaticResource RadRibbonBarButtonPressedBorderInner}" />
				<Setter Property="Background" TargetName="backgroundDecoratorInner"
						Value="{StaticResource RadRibbonBarButtonPressedBackground}" />
			</Trigger>
		</ControlTemplate.Triggers>
		
	</ControlTemplate>

	<ControlTemplate TargetType="telerik:RadToggleButton" x:Key="RadSplitButton_DropDownStyle">
		<Grid>
			

			<Border x:Name="backgroundDecorator"
					BorderThickness="{TemplateBinding BorderThickness}" 
					BorderBrush="{TemplateBinding BorderBrush}" 
					Background="{x:Null}" 
					CornerRadius="2">
				<Border x:Name="backgroundDecoratorInner" Margin="0,0,0,0"
						BorderThickness="{TemplateBinding BorderThickness}"
						BorderBrush="Transparent"
						Background="{TemplateBinding Background}"
						CornerRadius="1">
				</Border>
			</Border>

			<Border Opacity="0" x:Name="mouseOverDecorator" Background="{x:Null}" 
					BorderThickness="{TemplateBinding BorderThickness}" 
					BorderBrush="{StaticResource RadRibbonBarButtonOverBorder}" 
					CornerRadius="2">
				<Border x:Name="mouseOverDecoratorInner" Margin="0,0,0,0" CornerRadius="1" 
						BorderThickness="{TemplateBinding BorderThickness}" 
						BorderBrush="{StaticResource RadRibbonBarButtonOverBorderInner}" 
						Background="{StaticResource RadRibbonBarButtonOverBackground}">
				</Border>
			</Border>
			<Border BorderThickness="{TemplateBinding BorderThickness}">
				<ContentPresenter x:Name="contentPresenter"
					Content="{TemplateBinding Content}"
					ContentTemplate="{TemplateBinding ContentTemplate}"
					VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
					HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
					Margin="{TemplateBinding Padding}"
					RecognizesAccessKey="True" />
			</Border>
		</Grid>

		
		<ControlTemplate.Triggers>
			<Trigger Property="IsEnabled" Value="False" />
			<Trigger Property="IsFocused" Value="True" />
			<Trigger Property="IsMouseOver" Value="True">
				<Setter Property="Opacity" TargetName="mouseOverDecorator" Value="1"/>
			</Trigger>
			<Trigger Property="IsPressed" Value="True">
				<Setter Property="Opacity" TargetName="mouseOverDecorator" Value="1"/>
				<Setter Property="BorderBrush" TargetName="mouseOverDecorator" Value="{StaticResource RadRibbonBarButtonPressedBorder}"/>
				<Setter Property="BorderBrush" TargetName="mouseOverDecoratorInner" Value="{StaticResource RadRibbonBarButtonPressedBorderInner}"/>
				<Setter Property="Background" TargetName="mouseOverDecoratorInner" Value="{StaticResource RadRibbonBarButtonPressedBackground}"/>
			</Trigger>
			<Trigger Property="IsChecked" Value="True">
				<Setter Property="BorderBrush" TargetName="backgroundDecorator" Value="{StaticResource RadRibbonBarButtonPressedBorder}"/>
				<Setter Property="BorderBrush" TargetName="backgroundDecoratorInner" Value="{StaticResource RadRibbonBarButtonPressedBorderInner}"/>
				<Setter Property="Background" TargetName="backgroundDecoratorInner" Value="{StaticResource RadRibbonBarButtonPressedBackground}"/>
			</Trigger>
		</ControlTemplate.Triggers>
		
	</ControlTemplate>

	<Style TargetType="telerikRibbonView:RadRibbonSplitButton"
		
		x:Key="{telerik:ThemeResourceKey ThemeType=external:SummerThemeExternal, ElementType=telerikRibbonView:RadRibbonSplitButton}"
		
		>
		<Setter Property="IsTabStop" Value="false" />
		<Setter Property="IsOpen" Value="false" />
		<Setter Property="Padding" Value="3" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="HorizontalAlignment" Value="Left" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:RadRibbonSplitButton">
					<Grid >
						

						<Border x:Name="backgroundDecorator"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}"
								Background="{x:Null}"
								CornerRadius="2">
							<Border x:Name="backgroundDecoratorInner"
									BorderThickness="0"
									BorderBrush="{x:Null}"
									Background="{TemplateBinding Background}"/>
						</Border>

						<Border x:Name="mouseOverDecorator" Opacity="0"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{StaticResource RadRibbonBarDropDownButtonPressedBorder}"
								Background="{x:Null}"
								CornerRadius="2">
							<Border x:Name="mouseOverDecoratorInner"
									BorderThickness="{TemplateBinding BorderThickness}"
									BorderBrush="{StaticResource RadRibbonBarSplitButtonBorderInner_Over}"
									Background="{StaticResource RadRibbonBarSplitButtonBackgroundInner_Over}"/>
						</Border>

                        <Grid Margin="1" x:Name="ContentGrid">
							<Grid.RowDefinitions>
								<RowDefinition Height="*" />
								<RowDefinition Height="Auto" MaxHeight="28"/>
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="Auto" />
							</Grid.ColumnDefinitions>

							<Border x:Name="isCheckedIndicator"
									Opacity="0"
									Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2"
									BorderThickness="0"
									BorderBrush="{x:Null}"
									Background="{StaticResource RadRibbonBarButtonPressedBackground}"
									CornerRadius="1" />

							<telerik:RadButton
								x:Name="ButtonPart"
								Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2"
								BorderThickness="0"
								Background="Transparent"
								Content="{TemplateBinding Content}"
								ContentTemplate="{TemplateBinding ContentTemplate}"
								HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
								VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
								Template="{StaticResource RadSplitButton_ButtonStyle}"
								Padding="{TemplateBinding Padding}"
								/>
							<telerik:RadToggleButton
								x:Name="DropDownPart"
								Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2"
								BorderThickness="0" Margin="0"
								Background="Transparent"
								Visibility="{TemplateBinding DropDownIndicatorVisibility}"
								Template="{StaticResource RadSplitButton_DropDownStyle}"
								IsTabStop="false"                                
								MinWidth="11" MinHeight="7"
								HorizontalAlignment="Stretch"
								
								
								IsChecked="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
								 />
							<Rectangle
								x:Name="SeparatorPart"
								Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2"
								MinWidth="1" MinHeight="1"
								Visibility="Collapsed" IsHitTestVisible="False"
								HorizontalAlignment="Stretch" VerticalAlignment="Bottom"
								Fill="{StaticResource RadRibbonBarDropDownButtonPressedBorder}"
								 SnapsToDevicePixels="True"  />

							<Image Name="Image" IsHitTestVisible="False" Margin="2" Grid.ColumnSpan="2" HorizontalAlignment="Center" />

							<TextBlock Name="textRow1" Grid.Row="1" Grid.ColumnSpan="2" Margin="2,0,2,0" IsHitTestVisible="False"
									Text="{TemplateBinding TextRow1}" TextAlignment="Center" />

							<StackPanel Name="DropDownPanel" Grid.Row="1" Grid.ColumnSpan="2" Margin="2,12,0,2"
									HorizontalAlignment="Center"  Orientation="Horizontal"
									MinHeight="14" MaxHeight="14" VerticalAlignment="Center" Height="14"  IsHitTestVisible="False">
								<TextBlock Name="textRow2" Text="{TemplateBinding TextRow2}" Margin="0 -1 0 0" IsHitTestVisible="False"/>
								<Path
									Name="DropDownIndicator"
									IsHitTestVisible="False"
									Visibility="{TemplateBinding DropDownIndicatorVisibility}"
									Fill="{StaticResource RibbonButtonDropDownArrow}"
									Stroke="{StaticResource RibbonButtonDropDownArrowBorder}"
									Data="M 0,0 L 3.5,3.5 L 7,0"
									VerticalAlignment="Center"
									Margin="2,0,2,0"
									 SnapsToDevicePixels="True"  />
							</StackPanel>
						</Grid>
						
						<Popup x:Name="DropDownPopup" 
							AllowsTransparency="True" StaysOpen="False" Focusable="false" Placement="Bottom"
							IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
						

						

							<Grid>
								<!--<telerikRibbonViewPrimitives:DropShadow Margin="6,6,4,4" />-->
								<Border x:Name="DropDownPopupBorder"
										BorderBrush="{StaticResource DropDownPopupBorderBrush}"
										Background="{StaticResource DropDownPopupBackgroundBrush}"
										BorderThickness="1" Padding="1" Margin="0"
										MinWidth="3"
										MinHeight="3"
										Width="{TemplateBinding DropDownWidth}"
										Height="{TemplateBinding DropDownHeight}"
										MaxWidth="{TemplateBinding DropDownMaxWidth}"
										MaxHeight="{TemplateBinding DropDownMaxHeight}">
									<ContentPresenter
										x:Name="DropDownPopupContent"
										Content="{TemplateBinding DropDownContent}"
										ContentTemplate="{TemplateBinding DropDownContentTemplate}"
										HorizontalAlignment="Stretch"
										VerticalAlignment="Stretch"
										DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}" />
								</Border>
							</Grid>
						
						</Popup>
						
						

						<Rectangle x:Name="disabledDecorator" Visibility="Collapsed" Opacity="0" Fill="#FFFFFFFF" />
					</Grid>

					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="true">
							<Setter TargetName="mouseOverDecorator" Property="Opacity" Value="1" />
							<Setter TargetName="SeparatorPart" Property="Visibility" Value="Visible" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="ContentGrid" Property="Opacity" Value="0.5" />
                        </Trigger>
						<Trigger Property="IsFocused" Value="True" />
						<Trigger Property="IsOpen" Value="True">
							<Setter TargetName="mouseOverDecorator" Property="Opacity" Value="1" />
							<Setter TargetName="SeparatorPart" Property="Visibility" Value="Visible" />
						</Trigger>
						<Trigger Property="DropDownButtonPosition" Value="Top" />
						<Trigger Property="DropDownButtonPosition" Value="Left" />
						<Trigger Property="DropDownButtonPosition" Value="Bottom" />
						<Trigger Property="DropDownButtonPosition" Value="Right">
							<Setter TargetName="DropDownPanel" Property="Grid.Column" Value="2"/>
							<Setter TargetName="DropDownPanel" Property="Grid.Row" Value="0"/>
							<Setter TargetName="DropDownPanel" Property="Margin" Value="2,0,0,0"/>
							<Setter TargetName="SeparatorPart" Property="HorizontalAlignment" Value="Right"/>
							<Setter TargetName="SeparatorPart" Property="VerticalAlignment" Value="Stretch"/>
							<Setter TargetName="DropDownPart" Property="Grid.Column" Value="2"/>
							<Setter TargetName="DropDownPart" Property="Grid.Row" Value="0"/>
						</Trigger>
						<Trigger Property="CurrentSize" Value="Large">
							<Setter TargetName="Image" Property="Grid.ColumnSpan" Value="2" />
						</Trigger>
						<Trigger Property="CurrentSize" Value="Medium">
							<Setter TargetName="textRow1" Property="Grid.Row" Value="0" />
							<Setter TargetName="textRow1" Property="Grid.Column" Value="1" />
							<Setter TargetName="textRow1" Property="Margin" Value="4,0,3,0" />
							<Setter TargetName="textRow1" Property="VerticalAlignment" Value="Center" />
							<Setter TargetName="textRow1" Property="HorizontalAlignment" Value="Left" />
							<Setter TargetName="textRow1" Property="TextAlignment" Value="Left" />
							<Setter TargetName="textRow1" Property="Grid.ColumnSpan" Value="1" />
							<Setter TargetName="Image" Property="Grid.ColumnSpan" Value="1" />
						</Trigger>
						<Trigger Property="CurrentSize" Value="Small">
							<Setter TargetName="textRow1" Property="Visibility" Value="Collapsed" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
							<Setter TargetName="textRow1" Property="Grid.ColumnSpan" Value="1" />
						</Trigger>
						<Trigger Property="DropDownPlacement" Value="Top" />
						<Trigger Property="DropDownPlacement" Value="Left" />
						<Trigger Property="DropDownPlacement" Value="Right" />
						<Trigger Property="DropDownPlacement" Value="Bottom" />
						<Trigger Property="DropDownPlacement" Value="Center" />
						<Trigger Property="DropDownPlacement" Value="Absolute" />
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="IsToggle" Value="True" />
								<Condition Property="IsChecked" Value="True" />
							</MultiTrigger.Conditions>
							<Setter TargetName="isCheckedIndicator" Property="Opacity" Value="1"/>
						</MultiTrigger>
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="CurrentSize" Value="Medium" />
								<Condition Property="IsTextInMiddleButton" Value="False" />
							</MultiTrigger.Conditions>
							<Setter TargetName="textRow1" Property="Visibility" Value="Collapsed" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Visible" />
						</MultiTrigger>
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="CurrentSize" Value="Medium" />
								<Condition Property="IsTextInMiddleButton" Value="True" />
							</MultiTrigger.Conditions>
							<Setter TargetName="textRow1" Property="Visibility" Value="Visible" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</MultiTrigger>
					</ControlTemplate.Triggers>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
	

    <Style TargetType="telerikRibbonView:RadRibbonTab"
		x:Key="{telerik:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikRibbonView:RadRibbonTab}"
         >
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderThickness" Value="1,0,0,0" />
        <Setter Property="BorderBrush" Value="{StaticResource TabStripTabDividerBrush}" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Margin" Value="2 2 2 0" />
        
        <Setter Property="FocusVisualStyle">
            <Setter.Value>
                <Style TargetType="Control">
                    <Setter Property="Visibility" Value="Collapsed" />
                </Style>
            </Setter.Value>
        </Setter>
        
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonView:RadRibbonTab">
                    <Grid x:Name="RootPanel">
                        
                        <Rectangle x:Name="TabSeparator" Margin="0,0,-3,0" HorizontalAlignment="Right" Fill="{TemplateBinding BorderBrush}"
                                VerticalAlignment="Stretch" Width="1" Visibility="Collapsed" />
                        
                        <Grid x:Name="HeaderPanel">
                            <Border Background="{TemplateBinding Background}" />

                            <Border x:Name="MouseOverVisual" BorderThickness="1 1 1 0" CornerRadius="3 3 0 0" BorderBrush="{StaticResource TabMouseOverOuterBorder}"  Visibility="Collapsed" >
                                <Border BorderThickness="1 1 1 0" CornerRadius="2 2 0 0" BorderBrush="{StaticResource TabMouseOverInnerBorder}" Background="{StaticResource TabMouseOverBackground}" />
                            </Border>

                            <Border x:Name="SelectedVisual" Visibility="Collapsed" BorderThickness="1 1 1 0" CornerRadius="3 3 0 0" BorderBrush="{StaticResource TabSelectedOuterBorder}">
                                <Border BorderThickness="1 1 1 0" CornerRadius="2 2 0 0" BorderBrush="{StaticResource TabSelectedInnerBorder}" Background="{StaticResource TabSelectedBackground}" />
                            </Border>

                            <telerikNavigationPrimitives:TabItemContentPresenter x:Name="HeaderElement"
									Content="{TemplateBinding Header}"
									ContentTemplate="{TemplateBinding HeaderTemplate}" IsTabStop="False"
									VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
									HorizontalAlignment="Center" HorizontalContentAlignment="Center"
									VerticalAlignment="Center" Margin="4,2" Foreground="#000000" />
                            
                        </Grid>
                        
                        <Grid x:Name="ContentPanel" Width="0">
                            <Border Name="OuterBorder" CornerRadius="{StaticResource RibbonTabOuterCornerRadius}"
									BorderThickness="1" BorderBrush="{StaticResource RibbonTabOuterBorderBrush}"
									Background="{StaticResource RibbonTabBackgroundBrush}">
                                <Border Name="InnerBorder" CornerRadius="{StaticResource RibbonTabInnerCornerRadius}"
										BorderThickness="1" BorderBrush="{StaticResource RibbonTabInnerBorderBrush}">
                                    <telerikRibbonViewPrimitives:RibbonScrollViewer
											telerik:StyleManager.Theme="{StaticResource Theme}">
                                        <ItemsPresenter HorizontalAlignment="Left" />
                                    </telerikRibbonViewPrimitives:RibbonScrollViewer>
                                </Border>
                            </Border>
                        </Grid>
                    </Grid>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Opacity" TargetName="HeaderElement" Value="0.5" />
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter Property="Visibility" TargetName="MouseOverVisual" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter Property="Visibility" TargetName="SelectedVisual" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsContextualTabVisible" Value="False">
                            <Setter Property="Visibility" TargetName="HeaderPanel" Value="Collapsed" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikRibbonViewPrimitives:RibbonGroupsPanel />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
	

	<Style TargetType="telerikRibbonView:RadRibbonToggleButton"
		
		x:Key="{telerik:ThemeResourceKey ThemeType=external:SummerThemeExternal, ElementType=telerikRibbonView:RadRibbonToggleButton}"
		
		>
		<Setter Property="Padding" Value="3" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="HorizontalAlignment" Value="Left" />
		<Setter Property="HorizontalContentAlignment" Value="Left" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:RadRibbonToggleButton">
					<Grid>
						
						<Border x:Name="buttonSurface"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}"
								Background="{TemplateBinding Background}"
								CornerRadius="2">
							<Border x:Name="buttonSurfaceInner"
									BorderBrush="Transparent"
									BorderThickness="{TemplateBinding BorderThickness}" />
						</Border>
						<Border x:Name="mouseOverIndicator" CornerRadius="2" Opacity="0"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{StaticResource RadRibbonBarButtonSelectedBorder}"
								Background="{StaticResource RadRibbonBarButtonSelectedBackground}">
							<Border x:Name="mouseOverIndicatorInner"
									BorderBrush="{StaticResource RadRibbonBarButtonSelectedBorderInner}"
									BorderThickness="{TemplateBinding BorderThickness}" />
						</Border>

                        <Grid Margin="{TemplateBinding Padding}" x:Name="ContentGrid">
							<ContentPresenter
								Name="ContentPresenter"
								Content="{TemplateBinding Content}"
								ContentTemplate="{TemplateBinding ContentTemplate}"
								VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
								HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
								RecognizesAccessKey="True" />

							<Grid>
								<Grid.RowDefinitions>
									<RowDefinition Height="Auto" />
									<RowDefinition Height="*" MaxHeight="28" />
								</Grid.RowDefinitions>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="*" />
								</Grid.ColumnDefinitions>

								<Image Name="Image" />

								<TextBlock Name="textRow1" Grid.Row="1" Margin="0,2,0,0"
										Text="{TemplateBinding TextRow1}" TextAlignment="Center" />
								<TextBlock Name="textRow2" Grid.Row="1" Margin="0,14,0,0"
										Text="{TemplateBinding TextRow2}" TextAlignment="Center" />
							</Grid>
						</Grid>

						<Rectangle x:Name="DisabledIndicator" Visibility="Collapsed" Fill="#99FFFFFF" />
					</Grid>

					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="True">
							<Setter Property="BorderBrush" TargetName="buttonSurface" Value="{StaticResource RadRibbonBarButtonOverBorder}"/>
							<Setter Property="Background" TargetName="buttonSurface" Value="{StaticResource RadRibbonBarButtonOverBackground}"/>
							<Setter Property="BorderBrush" TargetName="buttonSurfaceInner" Value="{StaticResource RadRibbonBarButtonOverBorderInner}"/>
						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter Property="BorderBrush" TargetName="buttonSurface" Value="{StaticResource RadRibbonBarButtonPressedBorder}"/>
							<Setter Property="Background" TargetName="buttonSurface" Value="{StaticResource RadRibbonBarButtonPressedBackground}"/>
							<Setter Property="BorderBrush" TargetName="buttonSurfaceInner" Value="{StaticResource RadRibbonBarButtonPressedBorderInner}"/>
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="ContentGrid" Property="Opacity" Value="0.5" />
                        </Trigger>
						<Trigger Property="IsChecked" Value="True">
							<Setter Property="Opacity" TargetName="mouseOverIndicator" Value="1"/>
						</Trigger>
						<Trigger Property="IsFocused" Value="True" />
						
						<Trigger Property="CurrentSize" Value="Large" />
						<Trigger Property="CurrentSize" Value="Medium">
							<Setter TargetName="textRow1" Property="Grid.Row" Value="0" />
							<Setter TargetName="textRow1" Property="Grid.Column" Value="1" />
							<Setter TargetName="textRow1" Property="Margin" Value="4,0,1,0" />
							<Setter TargetName="textRow1" Property="VerticalAlignment" Value="Center" />
							<Setter TargetName="textRow1" Property="TextAlignment" Value="Left" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="CurrentSize" Value="Small">
							<Setter TargetName="textRow1" Property="Visibility" Value="Collapsed" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</Trigger>
					</ControlTemplate.Triggers>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	
	
	

	<ControlTemplate x:Key="ScrollButtonTemplate" TargetType="RepeatButton">
		<Grid>
			
			<Border Name="OuterBorder" Background="{TemplateBinding Background}"
					BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}"
					CornerRadius="3 0 0 3">
				<Border Name="InnerBorder" BorderBrush="{StaticResource PageScrollViewerButtonInnerBorderBrush}"
						BorderThickness="1" CornerRadius="2 0 0 2">
					<ContentPresenter Content="{TemplateBinding Content}" Margin="1" />
				</Border>
			</Border>
		</Grid>
		
		<ControlTemplate.Triggers>
			<Trigger Property="IsMouseOver" Value="true">
				<Setter TargetName="OuterBorder" Property="Background"
						Value="{StaticResource PageScrollViewerHotButtonBackgroundBrush}" />
				<Setter TargetName="OuterBorder" Property="BorderBrush"
						Value="{StaticResource ButtonHotOuterBorderBrush}" />
				<Setter TargetName="InnerBorder" Property="BorderBrush"
						Value="{StaticResource ButtonHotInnerBorderBrush}" />
			</Trigger>
			<Trigger Property="IsPressed" Value="True">
				<Setter TargetName="OuterBorder" Property="Background"
						Value="{StaticResource PageScrollViewerPressedButtonBackgroundBrush}" />
				<Setter TargetName="OuterBorder" Property="BorderBrush"
						Value="{StaticResource ButtonPushedOuterBorderBrush}" />
				<Setter TargetName="InnerBorder" Property="BorderBrush"
						Value="{StaticResource ButtonPushedInnerBorderBrush}" />
			</Trigger>
		</ControlTemplate.Triggers>
		
	</ControlTemplate>

	<Style x:Key="ScrollButtonStyle" TargetType="RepeatButton">
		<Setter Property="Template" Value="{StaticResource ScrollButtonTemplate}" />
		<Setter Property="Width" Value="12" />
		<Setter Property="Background" Value="{StaticResource PageScrollViewerButtonBackgroundBrush}" />
		<Setter Property="BorderBrush" Value="{StaticResource PageScrollViewerButtonOuterBorderBrush}" />
		<Setter Property="Interval" Value="50" />
		<Setter Property="BorderThickness" Value="1" />
	</Style>

	<Style x:Key="PathStyle" TargetType="Path">
		<Setter Property="Stretch" Value="Uniform" />
		<Setter Property="Height" Value="5" />
		<Setter Property="Width" Value="3" />
		<Setter Property="Fill" Value="{StaticResource PageScrollViewerButtonForegroundBrush}" />
	</Style>

	<Style TargetType="telerikRibbonViewPrimitives:RibbonScrollViewer"
           x:Key="{telerik:ThemeResourceKey ThemeType=
		external:SummerThemeExternal , ElementType=telerikRibbonViewPrimitives:RibbonScrollViewer}"
		 >
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonViewPrimitives:RibbonScrollViewer">
					<Grid>
						<ContentPresenter x:Name="Content" />
						<Grid x:Name="ButtonsGrid" HorizontalAlignment="Left">
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="Auto" />
							</Grid.ColumnDefinitions>
							<RepeatButton Grid.Column="0" x:Name="LeftScrollButtonElement"								
									Style="{StaticResource ScrollButtonStyle}">
								<Path Style="{StaticResource PathStyle}"
										Data="F1 M 110.692,342.252L 110.692,352.682L 104.594,347.467L 110.692,342.252 Z" />
							</RepeatButton>
							<RepeatButton x:Name="RightScrollButtonElement" Grid.Column="2"									
									Style="{StaticResource ScrollButtonStyle}"  RenderTransformOrigin="0.5,0.5">
                                <Path Style="{StaticResource PathStyle}"
										Data="F1 M 110.692,342.252L 110.692,352.682L 104.594,347.467L 110.692,342.252 Z" />
                                <RepeatButton.RenderTransform>
                                    <ScaleTransform ScaleX="-1" />
                                </RepeatButton.RenderTransform>
                            </RepeatButton>
						</Grid>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
	

	<Style TargetType="telerikRibbonView:ScreenTip"
            x:Key="{telerik:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikRibbonView:ScreenTip}"
        >
		<Setter Property="Padding" Value="4,2,4,2" />
		<Setter Property="Background" Value="{StaticResource QuickAccessKeyHintBackgroundBrush}" />
		<Setter Property="Foreground" Value="{StaticResource QuickAccessKeyHintForegroundBrush}" />
		<Setter Property="BorderBrush" Value="{StaticResource QuickAccessKeyHintBorderBrush}" />
		<Setter Property="MinHeight" Value="50" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:ScreenTip">
					<Grid x:Name="RootElement">
                        
                        <Border Name="Border" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" >
							<Grid Margin="{TemplateBinding Padding}">
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="*" />
								</Grid.ColumnDefinitions>
								<Grid.RowDefinitions>
									<RowDefinition Height="Auto" />
									<RowDefinition Height="Auto" />
								</Grid.RowDefinitions>
								<!-- Text -->
								<TextBlock Name="TitlePresenter" TextWrapping="Wrap" FontWeight="Bold" Grid.ColumnSpan="2" Text="{TemplateBinding Title}"/>

								<!-- Image -->
								<Image Name="Image" Grid.Row="1" Source="{TemplateBinding Icon}" Stretch="None" Margin="29,10,21,6" VerticalAlignment="Top" Visibility="Collapsed" />

								<!-- Description -->
								<TextBlock TextWrapping="Wrap"  Name="DescriptionPresenter" Grid.Row="1" Grid.Column="1" Text="{TemplateBinding Description}" VerticalAlignment="Top" Visibility="Collapsed" Margin="8,7,8,2" />
							</Grid>
						</Border>
					</Grid>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="ScreenTipState" Value="TitleDescriptionImage">
                            <Setter TargetName="DescriptionPresenter" Property="Visibility" Value="Visible" />
                            <Setter TargetName="TitlePresenter" Property="Margin" Value="0,4,0,0" />
                            <Setter TargetName="Image" Property="Visibility" Value="Visible" />
                            <Setter TargetName="RootElement" Property="Width" Value="318" />
                        </Trigger>
                        <Trigger Property="ScreenTipState" Value="TitleDescription">
                            <Setter TargetName="DescriptionPresenter" Property="Visibility" Value="Visible" />
                            <Setter TargetName="TitlePresenter" Property="Margin" Value="0,4,0,0" />
                            <Setter TargetName="RootElement" Property="Width" Value="210" />
          
                        </Trigger>
                        <Trigger Property="ScreenTipState" Value="Title">
                            <Setter TargetName="RootElement" Property="Width" Value="210" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
       

    <Style TargetType="telerikKeyTips:KeyTipControl"
        x:Key="{telerik:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikKeyTips:KeyTipControl}"
         >
        <Setter Property="MinWidth" Value="16" />
        <Setter Property="MinHeight" Value="16" />
        <Setter Property="IsHitTestVisible" Value="False" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="BorderBrush" Value="{StaticResource RibbonBarKeyTips_OuterBorderBrush}" />
        <Setter Property="Background" Value="{StaticResource RibbonBarKeyTips_Background}" />
        <Setter Property="Padding" Value="2 -2 2 -1" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikKeyTips:KeyTipControl">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" 
                            BorderThickness="{TemplateBinding BorderThickness}"  
                            Background="{TemplateBinding Background}"
                            CornerRadius="1">
                        <Border BorderThickness="1" BorderBrush="{StaticResource RibbonBarKeyTips_InnerBorderBrush}">
                            
                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" 
                                          VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
