	<Style x:Key="ApplicationButtonStyleOffice2010" TargetType="telerikRibbonView:RadRibbonDropDownButton">
		<Setter Property="Padding" Value="0" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="Margin" Value="2,0,0,0" />
		<Setter Property="Foreground" Value="{StaticResource ApplicationButtonType2010_Foreground}" />
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="DropDownIndicatorVisibility" Value="Collapsed" />
		<Setter Property="IsAutoSize" Value="False" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:RadRibbonDropDownButton">
					<Grid Width="46" Height="22">
						

						<Border BorderThickness="1 1 1 0" CornerRadius="2 2 0 0"
								Background="{StaticResource ApplicationButtonType2010_Background}"
								BorderBrush="{StaticResource ApplicationButtonType2010_OuterBorder}">
							<Border BorderThickness="1 1 1 0" CornerRadius="1 1 0 0"
									BorderBrush="{StaticResource ApplicationButtonType2010_InnerBorder}" />
						</Border>
						<Border x:Name="MouseOverPart" Opacity="0" BorderThickness="1 1 1 0" CornerRadius="2 2 0 0"
								Background="{StaticResource ApplicationButtonType2010_Background_MouseOver}"
								BorderBrush="{StaticResource ApplicationButtonType2010_OuterBorder_MouseOver}">
							<Border BorderThickness="1 1 1 0" CornerRadius="1 1 0 0"
									BorderBrush="{StaticResource ApplicationButtonType2010_InnerBorder_MouseOver}" />
						</Border>
						<Border x:Name="PressedPart" Opacity="0" BorderThickness="1 1 1 0" CornerRadius="2 2 0 0"
								Background="{StaticResource ApplicationButtonType2010_Background_Selected}"
								BorderBrush="{StaticResource ApplicationButtonType2010_OuterBorder_Selected}">
							<Border BorderThickness="1 1 1 0" CornerRadius="1 1 0 0"
									BorderBrush="{StaticResource ApplicationButtonType2010_InnerBorder_Selected}" />
						</Border>
						<Border x:Name="isOpenIndicator" Opacity="0" BorderThickness="1 1 1 0" CornerRadius="2 2 0 0"
								Background="{StaticResource ApplicationButtonType2010_Background_Selected}"
								BorderBrush="{StaticResource ApplicationButtonType2010_OuterBorder_Selected}">
							<Border BorderThickness="1 1 1 0" CornerRadius="1 1 0 0"
									BorderBrush="{StaticResource ApplicationButtonType2010_InnerBorder_Selected}" />
						</Border>

						<Grid Margin="{TemplateBinding Padding}">
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto" />
								<RowDefinition Height="*" />
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="Auto" />
							</Grid.ColumnDefinitions>

							<ContentPresenter Grid.ColumnSpan="3" Grid.RowSpan="2" Content="{TemplateBinding Content}"
									HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
									VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
									ContentTemplate="{TemplateBinding ContentTemplate}" />

							<Image x:Name="Image" Stretch="None" Grid.ColumnSpan="3" Grid.RowSpan="2"
									HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
									VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />

							<TextBlock x:Name="textRow1" Grid.Row="1" Margin="0,2,0,0" Text="{TemplateBinding TextRow1}"
									TextAlignment="Center" />

							<StackPanel x:Name="DropDownPanel" Grid.Row="1" Margin="0,14,0,0"
									HorizontalAlignment="Center" Orientation="Horizontal" MinHeight="14" MaxHeight="14"
									Height="14">
								<TextBlock x:Name="textRow2" Text="{TemplateBinding TextRow2}" />
							</StackPanel>
						</Grid>

						
						<Popup x:Name="DropDownPopup" HorizontalOffset="0" VerticalOffset="-22"
								AllowsTransparency="True" StaysOpen="False" Focusable="false" Placement="Bottom"
								IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
							
							
								<!--PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}">-->
								<Border x:Name="DropDownPopupBorder" BorderBrush="Transparent" Background="Transparent"
										BorderThickness="0" CornerRadius="1" Padding="0" MinWidth="33" MinHeight="33">
									<ContentControl x:Name="DropDownPopupContent"
											Content="{TemplateBinding DropDownContent}" HorizontalAlignment="Stretch"
											VerticalAlignment="Stretch" />
								</Border>
								
						</Popup>
						
						

						<Rectangle x:Name="DisabledIndicator" Visibility="Collapsed" Fill="#99FFFFFF" />
					</Grid>
					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="true">
							<Setter Property="Opacity" TargetName="MouseOverPart" Value="1" />
							<Setter Property="Opacity" TargetName="PressedPart" Value="0" />
						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter Property="Opacity" TargetName="PressedPart" Value="1" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
							<Setter Property="Opacity" TargetName="MouseOverPart" Value="0" />
							<Setter Property="Opacity" TargetName="PressedPart" Value="0" />
						</Trigger>
						<Trigger Property="IsOpen" Value="True">
							<Setter Property="Opacity" TargetName="isOpenIndicator" Value="1" />
						</Trigger>
						<Trigger Property="DropDownButtonPosition" Value="Top" />
						<Trigger Property="DropDownButtonPosition" Value="Left" />
						<Trigger Property="DropDownButtonPosition" Value="Bottom" />
						<Trigger Property="DropDownButtonPosition" Value="Right">
							<Setter TargetName="DropDownPanel" Property="Grid.Column" Value="2" />
							<Setter TargetName="DropDownPanel" Property="Grid.Row" Value="0" />
							<Setter TargetName="DropDownPanel" Property="Margin" Value="0,0,0,0" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="CurrentSize" Value="Large" />
						<Trigger Property="CurrentSize" Value="Medium">
							<Setter TargetName="textRow1" Property="Grid.Row" Value="0" />
							<Setter TargetName="textRow1" Property="Grid.Column" Value="0" />
							<Setter TargetName="textRow1" Property="Margin" Value="4,0,1,0" />
							<Setter TargetName="textRow1" Property="VerticalAlignment" Value="Center" />
							<Setter TargetName="textRow1" Property="TextAlignment" Value="Left" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="CurrentSize" Value="Small">
							<Setter TargetName="textRow1" Property="Visibility" Value="Collapsed" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="DropDownPlacement" Value="Absolute" />
						<Trigger Property="DropDownPlacement" Value="Bottom" />
						<Trigger Property="DropDownPlacement" Value="Center" />
						<Trigger Property="DropDownPlacement" Value="Top" />

						<Trigger Property="DropDownPlacement" Value="Left" />

						<Trigger Property="DropDownPlacement" Value="Right" />

					</ControlTemplate.Triggers>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
    <Style TargetType="telerikRibbonView:RadRibbonTab" x:Key="ContextualTabsStyle">
        <Setter Property="Padding" Value="0,1,0,1" />
        <Setter Property="Margin" Value="2 2 2 0" />
        <Setter Property="Background" Value="Pink" />
        <Setter Property="BorderThickness"  Value="1,0,0,0" />
        <Setter Property="Foreground" Value="Pink" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="BorderBrush" Value="{StaticResource TabStripTabDividerBrush}" />
        
        <Setter Property="FocusVisualStyle">
            <Setter.Value>
                <Style TargetType="Control">
                    <Setter Property="Visibility"
                            Value="Collapsed" />
                </Style>
            </Setter.Value>
        </Setter>
        
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonView:RadRibbonTab">
                    <Grid x:Name="RootPanel">
                        
                        <Rectangle x:Name="TabSeparator"
                                   Margin="0,0,-3,0"
                                   HorizontalAlignment="Right"
                                   Fill="{TemplateBinding BorderBrush}"
                                   VerticalAlignment="Stretch"
                                   Width="1"
                                   Visibility="Collapsed" />
                        <Grid x:Name="HeaderPanel">
                            <Border x:Name="MouseOverVisual" BorderBrush="#848484" Visibility="Collapsed"
                                    BorderThickness="1 1 1 0" CornerRadius="3 3 0 0">
                                <Border CornerRadius="2 2 0 0" BorderBrush="#99FFFFFF" BorderThickness="1 1 1 0">
                                    <Border CornerRadius="1 1 0 0" Opacity="0.6" Background="{TemplateBinding Background}">
                                        <Border.OpacityMask>
                                            <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                                <GradientStop Color="#00FFFFFF" Offset="0" />
                                                <GradientStop Color="#ffFFFFFF" Offset="1" />
                                                <GradientStop Color="#19FFFFFF" Offset="0.527" />
                                            </LinearGradientBrush>
                                        </Border.OpacityMask>
                                    </Border>
                                </Border>
                            </Border>
                            <Border x:Name="SelectedVisual" BorderBrush="#848484" Visibility="Collapsed"
                                    BorderThickness="1 1 1 0" CornerRadius="3 3 0 0" Background="#FFFFFFFF">
                                <Border CornerRadius="2 2 0 0" x:Name="InnerBackground" Background="{TemplateBinding Background}">
                                    <Border.OpacityMask>
                                        <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                            <GradientStop Color="#19FFFFFF" Offset="0" />
                                            <GradientStop Color="#59FFFFFF" Offset="0.173" />
                                            <GradientStop Color="#7FFFFFFF" Offset="0.191" />
                                            <GradientStop Color="Transparent" Offset="0.866" />
                                        </LinearGradientBrush>
                                    </Border.OpacityMask>
                                </Border>
                            </Border>
                            
                            <telerikNavigationPrimitives:TabItemContentPresenter Grid.Column="1" IsTabStop="False"
                                    x:Name="HeaderElement" Content="{TemplateBinding Header}"
                                    ContentTemplate="{TemplateBinding HeaderTemplate}"
                                    VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                    HorizontalAlignment="Center"  HorizontalContentAlignment="Center"
                                    VerticalAlignment="Center" Margin="5,0" Foreground="#000000" />
                        </Grid>
                        
                        <Grid x:Name="ContentPanel" Width="0">
                            <Border Name="OuterBorder" BorderThickness="1"
                                    CornerRadius="{StaticResource RibbonTabOuterCornerRadius}"
                                    BorderBrush="{StaticResource RibbonTabOuterBorderBrush}"
                                    Background="{StaticResource RibbonTabBackgroundBrush}">
                                <Border Name="InnerBorder" BorderThickness="1"
                                        CornerRadius="{StaticResource RibbonTabInnerCornerRadius}"
                                        BorderBrush="{StaticResource RibbonTabInnerBorderBrush}">
                                    <telerikRibbonViewPrimitives:RibbonScrollViewer telerik:StyleManager.Theme="{StaticResource Theme}">
                                        <ItemsPresenter HorizontalAlignment="Left" />
                                    </telerikRibbonViewPrimitives:RibbonScrollViewer>
                                </Border>
                            </Border>
                        </Grid>
                    </Grid>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="MouseOverVisual" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter TargetName="SelectedVisual"
                                    Property="Visibility"
                                    Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Opacity"  Value="0.5"  TargetName="HeaderElement" />
                        </Trigger>
                        <Trigger Property="IsContextualTabVisible"
                                 Value="False">
                            <Setter Property="Visibility"
                                    TargetName="HeaderPanel"
                                    Value="Collapsed" />
                        </Trigger>
                        
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsMouseOver"
                                           Value="True" />
                                <Condition Property="IsSelected"
                                           Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="InnerBackground"
                                    Property="Background"
                                    Value="#fbe0a1" />
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="telerikRibbonView:RadRibbonContextualGroup" x:Key="{telerik:ThemeResourceKey ThemeType=
		external:SummerThemeExternal , ElementType=telerikRibbonView:RadRibbonContextualGroup}"
		 >
		<Setter Property="Foreground" Value="White" />
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="HeaderAlignment" Value="Center" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:RadRibbonContextualGroup">
					<Grid x:Name="HeaderGrid">
						<Border Margin="1 0 0 0" Background="{TemplateBinding Color}">
							<Border.OpacityMask>
								<RadialGradientBrush Center="0.507,1.136" GradientOrigin="0.5,1.136" RadiusX="0.736"
										RadiusY="1.08">
									<GradientStop Offset="1" />
									<GradientStop Color="#B2FFFFFF" Offset="0.325" />
								</RadialGradientBrush>
							</Border.OpacityMask>
						</Border>
						<Border BorderThickness="0 0 0 1" Background="{TemplateBinding Color}"
								VerticalAlignment="Bottom">
							<Border.OpacityMask>
								<LinearGradientBrush EndPoint="0.007,0.986" StartPoint="1.006,0.986">
									<GradientStop Offset="0" Color="#26000000" />
									<GradientStop Color="White" Offset="0.5" />
									<GradientStop Offset="1" Color="#26000000" />
								</LinearGradientBrush>
							</Border.OpacityMask>
						</Border>
						<ContentPresenter VerticalAlignment="Center" Content="{TemplateBinding Header}"
								HorizontalAlignment="{TemplateBinding HeaderAlignment}"
								ContentTemplate="{TemplateBinding HeaderTemplate}" />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="telerikRibbonView:QuickAccessToolBar" 
        x:Key="{telerik:ThemeResourceKey ThemeType=
		external:SummerThemeExternal , ElementType=telerikRibbonView:QuickAccessToolBar}"
		
		>
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Background" Value="{StaticResource QatBelowBackgroundBrush}" />
		<Setter Property="BorderBrush" Value="{StaticResource QatBelowOuterBorderBrush}" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
		<Setter Property="Margin" Value="0" />
		<Setter Property="CustomizationMenuVisibility" Value="Visible" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:QuickAccessToolBar">
					<Grid MinHeight="22" x:Name="RootElement">
						
						<Grid x:Name="BelowBackgroundGrid" Visibility="Collapsed">
							<Border BorderThickness="1" CornerRadius="2 2 0 0" Margin="1 1 0 0"
									Background="{TemplateBinding Background}" />
							<Border BorderThickness="1" CornerRadius="2 2 0 0"
									BorderBrush="{TemplateBinding BorderBrush}" Margin="0,0,1,1" />
						</Grid>

						<Grid>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="13" />
							</Grid.ColumnDefinitions>

							<Grid x:Name="AboveBackgroundGridWrapper">
								<Grid x:Name="AboveBackgroundGrid" Height="22" />
							</Grid>

							<Grid HorizontalAlignment="Left" x:Name="PART_StripPanelGrid" Grid.Column="0"
									Margin="3 0 0 0">
								<StackPanel x:Name="PART_StripPanel" Orientation="Horizontal" HorizontalAlignment="Left" />
							</Grid>

							<Popup Grid.Column="1" x:Name="PART_Overflow">
								<Border Margin="0" MinWidth="55" MinHeight="33">
									<Border BorderThickness="1" Padding="1,2,1,2" Background="Blue" BorderBrush="Green">
										<StackPanel x:Name="PART_OverflowPanel" Orientation="Vertical"
												VerticalAlignment="Top" />
									</Border>
								</Border>
							</Popup>

							<telerikRibbonView:RadRibbonDropDownButton
									Visibility="{TemplateBinding CustomizationMenuVisibility}"
									DropDownIndicatorVisibility="Collapsed" Width="13" Height="22"
									HorizontalAlignment="Left" Grid.Column="1" VerticalAlignment="Top" Padding="0,2,0,2">
								<telerikRibbonView:RadRibbonDropDownButton.DropDownContent>
									<StackPanel>
										<telerikRibbonView:RadGroupHeader Content="Customize Quick Access Toolbar"
												telerik:LocalizationManager.ResourceKey="RibbonViewQATCustomize" />
										<telerikNavigation:RadContextMenu BorderThickness="0">
											<telerikNavigation:RadMenuItem x:Name="TogglePositionMenuItem"
													Header="Show Below the Ribbon."
													telerik:LocalizationManager.ResourceKey="RibbonViewQATShowBelow" />
											<telerikNavigation:RadMenuItem IsSeparator="True" Padding="0" Margin="0" />
											<telerikNavigation:RadMenuItem x:Name="ToggleMinimizeMenuItem"
													Header="Minimize the Ribbon."
													telerik:LocalizationManager.ResourceKey="RibbonViewQATMinimize" />
										</telerikNavigation:RadContextMenu>
									</StackPanel>
								</telerikRibbonView:RadRibbonDropDownButton.DropDownContent>
								<Path x:Name="MainPopupButtonPath" HorizontalAlignment="Center"
										VerticalAlignment="Center" Margin="4,7,2,0"
										Data="M 0,-2.5 L 0,-1.5 L 7,-1.5 L 7,-2.5 M 0,0 L 3.5,3.5 L 7,0"
										Fill="{StaticResource QatButtonArrow}"
										Stroke="{StaticResource QatButtonArrowHighlight}" />
							</telerikRibbonView:RadRibbonDropDownButton>
						</Grid>
					</Grid>
					
					<ControlTemplate.Triggers>
						<Trigger Property="Position" Value="BelowRibbon">
							<Setter Property="Visibility" Value="Collapsed" TargetName="AboveBackgroundGrid" />
							<Setter Property="Visibility" Value="Visible" TargetName="BelowBackgroundGrid" />
							<Setter Property="Margin" Value="2,2,0,2" TargetName="PART_StripPanelGrid" />
							<Trigger.EnterActions>
								<BeginStoryboard>
									<Storyboard>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="TogglePositionMenuItem"
												Storyboard.TargetProperty="Header">
											<DiscreteObjectKeyFrame KeyTime="0"
													telerik:LocalizationManager.ResourceKey="RibbonViewQATShowAbove" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</BeginStoryboard>
							</Trigger.EnterActions>
							<Trigger.ExitActions>
								<BeginStoryboard>
									<Storyboard>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="TogglePositionMenuItem"
												Storyboard.TargetProperty="Header">
											<DiscreteObjectKeyFrame KeyTime="0"
													telerik:LocalizationManager.ResourceKey="RibbonViewQATShowBelow" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</BeginStoryboard>
							</Trigger.ExitActions>
						</Trigger>
					</ControlTemplate.Triggers>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="telerikRibbonViewPrimitives:WindowTitle" x:Key="{telerik:ThemeResourceKey ThemeType=
		external:SummerThemeExternal , ElementType=telerikRibbonViewPrimitives:WindowTitle}"
		 >
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="VerticalAlignment" Value="Center" />
		<Setter Property="FontSize" Value="11" />
		<Setter Property="IsHitTestVisible" Value="false" />
		<Setter Property="IsActive" Value="True" />
		<Setter Property="HorizontalAlignment" Value="Center" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonViewPrimitives:WindowTitle">
					<StackPanel Orientation="Horizontal" HorizontalAlignment="{TemplateBinding HorizontalAlignment}">
						<TextBlock x:Name="Title" Text="{TemplateBinding Title}"
								Foreground="{StaticResource RibbonTitlebarTitleBrush}" TextTrimming="WordEllipsis" />
						<TextBlock x:Name="Divider" Text=" - " Visibility="{TemplateBinding DividerVisibility}"
								telerik:LocalizationManager.ResourceKey="RibbonViewWindowTitleDivider"
								Foreground="{StaticResource RibbonTitlebarApplicationNameBrush}" />
						<TextBlock x:Name="ApplicationName" Text="{TemplateBinding ApplicationName}"
								Foreground="{StaticResource RibbonTitlebarApplicationNameBrush}"
								TextTrimming="WordEllipsis" />
					</StackPanel>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style x:Key="HelpButtonStyle" TargetType="telerikRibbonView:RadRibbonButton">
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="VerticalAlignment" Value="Center" />
	</Style>
	<Style x:Key="MinimizeButtonStyle" TargetType="telerikRibbonView:RadRibbonToggleButton">
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Margin" Value="1" />
		<Setter Property="Padding" Value="2" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Size" Value="Small" />
		<Setter Property="Height" Value="21" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:RadRibbonToggleButton">
					<Grid Width="20">
						

						<Border x:Name="OuterBorder" CornerRadius="2"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}">
							<Border x:Name="InnerBorder" BorderBrush="Transparent" BorderThickness="1" />
						</Border>

						<Path x:Name="arrow" Fill="White" Stretch="None" Stroke="Black" VerticalAlignment="Center"
								HorizontalAlignment="Center"
								Data="M0.25,4 L3.75,0.25 L4.75,0.25 L8.25,3.875 L8.3125,5.5 L7.25,6.627 L6.25,6.627 L4.25,4.627 L2.25,6.627 L1.25,6.627 L0.25,5.627 z"
								StrokeThickness="0.5" RenderTransformOrigin="0.5 0.5">
							<Path.RenderTransform>
								<RotateTransform x:Name="directionRotation" Angle="0" />
							</Path.RenderTransform>
						</Path>
					</Grid>

					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="True">
							<Setter Property="Background" TargetName="OuterBorder"
									Value="{StaticResource RadRibbonBarButtonOverBackground}" />
							<Setter Property="BorderBrush" TargetName="OuterBorder"
									Value="{StaticResource RadRibbonBarButtonOverBorder}" />
							<Setter Property="BorderBrush" TargetName="InnerBorder"
									Value="{StaticResource RadRibbonBarButtonOverBorderInner}" />
						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter Property="Background" TargetName="OuterBorder"
									Value="{StaticResource RadRibbonBarButtonPressedBackground}" />
							<Setter Property="BorderBrush" TargetName="OuterBorder"
									Value="{StaticResource RadRibbonBarButtonPressedBorder}" />
							<Setter Property="BorderBrush" TargetName="InnerBorder"
									Value="{StaticResource RadRibbonBarButtonPressedBorderInner}" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
							<Setter Property="Opacity" TargetName="arrow" Value="0.5" />
						</Trigger>
						<Trigger Property="IsChecked" Value="True">
							<Setter TargetName="arrow" Property="LayoutTransform">
								<Setter.Value>
									<TransformGroup>
										<RotateTransform Angle="180" />
									</TransformGroup>
								</Setter.Value>
							</Setter>
						</Trigger>
					</ControlTemplate.Triggers>
					

				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<!-- RibbonView -->
	<Style TargetType="telerikRibbonView:RadRibbonView"
		x:Key="{telerik:ThemeResourceKey ThemeType=
		external:SummerThemeExternal , ElementType=telerikRibbonView:RadRibbonView}"
		 >
		<Setter Property="ItemsPanel">
			<Setter.Value>
				<ItemsPanelTemplate>
					<telerikRibbonViewPrimitives:RibbonTabStripPanel />
				</ItemsPanelTemplate>
			</Setter.Value>
		</Setter>
		
		<Setter Property="FocusManager.IsFocusScope" Value="True" />
		
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Background" Value="{StaticResource RibbonBackgroundBrush}" />
		<Setter Property="Foreground" Value="{StaticResource RibbonForegroundBrush}" />
		<Setter Property="FontSize" Value="10.8" />
		<Setter Property="TitleBarBackground" Value="{StaticResource RibbonTitlebarBackgroundBrush}" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="VerticalAlignment" Value="Top" />
		<Setter Property="ApplicationButtonStyle" Value="{StaticResource ApplicationButtonStyleOffice2010}" />
		<Setter Property="HelpButtonStyle" Value="{StaticResource HelpButtonStyle}" />
		<Setter Property="MinimizeButtonStyle" Value="{StaticResource MinimizeButtonStyle}" />
        <Setter Property="ContextualTabsStyle"
                Value="{StaticResource ContextualTabsStyle}" />
		<Setter Property="HelpButtonImageSource" Value="/Telerik.Windows.Controls.RibbonView;component/HelpIcon.png" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:RadRibbonView">
					<Grid x:Name="RootPanel">
						<Grid.RowDefinitions>
                            <RowDefinition MaxHeight="28" />
							<RowDefinition Height="24" />
							<RowDefinition Height="Auto" />
							<RowDefinition Height="Auto" />
						</Grid.RowDefinitions>

						<Grid Height="28" Visibility="{TemplateBinding TitleBarVisibility}">
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="Auto" />
							</Grid.ColumnDefinitions>
							<Border x:Name="titleBarBackground" BorderThickness="0,0,0,1" Grid.ColumnSpan="3"
									BorderBrush="{StaticResource RibbonTitlebarBorderBrush}"
									Background="{TemplateBinding TitleBarBackground}" />
                            <ContentPresenter x:Name="AboveToolBarHost" VerticalAlignment="Bottom"/>
							<telerikRibbonViewPrimitives:WindowTitle x:Name="WindowTitle"  Grid.Column="1" Title="{TemplateBinding Title}"
									ApplicationName="{TemplateBinding ApplicationName}"
									Style="{TemplateBinding WindowTitleStyle}" />
							<StackPanel x:Name="ContextualGroupsHeaders" Orientation="Horizontal" Grid.Column="2" />
						</Grid>

						<Rectangle Grid.Row="1" Grid.RowSpan="3" Fill="{TemplateBinding Background}" />

                        <ContentPresenter x:Name="SelectedTabContent" Grid.Row="2" Height="91" Margin="1,-1,1,1" />

                        <Grid x:Name="TopGrid" Grid.Row="1">
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="Auto" />
							</Grid.ColumnDefinitions>

							<telerikRibbonView:RadRibbonDropDownButton x:Name="ApplicationButton" 									
									IsOpen="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsApplicationMenuOpen, Mode=TwoWay}"
									SmallImage="{TemplateBinding ApplicationButtonImageSource}"
									Content="{TemplateBinding ApplicationButtonContent}"
									Visibility="{TemplateBinding ApplicationButtonVisibility}"
									Style="{TemplateBinding ApplicationButtonStyle}" Margin="0 0 3 0" />

							<telerikRibbonViewPrimitives:RibbonScrollViewer Grid.Column="1" x:Name="TabsScrollViewer"
									telerik:StyleManager.Theme="{StaticResource Theme}">
								<ItemsPresenter />
							</telerikRibbonViewPrimitives:RibbonScrollViewer>

							<StackPanel Orientation="Horizontal" Grid.Column="2">
								<telerikRibbonView:RadRibbonToggleButton Style="{TemplateBinding MinimizeButtonStyle}"
										Visibility="{TemplateBinding MinimizeButtonVisibility}"
										IsEnabled="{TemplateBinding IsMinimizable}"
										IsChecked="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsMinimized, Mode=TwoWay}" />
								<telerikRibbonView:RadRibbonButton telerik:StyleManager.Theme="{StaticResource Theme}"
										x:Name="HelpButton" Visibility="{TemplateBinding HelpButtonVisibility}"
										SmallImage="{TemplateBinding HelpButtonImageSource}"
										Style="{TemplateBinding HelpButtonStyle}" />
								<ContentPresenter Content="{TemplateBinding TabStripAdditionalContent}" />
							</StackPanel>
						</Grid>

						<ContentPresenter x:Name="BelowToolBarHost" Grid.Row="3" />
						
						<Popup x:Name="TabContentPopup" VerticalOffset="-2"
								PlacementTarget="{Binding ElementName=TopGrid}" AllowsTransparency="True">
							<ContentPresenter x:Name="SelectedTabContentPopup" Height="91" />
						</Popup>
						
						
					</Grid>
					
					<ControlTemplate.Triggers>
						<Trigger Property="IsHostedInRibbonWindow" Value="true">
							<Setter TargetName="titleBarBackground" Property="Visibility" Value="Collapsed" />
						</Trigger>
					</ControlTemplate.Triggers>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
    

	<Style TargetType="telerikRibbonView:RadRibbonButton"
		
		x:Key="{telerik:ThemeResourceKey ThemeType=
		external:SummerThemeExternal , ElementType=telerikRibbonView:RadRibbonButton}"
		 >
		<Setter Property="Padding" Value="3" />
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="HorizontalAlignment" Value="Left" />
		<Setter Property="HorizontalContentAlignment" Value="Left" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonView:RadRibbonButton">
					<Grid>
						

						<Border x:Name="buttonSurface" BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}"
								CornerRadius="2">
							<Border x:Name="buttonSurfaceInner" BorderBrush="Transparent"
									BorderThickness="{TemplateBinding BorderThickness}" />
						</Border>

						<Grid Margin="{TemplateBinding Padding}" x:Name="ContentGrid">
							<ContentPresenter Name="ContentPresenter" Content="{TemplateBinding Content}"
									ContentTemplate="{TemplateBinding ContentTemplate}"
									VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
									HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
								RecognizesAccessKey="True"
								 />

								<Grid>
									<Grid.RowDefinitions>
										<RowDefinition Height="Auto" />
										<RowDefinition Height="*" MaxHeight="28" />
									</Grid.RowDefinitions>
									<Grid.ColumnDefinitions>
										<ColumnDefinition Width="Auto" />
										<ColumnDefinition Width="*" />
									</Grid.ColumnDefinitions>

									<Image Name="Image" />

									<TextBlock Name="textRow1" Grid.Row="1" Margin="0,2,0,0"
											Text="{TemplateBinding TextRow1}" TextAlignment="Center" />
									<TextBlock Name="textRow2" Grid.Row="1" Margin="0,14,0,0"
											Text="{TemplateBinding TextRow2}" TextAlignment="Center" />
								</Grid>
						</Grid>
					</Grid>

					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="True">
							<Setter Property="BorderBrush" TargetName="buttonSurface"
									Value="{StaticResource RadRibbonBarButtonOverBorder}" />
							<Setter Property="Background" TargetName="buttonSurface"
									Value="{StaticResource RadRibbonBarButtonOverBackground}" />
							<Setter Property="BorderBrush" TargetName="buttonSurfaceInner"
									Value="{StaticResource RadRibbonBarButtonOverBorderInner}" />
						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter Property="BorderBrush" TargetName="buttonSurface"
									Value="{StaticResource RadRibbonBarButtonPressedBorder}" />
							<Setter Property="Background" TargetName="buttonSurface"
									Value="{StaticResource RadRibbonBarButtonPressedBackground}" />
							<Setter Property="BorderBrush" TargetName="buttonSurfaceInner"
									Value="{StaticResource RadRibbonBarButtonPressedBorderInner}" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
							<Setter TargetName="ContentGrid" Property="Opacity" Value="0.5" />
						</Trigger>
						<Trigger Property="IsDefault" Value="True" />
						<Trigger Property="CurrentSize" Value="Large" />
						<Trigger Property="CurrentSize" Value="Medium">
							<Setter TargetName="textRow1" Property="Grid.Row" Value="0" />
							<Setter TargetName="textRow1" Property="Grid.Column" Value="1" />
							<Setter TargetName="textRow1" Property="Margin" Value="4,0,1,0" />
							<Setter TargetName="textRow1" Property="VerticalAlignment" Value="Center" />
							<Setter TargetName="textRow1" Property="TextAlignment" Value="Left" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="CurrentSize" Value="Small">
							<Setter TargetName="textRow1" Property="Visibility" Value="Collapsed" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</Trigger>
					</ControlTemplate.Triggers>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
	

	<!-- For  ComboBox, ... -->
	<Style TargetType="telerik:PickerToggleButton" x:Key="ToogleButtonStyle" >
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="BorderBrush" Value="{StaticResource ExpanderButtonNormal_Border}" />
		<Setter Property="Background" Value="{StaticResource ExpanderButtonNormal_Background}" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerik:PickerToggleButton">
					<Grid>
                        
						<Border x:Name="Chrome"
								BorderBrush="{TemplateBinding BorderBrush}"
								BorderThickness="1">
							<Border x:Name="ChromeInner"
									BorderBrush="{TemplateBinding Background}"
									BorderThickness="1"
									Background="{TemplateBinding Background}" />
						</Border>

						<Border x:Name="ChromeCheck" Visibility="Collapsed"
								BorderBrush="{StaticResource ExpandButtonPressed_OuterBorder}"
								BorderThickness="1">
							<Border BorderBrush="{StaticResource ExpanderButtonPressed_Border}"
									BorderThickness="1"
									Background="{StaticResource ExpanderButtonPressed_Background}" />
						</Border>

						<Path x:Name="Path"
								Data="M-0.7,0.45 L6.2,0.45 L2.814,5.509 L2.814,5.509 z" 
								Fill="{StaticResource RibbonGroupButtonArrow}" 
								Stretch="Fill" Width="5" Height="3"
								HorizontalAlignment="Right"
								Margin="0 0 4 0"
								VerticalAlignment="Center" />

						<ContentPresenter Content="{TemplateBinding Content}"
								ContentTemplate="{TemplateBinding ContentTemplate}" />

					</Grid>

					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="true">
							<Setter TargetName="Chrome"  Property="BorderBrush" 
									Value="{StaticResource ExpandButtonOver_OuterBorder}" />
							<Setter TargetName="ChromeInner" Property="BorderBrush"
									Value="{StaticResource ExpanderButtonOver_Border}" />
							<Setter TargetName="ChromeInner" Property="Background" 
									Value="{StaticResource ExpanderButtonOver_Background}" />
						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter TargetName="Chrome" Property="BorderBrush"
									Value="{StaticResource ExpandButtonPressed_OuterBorder}" />
							<Setter TargetName="ChromeInner" Property="BorderBrush"
									Value="{StaticResource ExpanderButtonPressed_Border}" />
							<Setter TargetName="ChromeInner" Property="Background"
									Value="{StaticResource ExpanderButtonPressed_Background}" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False" />
						<Trigger Property="IsChecked" Value="True">
							<Setter TargetName="ChromeCheck" Property="Visibility" Value="Collapsed"/>
						</Trigger>
					</ControlTemplate.Triggers>
					

				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<!-- NonEditableComboBoxTemplate -->
	<ControlTemplate x:Key="NonEditableComboBox" TargetType="telerikRibbonView:RadRibbonComboBox">
		<Grid Name="MainGrid">
            

			<Border x:Name="ContentPresenterBorder" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition  Width="*"/>
						<ColumnDefinition Width="20" />
					</Grid.ColumnDefinitions>

					<telerik:PickerToggleButton Margin="-1" x:Name="PART_DropDownButton"
                                Grid.ColumnSpan="2"
								Style="{StaticResource ToogleButtonStyle}"
								IsChecked="{TemplateBinding IsDropDownOpen}"
								IsEnabled="{TemplateBinding IsEnabled}" />


					<ContentPresenter x:Name="ContentPresenterElement"
								HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
								Margin="{TemplateBinding Padding}"
								VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
								IsHitTestVisible="False"
								ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}"
								Content="{TemplateBinding SelectionBoxItem}" />
				</Grid>
			</Border>

			
			<Popup x:Name="PART_Popup" AllowsTransparency="True"
				   PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}" >
			
			
					
				<Border x:Name="DropDownBorder"
						HorizontalAlignment="Stretch"
						MinWidth="{TemplateBinding MinDropDownWidth}"
						MaxHeight="{TemplateBinding MaxDropDownHeight}">
					<Grid>
						<Border Background="{StaticResource RibbonPopupBackgroundBrush}" 
								BorderThickness="1"
                                BorderBrush="{StaticResource RibbonPopupOuterBorderBrush}" >
							<Border BorderBrush="{StaticResource RibbonPopupInnerBorderBrush}" BorderThickness="1">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <telerik:RadButton x:Name="PART_ClearButton" Grid.Row="0"
								            Margin="-1 -1 -1 0"
								            Visibility="{TemplateBinding ClearSelectionButtonVisibility}"
								            Content="{TemplateBinding ClearSelectionButtonContent}"
								            telerik:StyleManager.Theme="{StaticResource Theme}" />

                                    <ScrollViewer x:Name="PART_ScrollViewer" Padding="0 1" Grid.Row="1"
											VerticalScrollBarVisibility="Auto" BorderThickness="0"
                                            telerik:StyleManager.Theme="{StaticResource Theme}"
                                            
                                            
                                            CanContentScroll="True"
                                            >
                                        
                                        <ItemsPresenter />
                                    </ScrollViewer>
                                </Grid>
                            </Border>
						</Border>
					</Grid>
				</Border>
			
			</Popup>
			
			


			<Rectangle x:Name="DisabledVisual" Visibility="Collapsed"  
					Fill="{StaticResource DisabledBrush}" />
		</Grid>
		
		
		<ControlTemplate.Triggers>
			<Trigger Property="IsMouseOver" Value="True" />
			<Trigger Property="IsKeyboardFocusWithin" Value="True" />
			<Trigger Property="IsEnabled" Value="False">
				<Setter TargetName="DisabledVisual" Property="Visibility" Value="Visible" />
			</Trigger>
		</ControlTemplate.Triggers>
		
		
	</ControlTemplate>
	<!-- EditableComboBoxTemplate -->
	<ControlTemplate x:Key="EditableComboBox" TargetType="telerikRibbonView:RadRibbonComboBox">
		<Grid>
            
			<Border x:Name="ContentPresenterBorder" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="*" />
						<ColumnDefinition Width="Auto" />
					</Grid.ColumnDefinitions>
					<telerik:PickerTextBox Grid.Column="0" Name="PART_EditableTextBox"
								Margin="{TemplateBinding Padding}"
								HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
								VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
								IsReadOnly="{TemplateBinding IsReadOnly}"/>

					<telerik:PickerToggleButton MinWidth="14" Margin="0,-1,-1,-1" x:Name="PART_DropDownButton" Grid.Column="1"
								Style="{StaticResource ToogleButtonStyle}"
								IsChecked="{TemplateBinding IsDropDownOpen}"
								IsEnabled="{TemplateBinding IsEnabled}" />

                    <TextBlock x:Name="Watermark" Text="{TemplateBinding EmptyText}" Opacity=".5" IsHitTestVisible="False" 
					   VerticalAlignment="Center" HorizontalAlignment="Left" Margin="5,0,0,0" Visibility="Collapsed" />
                </Grid>
			</Border>

			
			<Popup x:Name="PART_Popup" AllowsTransparency="True"
				   PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}" >
			
			
					
				<Border x:Name="DropDownBorder"
						HorizontalAlignment="Stretch"
						MinWidth="{TemplateBinding MinDropDownWidth}"
						MaxHeight="{TemplateBinding MaxDropDownHeight}">
					<Grid>
						<Border Background="{StaticResource RibbonPopupBackgroundBrush}" 
								BorderThickness="1"  
                                BorderBrush="{StaticResource RibbonPopupOuterBorderBrush}" >
							<Border BorderBrush="{StaticResource RibbonPopupInnerBorderBrush}" BorderThickness="1" >
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <telerik:RadButton x:Name="PART_ClearButton" Grid.Row="0"
								            Margin="-1 -1 -1 0"
								            Visibility="{TemplateBinding ClearSelectionButtonVisibility}"
								            Content="{TemplateBinding ClearSelectionButtonContent}"
								            telerik:StyleManager.Theme="{StaticResource Theme}" />

                                    <ScrollViewer x:Name="PART_ScrollViewer" Padding="0 1" Grid.Row="1"
											VerticalScrollBarVisibility="Auto" BorderThickness="0"	
                                            telerik:StyleManager.Theme="{StaticResource Theme}" 
                                            
                                            
                                            CanContentScroll="True"
                                             >

                                        <ItemsPresenter />
                                    </ScrollViewer>
                                </Grid>
                            </Border>
						</Border>
					</Grid>
				</Border>
			
			</Popup>
			
			

			<Rectangle x:Name="DisabledVisual" Visibility="Collapsed" Fill="{StaticResource DisabledBrush}" />
		</Grid>
		
		<ControlTemplate.Triggers>
			<Trigger Property="IsMouseOver" Value="True" >
				<Setter TargetName="ContentPresenterBorder" Property="Background" 
						Value="{StaticResource RibbonComboBoxHotBackgroundBrush}" />
				<Setter TargetName="ContentPresenterBorder" Property="BorderBrush"
						Value="{StaticResource RibbonComboBoxHotBorderBrush}" />
				<Setter TargetName="PART_DropDownButton" Property="IsEnabled" Value="True" />
				<Setter TargetName="PART_DropDownButton" Property="Background"
						Value="{StaticResource RibbonComboBoxButtonBackgroundBrush}" />
				<Setter TargetName="PART_DropDownButton" Property="BorderBrush"
						Value="{StaticResource RibbonComboBoxBorderBrush}" />
			</Trigger>
            <Trigger Property="IsWatermarkVisible" Value="True">
                <Setter TargetName="Watermark" Property="Visibility" Value="Visible" />
            </Trigger>
            <Trigger Property="IsKeyboardFocusWithin" Value="True" />
            <Trigger Property="IsEnabled" Value="False" >
                <Setter TargetName="DisabledVisual" Property="Visibility" Value="Visible" />
            </Trigger>
        </ControlTemplate.Triggers>
		
	</ControlTemplate>
	<!-- RadComboBox -->
	<Style TargetType="telerikRibbonView:RadRibbonComboBox"
            x:Key="{telerik:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikRibbonView:RadRibbonComboBox}"
        >
