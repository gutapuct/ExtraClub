    <!--TabItem Template-->
    <ControlTemplate x:Key="TabItemTemplate" TargetType="telerikRibbonView:RadRibbonBackstageItem">
		<Grid x:Name="wrapper" Background="Transparent">
            

                <!-- Mouse Over -->
                <Border Name="OuterBorder" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="0 1" Margin="0 0 1 0" CornerRadius="0">
                    <Border Name="InnerBorder" BorderBrush="Transparent" BorderThickness="0 1" CornerRadius="0" Background="{TemplateBinding Background}" />
                </Border>
                
                <!-- Selected -->
                <Grid x:Name="SelectedVisualState" Visibility="Collapsed">
                    <Rectangle Stroke="{StaticResource RibbonnBackStage_Item_OuterBorderSelected}" Fill="{StaticResource RibbonnBackStage_Item_OuterBackgroundSelected}" />
                    <Border BorderThickness="0" Margin="0,0,0,-1" Background="{StaticResource RibbonnBackStage_Item_InnerBackgroundSelected}">
                        <Rectangle Fill="{StaticResource RibbonnBackStage_Item_InnerBorderSelected}" />
                    </Border>
                </Grid>
                
                <StackPanel Orientation="Horizontal" x:Name="ContentPanel" Margin="20 10">
                    <Image x:Name="Image" Source="{TemplateBinding Icon}" MaxWidth="16" MaxHeight="16" Stretch="None" Margin="0 0 5 0" Visibility="Collapsed" />
                <ContentControl x:Name="HeaderElement"
                                Content="{TemplateBinding Header}"
                                Foreground="{TemplateBinding Foreground}"
					            ContentTemplate="{TemplateBinding HeaderTemplate}"
					            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
					            
                                ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
					            SnapsToDevicePixels="True"
                                 />
                </StackPanel>

            <Path Name="path" Data="M129.5889,9.2226782 L129.56734,24.204977 L121.53184,16.456234 z " Fill="{StaticResource RibbonnBackStage_ArrowBackground}" HorizontalAlignment="Right" 
                      Height="15" Margin="0,0,-1,0" StrokeStartLineCap="Flat" Stretch="Fill" StrokeEndLineCap="Flat"  StrokeThickness="1" 
                      StrokeMiterLimit="10" StrokeLineJoin="Miter" VerticalAlignment="Center" Width="8" Visibility="Collapsed" />
		</Grid>
        
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="true">
                <Setter TargetName="path" Property="Visibility" Value="Visible" />
            </Trigger>
            <Trigger Property="IsSelectable" Value="false">
                <Setter TargetName="ContentPanel" Property="Margin" Value="20 5" />
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsMouseOver" Value="True" />
                    <Condition Property="IsSelectable" Value="True" />
                </MultiTrigger.Conditions>
                <Setter Property="BorderBrush" TargetName="OuterBorder" Value="{StaticResource RibbonnBackStage_Item_OuterBorderMouseOVer}" />
                <Setter Property="BorderBrush" TargetName="InnerBorder" Value="{StaticResource RibbonnBackStage_Item_InnerBorderMouseOVer}" />
                <Setter Property="Background" TargetName="InnerBorder" Value="{StaticResource RibbonnBackStage_Item_BackgroundMouseOVer}" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsSelectable" Value="True" />
                    <Condition Property="IsSelected" Value="True" />
                </MultiTrigger.Conditions>
                <Setter TargetName="HeaderElement" Property="Foreground" Value="{StaticResource RibbonBackstage_Item_ForegroundSelected}" />
                <Setter Property="Visibility" TargetName="SelectedVisualState" Value="Visible" />
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsMouseOver" Value="True" />
                    <Condition Property="IsSelectable" Value="False" />
                </MultiTrigger.Conditions>
                <Setter TargetName="ContentPanel" Property="Margin" Value="20 5" />
                <Setter TargetName="OuterBorder" Property="Margin" Value="5 0" />
                <Setter TargetName="OuterBorder" Property="BorderThickness" Value="1" />
                <Setter TargetName="OuterBorder" Property="BorderBrush" Value="{StaticResource RibbonnBackStage_Item_OuterBorderMouseOVer}" />
                <Setter TargetName="OuterBorder" Property="CornerRadius" Value="{StaticResource RibbonnBackStage_Button_OuterCornerRadius}" />
                <Setter TargetName="InnerBorder" Property="BorderThickness" Value="1" />
                <Setter TargetName="OuterBorder" Property="BorderBrush"  Value="{StaticResource RibbonnBackStage_Item_OuterBorderMouseOVer}" />
                <Setter TargetName="InnerBorder" Property="BorderBrush" Value="{StaticResource RibbonnBackStage_Item_InnerBorderMouseOVer}" />
                <Setter TargetName="InnerBorder" Property="Background" Value="{StaticResource RibbonnBackStage_Item_BackgroundMouseOVer}" />
            </MultiTrigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="ContentPanel" Property="Opacity" Value="0.5" />
            </Trigger>
        </ControlTemplate.Triggers>
        
    </ControlTemplate>

