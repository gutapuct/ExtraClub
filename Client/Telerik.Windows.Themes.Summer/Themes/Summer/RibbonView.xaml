                        <Trigger Property="IsSelected" Value="True">
                            <Setter TargetName="SelectedIndicator" Property="Opacity" Value="1" />
                            <!--<Setter TargetName="mouseOverIndicator" Property="BorderBrush" Value="{StaticResource RadRibbonBarButtonPressedBorderInner}" />-->
                        </Trigger>
