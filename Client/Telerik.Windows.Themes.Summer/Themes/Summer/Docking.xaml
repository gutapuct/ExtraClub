						<!-- Vertical Template -->
						<Grid x:Name="VerticalTemplate" Visibility="Collapsed" Width="6">
							<!-- Just show the faint gray grid splitter rectangle with no other details -->
						</Grid>

					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
