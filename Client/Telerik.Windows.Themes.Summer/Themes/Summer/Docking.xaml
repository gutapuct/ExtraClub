			<telerikNavigationPrimitives:TabItemContentPresenter x:Name="HeaderElement" Margin="5 3 -10 3" Content="{TemplateBinding Header}" ContentTemplate="{TemplateBinding HeaderTemplate}" Padding="{TemplateBinding Padding}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" />

			<Border x:Name="BottomBorderUnselected" Visibility="Collapsed" BorderThickness="0 0 0 1" BorderBrush="{StaticResource PaneGroup_BorderBrush}" Margin="0 0 -32 0" />
			<Border x:Name="BottomBorderSelected" Visibility="Collapsed" BorderThickness="0 0 0 1" BorderBrush="{StaticResource PaneGroup_DocumentFrameL2_Brush}" Margin="2 0 -30 -1" />

		</Grid>
		
        
        <ControlTemplate.Triggers>
			<Trigger Property="IsEnabled" Value="False">
				<Setter TargetName="RightShapeOuterBorder" Property="Stroke" Value="{StaticResource PaneDocumentTabOuterBorder_Disabled}" />
				<Setter TargetName="RightShapeInnerBorder" Property="Stroke" Value="{StaticResource PaneDocumentTabInnerBorder_Disabled}" />
				<Setter TargetName="RightShapeBackground" Property="Fill" Value="{StaticResource PaneDocumentTabBackground_Disabled}" />
				<Setter TargetName="OuterBorder" Property="BorderBrush" Value="{StaticResource PaneDocumentTabOuterBorder_Disabled}" />
				<Setter TargetName="InnerBorder" Property="BorderBrush" Value="{StaticResource PaneDocumentTabInnerBorder_Disabled}" />
				<Setter TargetName="OuterBorder" Property="Background" Value="{StaticResource PaneDocumentTabBackground_Disabled}" />
			</Trigger>
            <Trigger Property="IsMouseOver" Value="True">
				<Setter TargetName="RightShapeOuterBorder" Property="Stroke" Value="{StaticResource PaneDocumentTabOuterBorder_Highlighted}" />
				<Setter TargetName="RightShapeInnerBorder" Property="Stroke" Value="{StaticResource PaneDocumentTabInnerBorder_Highlighted}" />
				<Setter TargetName="RightShapeBackground" Property="Fill" Value="{StaticResource PaneDocumentTabBackground_Highlighted}" />
				<Setter TargetName="OuterBorder" Property="BorderBrush" Value="{StaticResource PaneDocumentTabOuterBorder_Highlighted}" />
				<Setter TargetName="InnerBorder" Property="BorderBrush" Value="{StaticResource PaneDocumentTabInnerBorder_Highlighted}" />
				<Setter TargetName="OuterBorder" Property="Background" Value="{StaticResource PaneDocumentTabBackground_Highlighted}" />
            </Trigger>
			<Trigger Property="IsSelected" Value="False">
				<Setter TargetName="BottomBorderUnselected" Property="Visibility" Value="Visible" />
			</Trigger>
			<Trigger Property="IsSelected" Value="True">
				<Setter TargetName="BottomBorderSelected" Property="Visibility" Value="Visible" />
				<Setter TargetName="RightShapeOuterBorder" Property="Stroke" Value="{StaticResource PaneDocumentTabOuterBorder_Selected}" />
				<Setter TargetName="RightShapeInnerBorder" Property="Stroke" Value="{StaticResource PaneDocumentTabInnerBorder_Selected}" />
				<Setter TargetName="RightShapeBackground" Property="Fill" Value="{StaticResource PaneDocumentTabBackground_Selected}" />
				<Setter TargetName="OuterBorder" Property="BorderBrush" Value="{StaticResource PaneDocumentTabOuterBorder_Selected}" />
				<Setter TargetName="InnerBorder" Property="BorderBrush" Value="{StaticResource PaneDocumentTabInnerBorder_Selected}" />
				<Setter TargetName="OuterBorder" Property="Background" Value="{StaticResource PaneDocumentTabBackground_Selected}" />
			</Trigger>
        </ControlTemplate.Triggers>
        

    </ControlTemplate>

