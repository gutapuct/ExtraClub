								HorizontalAlignment="Left" VerticalAlignment="Center" 
								Style="{StaticResource Compass_IndicatorLeftStyle}"/>
							<ContentControl
								x:Name="PART_TopIndicator" Margin="4"
								HorizontalAlignment="Center" VerticalAlignment="Top"
								Style="{StaticResource Compass_IndicatorTopStyle}"/>
							<ContentControl
								x:Name="PART_RightIndicator" Margin="4"
								HorizontalAlignment="Right" VerticalAlignment="Center" 
								Style="{StaticResource Compass_IndicatorRightStyle}"/>
							<ContentControl
								x:Name="PART_BottomIndicator" Margin="4"
								HorizontalAlignment="Center" VerticalAlignment="Bottom"
								Style="{StaticResource Compass_IndicatorBottomStyle}"/>
							<ContentControl
								x:Name="PART_CenterIndicator"
								HorizontalAlignment="Center" VerticalAlignment="Center"
								Style="{StaticResource Compass_IndicatorCenterStyle}"/>
						</Grid>


					</Border>
					
					<ControlTemplate.Triggers>
						<Trigger Property="DockPosition" Value="Left">
							<Setter TargetName="PART_LeftIndicator" Property="Style"
									Value="{StaticResource Compass_IndicatorLeftStyle_Highlighted}" />
						</Trigger>
						<Trigger Property="DockPosition" Value="Top">
							<Setter TargetName="PART_TopIndicator" Property="Style"
									Value="{StaticResource Compass_IndicatorTopStyle_Highlighted}" />
						</Trigger>
						<Trigger Property="DockPosition" Value="Right">
							<Setter TargetName="PART_RightIndicator" Property="Style"
									Value="{StaticResource Compass_IndicatorRightStyle_Highlighted}" />
						</Trigger>
						<Trigger Property="DockPosition" Value="Bottom">
							<Setter TargetName="PART_BottomIndicator" Property="Style"
									Value="{StaticResource Compass_IndicatorBottomStyle_Highlighted}" />
						</Trigger>
						<Trigger Property="DockPosition" Value="Center">
							<Setter TargetName="PART_CenterIndicator" Property="Style"
									Value="{StaticResource Compass_IndicatorCenterStyle_Highlighted}" />
						</Trigger>

						<Trigger Property="IsLeftIndicatorVisible" Value="False">
							<Setter TargetName="PART_LeftIndicator" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="IsTopIndicatorVisible" Value="False">
							<Setter TargetName="PART_TopIndicator" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="IsRightIndicatorVisible" Value="False">
							<Setter TargetName="PART_RightIndicator" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="IsBottomIndicatorVisible" Value="False">
							<Setter TargetName="PART_BottomIndicator" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="IsCenterIndicatorVisible" Value="False">
							<Setter TargetName="PART_CenterIndicator" Property="Visibility" Value="Collapsed" />
							<Setter TargetName="PART_Center" Property="IsHitTestVisible" Value="False" />
						</Trigger>
					</ControlTemplate.Triggers>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

