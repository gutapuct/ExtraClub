    <ControlTemplate x:Key="Office_BlackStripLineControl3DTemplate" TargetType="{x:Type telerikCharting:StripLineControl3D}">
        <Grid>
            <Grid x:Name="PART_Container"></Grid>
            <Viewport3D x:Name="PART_Viewport">
                <Viewport3D.Children>
                    <ContainerUIElement3D x:Name="PART_ModelElement">
                        <ContainerUIElement3D.Children>
                            <telerikCharting:Box3D Width="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:StripLine3D}}, Path=Width}"
                                 Height="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:StripLine3D}}, Path=Height}"
                                 Depth="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:StripLine3D}}, Path=Depth}" >
                                <telerikCharting:Box3D.Material>
                                    <Binding RelativeSource="{RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:StripLine3D}}" 
                                             Converter="{StaticResource stripline3DMaterialConverter}"
                                             ConverterParameter="{telerikControls:ThemeResourceKey ThemeType=external:SummerThemeExternal, ElementType={x:Type telerikCharting:ChartArea}, ResourceId={x:Static telerikCharting:ResourceHelper.ResourceKey3DStripLinesStyle}}"/>
                                </telerikCharting:Box3D.Material>
                            </telerikCharting:Box3D>
                        </ContainerUIElement3D.Children>
                    </ContainerUIElement3D>
                </Viewport3D.Children>
