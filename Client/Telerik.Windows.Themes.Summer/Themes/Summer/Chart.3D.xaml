                                Points="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:PlaneYZ}}, Path=PrimaryAxis3DPoints}"
								Visibility="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:PlaneYZ}}, Path=Visibility}"
								/>
                            <telerikCharting:LabelsHitTestVisual 
                                Radius="1"
                                LabelData="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:AxisY3D}}, Path=AxisTitleLabel}"
								Transform="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:AxisY3D}}, Path=AxisTitleLabel.Position, Converter={StaticResource translateTransform3DConverter}}" />
                            <telerikCharting:ItemsPresenter3D x:Name="PART_ItemsPresenter3D" />
