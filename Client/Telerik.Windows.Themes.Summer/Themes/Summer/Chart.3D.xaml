    <ControlTemplate x:Key="Office_BlackAxisItemControl3DTemplate" TargetType="{x:Type telerikCharting:AxisItemControl3D}">
        <Grid>
            <Grid x:Name="PART_Container"></Grid>
            <Viewport3D x:Name="PART_Viewport">
                <Viewport3D.Children>
                    <ContainerUIElement3D x:Name="PART_ModelElement">
