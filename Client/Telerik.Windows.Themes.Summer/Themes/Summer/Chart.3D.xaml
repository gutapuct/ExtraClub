<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mscorlib="clr-namespace:System;assembly=mscorlib"
	
    xmlns:system="clr-namespace:System;assembly=mscorlib"
    xmlns:telerikControls="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls,Version=2011.2.0712.35,Culture=neutral,PublicKeyToken=5803cfa389c90ce7"
    xmlns:telerikData="clr-namespace:Telerik.Windows.Data;assembly=Telerik.Windows.Data,Version=2011.2.0712.35,Culture=neutral,PublicKeyToken=5803cfa389c90ce7"
    xmlns:telerikEffects="clr-namespace:Telerik.Windows.Controls.TransitionEffects;assembly=Telerik.Windows.Controls,Version=2011.2.0712.35,Culture=neutral,PublicKeyToken=5803cfa389c90ce7"

    xmlns:vsm="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:telerikPrimitives="clr-namespace:Telerik.Windows.Controls.Primitives;assembly=Telerik.Windows.Controls,Version=2011.2.0712.35,Culture=neutral,PublicKeyToken=5803cfa389c90ce7"
    xmlns:telerikChart="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.Charting,Version=2011.2.0712.35,Culture=neutral,PublicKeyToken=5803cfa389c90ce7"
    xmlns:telerikCharting="clr-namespace:Telerik.Windows.Controls.Charting;assembly=Telerik.Windows.Controls.Charting,Version=2011.2.0712.35,Culture=neutral,PublicKeyToken=5803cfa389c90ce7"
    xmlns:external="clr-namespace:Telerik.Windows.Controls.External"


 >

    <SolidColorBrush x:Key="StripLinesDiffuseMaterialBrush1" Color="#FFFFFFFF" />
<SolidColorBrush x:Key="StripLinesSpecularMaterialBrush1" Color="#FFFFFFFF"/>
<SolidColorBrush x:Key="StripLinesDiffuseMaterialBrush2" Color="#FFEEEEEE" />
<SolidColorBrush x:Key="StripLinesSpecularMaterialBrush2" Color="#FFFFFFFF"/>
<SolidColorBrush x:Key="StripLinesDiffuseMaterialBrush3" Color="#FFD4D4D4" />
<SolidColorBrush x:Key="StripLinesSpecularMaterialBrush3" Color="#FFFFFFFF"/>
<SolidColorBrush x:Key="PlaneMaterialBrush" Color="AliceBlue"/>
<Color x:Key="AxisColor">#FF696969</Color>
<system:Double x:Key="AxisThickness">1.5</system:Double>
<telerikCharting:TranslateTransform3DConverter x:Key="translateTransform3DConverter" />
<Color x:Key="GridLineColor">#FF696969</Color>
<telerikCharting:StripLine3DMaterialConverter x:Key="stripline3DMaterialConverter" />
<telerikCharting:Thickness3D x:Key="RoundedBarRadiusThickness"  
                       Front="0" 
                       Back="0" 
                       Top="0" 
                       Bottom="0" 
                       Left="0" 
                       Right="0" />
<telerikCharting:SeriesMaterialConverter x:Key="SeriesMaterialConverter" />
<telerikCharting:DelayConverter x:Key="delayConverter"/>
<telerikCharting:DurationConverter x:Key="durationConverter"/>
<telerikCharting:EnableAnimationsConverter x:Key="enableAnimationsConverter" />
<telerikControls:BooleanToVisibilityConverter x:Key="boolToVisibility"/>
<telerikCharting:BubbleSeriesMaterialConverter x:Key="bubbleSeriesMaterialConverter" />
<ItemsPanelTemplate x:Key="DummyPanel">
    <Canvas />
</ItemsPanelTemplate>

    

    

    <telerikCharting:MaterialsCollection x:Key="{telerikControls:ThemeResourceKey ThemeType=external:SummerThemeExternal, ElementType={x:Type telerikCharting:ChartArea}, ResourceId={x:Static telerikCharting:ResourceHelper.ResourceKey3DStripLinesStyle}}" >
        <MaterialGroup>
            <MaterialGroup.Children>
                <DiffuseMaterial Brush="{StaticResource StripLinesDiffuseMaterialBrush1}" />
                <SpecularMaterial Brush="{StaticResource StripLinesSpecularMaterialBrush1}" />
                <SpecularMaterial SpecularPower="0" />
            </MaterialGroup.Children>
        </MaterialGroup>

        <MaterialGroup>
            <MaterialGroup.Children>
                <DiffuseMaterial Brush="{StaticResource StripLinesDiffuseMaterialBrush2}" />
                <SpecularMaterial Brush="{StaticResource StripLinesSpecularMaterialBrush2}" />
                <SpecularMaterial SpecularPower="0" />
            </MaterialGroup.Children>
        </MaterialGroup>

        <MaterialGroup>
            <MaterialGroup.Children>
                <DiffuseMaterial Brush="{StaticResource StripLinesDiffuseMaterialBrush3}" />
                <SpecularMaterial Brush="{StaticResource StripLinesSpecularMaterialBrush3}" />
                <SpecularMaterial SpecularPower="0" />
            </MaterialGroup.Children>
        </MaterialGroup>
    </telerikCharting:MaterialsCollection>

    <MaterialGroup x:Key="PlaneMaterial">
        <MaterialGroup.Children>
            <DiffuseMaterial Brush="{StaticResource PlaneMaterialBrush}" />
        </MaterialGroup.Children>
    </MaterialGroup>

    <Transform3DGroup x:Key="ChartAreaCenterTransform">
        <TranslateTransform3D OffsetX="-50" OffsetY="-37.5" />
    </Transform3DGroup>

    <Transform3DGroup x:Key="PieTransform">
        <RotateTransform3D>
            <RotateTransform3D.Rotation>
                <AxisAngleRotation3D Axis="1,0,0" Angle="-75" />
            </RotateTransform3D.Rotation>
        </RotateTransform3D>
        <MatrixTransform3D Matrix="2 0 0 0 0 2 0 0 0 0 2 0 50 37.5 0 1" />
    </Transform3DGroup>

    <Transform3DGroup x:Key="FunnelTransform">
        <TranslateTransform3D OffsetX="50" OffsetY="72.5" />
    </Transform3DGroup>

    <Transform3DGroup x:Key="PyramidTransform">
        <RotateTransform3D>
            <RotateTransform3D.Rotation>
                <AxisAngleRotation3D Axis="0,1,0" Angle="60" />
            </RotateTransform3D.Rotation>
        </RotateTransform3D>
        <TranslateTransform3D OffsetX="50" OffsetY="17.5" />
    </Transform3DGroup>


    <ControlTemplate x:Key="Office_BlackAxisXControl3DTemplate" TargetType="{x:Type telerikCharting:AxisXControl3D}">
        <Grid>
            <ItemsPresenter />
            <Grid x:Name="PART_Container"></Grid>
            <Viewport3D x:Name="PART_Viewport">
                <Viewport3D.Children>
                    <ContainerUIElement3D x:Name="PART_ModelElement" 
                        Visibility="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:ChartArea}}, Path=AxisX.Visibility}">
                        <ContainerUIElement3D.Children>
                            <telerikCharting:Polyline3D 
                                Color="{StaticResource AxisColor}" 
                                Thickness="{StaticResource AxisThickness}" 
                                Points="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:PlaneZX}}, Path=SecondaryAxis3DPoints}"
							    Visibility="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:PlaneZX}}, Path=Visibility}"
                                />
                            <telerikCharting:LabelsHitTestVisual 
                                Radius="1"
                                LabelData="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:AxisX3D}}, Path=AxisTitleLabel}"
								Transform="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:AxisX3D}}, Path=AxisTitleLabel.Position, Converter={StaticResource translateTransform3DConverter}}" />
                            <telerikCharting:ItemsPresenter3D 
                                x:Name="PART_ItemsPresenter3D"
                                />
                        </ContainerUIElement3D.Children>
                    </ContainerUIElement3D>
                </Viewport3D.Children>
            </Viewport3D>
        </Grid>
    </ControlTemplate>

