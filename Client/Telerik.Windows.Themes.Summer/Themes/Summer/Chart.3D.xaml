    <ControlTemplate x:Key="Office_BlackBubbleControl3DTemplate" TargetType="{x:Type telerikCharting:BubbleControl3D}">
        <Grid>
            <Grid x:Name="PART_Container"></Grid>
            <Viewport3D x:Name="PART_Viewport">
                <Viewport3D.Children>
                    <ContainerUIElement3D x:Name="PART_ModelElement">
                        <ContainerUIElement3D.Transform>
                            <Transform3DGroup>
                                <ScaleTransform3D x:Name="scaleTransform">
                                    <ScaleTransform3D.ScaleX>
