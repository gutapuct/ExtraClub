                                        <Binding RelativeSource="{RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:Bubble3D}}"
                                                 Path="DataPoint.BubbleSize" />
                                    </MultiBinding>
                                </telerikCharting:Sphere.Material>
                            </telerikCharting:Sphere>
                        </ContainerUIElement3D.Children>
                    </ContainerUIElement3D>
                </Viewport3D.Children>
                <Viewport3D.Triggers>
                    <EventTrigger RoutedEvent="Window.Loaded">
                        <BeginStoryboard>
