                            <telerikCharting:Polyline3D 
								Color="{StaticResource AxisColor}" 
                                Thickness="{StaticResource AxisThickness}" 
								Points="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:AxisItem3D}}, Path=TickPoints3D}" 
								Visibility="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:AxisItem3D}}, Path=Visibility}"
								/>
                            <telerikCharting:LabelsHitTestVisual 
                                Radius="1"
                                LabelData="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:AxisItem3D}}, Path=LabelData}" >
                                <telerikCharting:LabelsHitTestVisual.Transform>
                                    <TranslateTransform3D 
                                        OffsetX="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:AxisItem3D}}, Path=LabelData.Position.X, Mode=OneTime}"
                                        OffsetY="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:AxisItem3D}}, Path=LabelData.Position.Y, Mode=OneTime}"
                                        OffsetZ="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type telerikCharting:AxisItem3D}}, Path=LabelData.Position.Z, Mode=OneTime}"
                                        />
                                </telerikCharting:LabelsHitTestVisual.Transform>
                            </telerikCharting:LabelsHitTestVisual>
                        </ContainerUIElement3D.Children>
                    </ContainerUIElement3D>
                </Viewport3D.Children>
            </Viewport3D>
        </Grid>
    </ControlTemplate>

