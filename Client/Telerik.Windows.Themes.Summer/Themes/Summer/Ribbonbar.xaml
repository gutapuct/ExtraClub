            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonBackstageItem">
                    <StackPanel>
                        <ContentPresenter Content="{TemplateBinding Header}" ContentTemplate="{TemplateBinding HeaderTemplate}" Margin="20 5 0 5" VerticalAlignment="Center" />
                        <Rectangle Height="1" Fill="{StaticResource RibbonnBackStage_DottedLineBrush}" Margin="18 0 10 4" />
                    </StackPanel> 
                </ControlTemplate>
            </Setter.Value>
        </Setter>
