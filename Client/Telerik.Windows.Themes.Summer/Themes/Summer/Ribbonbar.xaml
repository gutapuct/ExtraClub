									Background="{StaticResource ApplicationRibbonButtonOverBackground}"/>
						</Border>

						<Grid Margin="1">
							<Grid.RowDefinitions>
								<RowDefinition Height="*" />
								<RowDefinition Height="Auto" MaxHeight="28"/>
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="Auto" />
							</Grid.ColumnDefinitions>

							<telerik:RadButton
								x:Name="ButtonPart"
								Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2"
								BorderThickness="0"
								Background="Transparent"
								Content="{TemplateBinding Content}"
								ContentTemplate="{TemplateBinding ContentTemplate}"
								HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
								VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
								Template="{StaticResource AppMenuRadSplitButton_ButtonStyle}"
								Padding="{TemplateBinding Padding}" />
							<telerik:RadToggleButton
								x:Name="DropDownPart"
								Grid.Row="0" Grid.Column="2"
								BorderThickness="0" Margin="0"
								Background="Transparent"
								Visibility="{TemplateBinding DropDownIndicatorVisibility}"
								Template="{StaticResource AppMenuRadSplitButton_DropDownStyle}"
								IsTabStop="false"
								
								
								IsChecked="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
								 />
								<Rectangle
								x:Name="SeparatorPart"
								Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2"
								MinWidth="1" MinHeight="1"
								Visibility="Collapsed" IsHitTestVisible="False"
								HorizontalAlignment="Right" VerticalAlignment="Stretch"
								Fill="{StaticResource RadRibbonBarDropDownButtonPressedBorder}" />

								<Image Name="Image" IsHitTestVisible="False" Margin="4"/>

								<TextBlock Name="textRow1" Grid.Row="0" Grid.Column="1" Margin="0,0,2,0" IsHitTestVisible="False"
									Text="{TemplateBinding Text}" TextAlignment="Left"  VerticalAlignment="Center" />

								<Grid Name="DropDownPanel" Grid.Row="0" Grid.Column="2" Margin="0"
									Width="22" IsHitTestVisible="False">
									<Path Name="DropDownIndicator" VerticalAlignment="Center" HorizontalAlignment="Center"
										Data="M 0,0 L 3.5,3.5 L 0,7" Margin="0" IsHitTestVisible="False"
										Visibility="{TemplateBinding DropDownIndicatorVisibility}"
										Fill="{StaticResource RibbonButtonDropDownArrow}"
										Stroke="{StaticResource RibbonButtonDropDownArrowBorder}" />
								</Grid>
						</Grid>
						
						<Popup x:Name="DropDownPopup" 
                            HorizontalOffset="2" VerticalOffset="-1"
							AllowsTransparency="True" StaysOpen="False" Focusable="false" Placement="Bottom"
							IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
						
						
							<!--PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}">-->
							<Grid>
								<!--<telerikRibbonBarPrimitives:DropShadow telerik:StyleManager.Theme="{StaticResource Theme}" Margin="6,6,4,4" />-->
								<Border x:Name="DropDownPopupBorder"
										BorderBrush="{StaticResource DropDownPopupBorderBrush}"
										Background="{StaticResource DropDownPopupBackgroundBrush}"
										BorderThickness="1"
										MinWidth="3" Width="{TemplateBinding DropDownWidth}"
										MinHeight="3" Height="{TemplateBinding DropDownHeight}">
									<ContentControl
										x:Name="DropDownPopupContent"
										Content="{TemplateBinding DropDownContent}"
										HorizontalAlignment="Stretch"
										VerticalAlignment="Stretch" />
								</Border>
							</Grid>
						
						</Popup>
						
						

						<Rectangle x:Name="disabledDecorator" Visibility="Collapsed" Opacity="0" Fill="#FFFFFFFF" />
					</Grid>

					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="true">
							<Setter TargetName="mouseOverDecorator" Property="Opacity" Value="1" />
							<Setter TargetName="SeparatorPart" Property="Visibility" Value="Visible" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
							<Setter TargetName="disabledDecorator" Property="Opacity" Value="0.55" />
							<Setter TargetName="disabledDecorator" Property="Visibility" Value="Visible"/>
						</Trigger>
						<Trigger Property="IsFocused" Value="True" />
						<Trigger Property="IsOpen" Value="True">
							<Setter TargetName="mouseOverDecorator" Property="Opacity" Value="1" />
							<Setter TargetName="SeparatorPart" Property="Visibility" Value="Visible" />
						</Trigger>
						<Trigger Property="DropDownButtonPosition" Value="Top" />
						<Trigger Property="DropDownButtonPosition" Value="Left" />
						<Trigger Property="DropDownButtonPosition" Value="Bottom" />
						<Trigger Property="DropDownButtonPosition" Value="Right" />
						<Trigger Property="CurrentSize" Value="Large" />
						<Trigger Property="CurrentSize" Value="Medium" />
						<Trigger Property="CurrentSize" Value="Small" />
						<Trigger Property="DropDownPlacement" Value="Top" />
						<Trigger Property="DropDownPlacement" Value="Left" />
						<Trigger Property="DropDownPlacement" Value="Right" />
						<Trigger Property="DropDownPlacement" Value="Bottom" />
						<Trigger Property="DropDownPlacement" Value="Center" />
						<Trigger Property="DropDownPlacement" Value="Relative">
							<Setter TargetName="DropDownPopup" Property="Placement" Value="Relative" />
						</Trigger>
					</ControlTemplate.Triggers>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="telerikRibbonBar:Separator" x:Key="ApplicationSeparatorStyle">
		<Setter Property="Background" Value="#f5f5f5" />
		<Setter Property="BorderThickness" Value="0,1,0,0" />
		<Setter Property="BorderBrush" Value="#c5c5c5" />
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Height" Value="2" />
		<Setter Property="Margin" Value="44,0,0,0" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBar:Separator">
					<Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" />
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style TargetType="telerikRibbonBar:ApplicationMenu"
           x:Key="{telerik:ThemeResourceKey ThemeType=
		external:SummerThemeExternal , ElementType=telerikRibbonBar:ApplicationMenu}"
		 >
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Background" Value="{StaticResource RibbonPopupBackgroundBrush}" />
		<Setter Property="BorderBrush" Value="{StaticResource ApplicationMenuOuterBorderBrush}" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="Foreground" Value="{StaticResource RibbonPopupForegroundBrush}" />
        <Setter Property="ItemContainerStyleSelector">
            <Setter.Value>
                <telerikRibbonBar:CustomContainerStyleSelector>
                    <telerikRibbonBar:CustomContainerStyle TypeName="RadRibbonButton"
                                                           ContainerStyle="{StaticResource ApplicationRibbonButton}" />
                    <telerikRibbonBar:CustomContainerStyle TypeName="RadRibbonDropDownButton"
                                                           ContainerStyle="{StaticResource ApplicationRibbonDropDownButton}" />
                    <telerikRibbonBar:CustomContainerStyle TypeName="RadRibbonSplitButton"
                                                           ContainerStyle="{StaticResource ApplicationRibbonSplitButton}" />
                    <telerikRibbonBar:CustomContainerStyle TypeName="Separator"
                                                           ContainerStyle="{StaticResource ApplicationSeparatorStyle}" />
                </telerikRibbonBar:CustomContainerStyleSelector>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBar:ApplicationMenu">
                    <Grid x:Name="rootGrid" SnapsToDevicePixels="True"
                         >
						
                        
                        <Grid.OpacityMask>                           
                            <RadialGradientBrush MappingMode="Absolute" Center="24,-2" RadiusX="19" RadiusY="18" GradientOrigin="24,-2">
                                <GradientStop Offset="0.99" Color="#00000000" />
                                <GradientStop Offset="1" Color="#FF000000" />
                            </RadialGradientBrush>                  
                        </Grid.OpacityMask>
                        
                        <Border Name="PART_OuterBorder" CornerRadius="1" BorderThickness="1" BorderBrush="{TemplateBinding BorderBrush}" Background="Transparent">
							<Grid>
								<Grid.RowDefinitions>
									<RowDefinition Height="Auto" />
									<RowDefinition Height="*" />
									<RowDefinition Height="Auto" />
								</Grid.RowDefinitions>
								<Rectangle x:Name="clippingRectangle" Grid.Row="0" Height="21"  Fill="{StaticResource ApplicationMenuTopBorderBrush}" />
                                
                                <Rectangle x:Name="buttonRectangle" Grid.Row="0" Height="21" Width="44" HorizontalAlignment="Left"  Fill="Transparent" />
                                
                                <Border Grid.Row="1" Background="{StaticResource ApplicationMenuMiddleBorderBrush}">
									<Border Margin="2,0,2,0" BorderThickness="1" BorderBrush="{StaticResource ApplicationMenuContentOuterBorderBrush}">
										<Border BorderThickness="1" BorderBrush="{StaticResource ApplicationMenuContentInnerBorderBrush}">
											<Grid>
												<Grid.ColumnDefinitions>
													<ColumnDefinition></ColumnDefinition>
													<ColumnDefinition Width="Auto"></ColumnDefinition>
												</Grid.ColumnDefinitions>
												<Border Name="LeftPane" BorderBrush="#c5c5c5" Background="{TemplateBinding Background}" BorderThickness="0,0,1,0">
													<ItemsPresenter />
												</Border>
												<Border Name="RightPane" Visibility="{TemplateBinding RightPaneVisibility}" Grid.Column="1" BorderBrush="{StaticResource ApplicationMenuDividerLightBrush}" Background="{StaticResource ApplicationMenuContentBackgroundBrush}" BorderThickness="1,0,0,0">
													<ContentPresenter Content="{TemplateBinding Content}" Name="ContentPresenter" MinWidth="100" />
												</Border>
											</Grid>
										</Border>
									</Border>
								</Border>
								<Border Grid.Row="2" Background="{StaticResource ApplicationMenuFooterBorderBrush}">
									<ContentPresenter Content="{TemplateBinding FooterContent}" HorizontalAlignment="Right" VerticalAlignment="Center" />
								</Border>
							</Grid>
						</Border>
					</Grid>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="ApplicationButtonType" Value="Office2010">
                            <Setter TargetName="clippingRectangle" Property="Margin" Value="45 0 0 0" />
                            <Setter TargetName="rootGrid" Property="OpacityMask" Value="{x:Null}" />
                        </Trigger>
                        <Trigger Property="ApplicationButtonType" Value="Office2007">
                            <Setter TargetName="clippingRectangle" Property="Height" Value="14" />
                            <Setter TargetName="buttonRectangle" Property="Height" Value="14" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
	

	<Style TargetType="telerikRibbonBarPrimitives:DropShadow"
           x:Key="{telerik:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikRibbonBarPrimitives:DropShadow}"
        >
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="HorizontalContentAlignment" Value="Stretch" />
		<Setter Property="VerticalContentAlignment" Value="Stretch" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBarPrimitives:DropShadow">
					<Grid>
						<!-- Shadow -->
						<Rectangle Name="Left" HorizontalAlignment="Left" VerticalAlignment="Stretch" IsHitTestVisible="False" />
						<Rectangle Name="TopLeft" HorizontalAlignment="Left" VerticalAlignment="Top"  IsHitTestVisible="False" />
						<Rectangle Name="Top" HorizontalAlignment="Stretch" VerticalAlignment="Top" IsHitTestVisible="False" />
						<Rectangle Name="TopRight" HorizontalAlignment="Right" VerticalAlignment="Top" IsHitTestVisible="False" />
						<Rectangle Name="Right" HorizontalAlignment="Right" VerticalAlignment="Stretch" IsHitTestVisible="False" />
						<Rectangle Name="BottomRight" HorizontalAlignment="Right" VerticalAlignment="Bottom" IsHitTestVisible="False" />
						<Rectangle Name="Bottom" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" IsHitTestVisible="False" />
						<Rectangle Name="BottomLeft" HorizontalAlignment="Left" VerticalAlignment="Bottom" IsHitTestVisible="False" />

						<ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
	

	<Style TargetType="telerikRibbonBarPrimitives:GroupChrome"
        x:Key="{telerik:ThemeResourceKey ThemeType=
		external:SummerThemeExternal , ElementType=telerikRibbonBarPrimitives:GroupChrome}"
		 >
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Foreground" Value="{StaticResource RibbonForegroundBrush}" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBarPrimitives:GroupChrome">
					<Grid Background="Transparent" SnapsToDevicePixels="True"   >
						
						<Border Name="InnerBorder" Margin="1,1,0,0" BorderThickness="1" CornerRadius="1">
							<Border.BorderBrush>
								<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
									<GradientStop x:Name="InnerBorder1" Offset="0" Color="{StaticResource RibbonGroupNormalInnerBorderColor1}" />
									<GradientStop x:Name="InnerBorder2" Offset="1" Color="{StaticResource RibbonGroupNormalInnerBorderColor2}" />
								</LinearGradientBrush>
							</Border.BorderBrush>
						</Border>
						<Border Name="OuterBorder" Margin="0,0,1,1" BorderThickness="1" CornerRadius="2">
							<Border.Background>
								<LinearGradientBrush x:Name="BackgroundBrush" StartPoint="0,0" EndPoint="0,1" Opacity="0">
									<GradientStop Offset="0" Color="{StaticResource RibbonGroupBackground1}" />
									<GradientStop Offset="0.17" Color="{StaticResource RibbonGroupBackground2}" />
									<GradientStop Offset="0.17" Color="{StaticResource RibbonGroupBackground3}" />
									<GradientStop Offset="1" Color="{StaticResource RibbonGroupBackground4}" />
								</LinearGradientBrush>
							</Border.Background>
							<Border.BorderBrush>
								<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
									<GradientStop x:Name="OuterBorder1" Offset="0" Color="{StaticResource RibbonGroupNormalOuterBorderColor1}" />
									<GradientStop x:Name="OuterBorder2" Offset="1" Color="{StaticResource RibbonGroupNormalOuterBorderColor2}" />
								</LinearGradientBrush>
							</Border.BorderBrush>
						</Border>
						<Border Name="Tray" Height="15" VerticalAlignment="Bottom" Margin="1,0,2,2">
							<Border.Background>
								<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
									<GradientStop x:Name="Tray1" Offset="0.5" Color="{StaticResource RibbonGroupNormalTrayBackgroundColor1}" />
									<GradientStop x:Name="Tray2" Offset="1" Color="{StaticResource RibbonGroupNormalTrayBackgroundColor2}" />
								</LinearGradientBrush>
							</Border.Background>
							<Grid>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="*" />
									<ColumnDefinition Width="Auto" />
								</Grid.ColumnDefinitions>
								<ContentControl Content="{TemplateBinding Header}" ContentTemplate="{TemplateBinding HeaderTemplate}" Margin="3,0,3,0" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{StaticResource RibbonGroupHeaderBrush}" IsTabStop="False"/>

								<telerikRibbonBar:RadRibbonButton telerik:StyleManager.Theme="{StaticResource Theme}" Visibility="{TemplateBinding DialogLauncherVisibility}" Name="DialogLauncher" Grid.Column="1" IsAutoSize="False" VerticalAlignment="Top" Padding="4,3,3,3" TabIndex="1"
                                                                  IsTabStop="False">
									<telerikRibbonBar:RadRibbonButton.Content>
										<Rectangle Fill="{StaticResource RibbonGroupDialogLauncher}" Width="8" Height="8" />
									</telerikRibbonBar:RadRibbonButton.Content>
								</telerikRibbonBar:RadRibbonButton>

							</Grid>
						</Border>
						<ContentPresenter Margin="4,2,4,18" />
					</Grid>
					
					<ControlTemplate.Triggers>
						<Trigger Property="IsCollapsed" Value="True">
							<Setter TargetName="Tray" Property="Visibility" Value="Collapsed" />
						</Trigger>
						
						<Trigger Property="IsMouseOver" Value="True" >
							<Setter  Property="BorderBrush" TargetName="InnerBorder">
								<Setter.Value>
									<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
										<GradientStop  Offset="0" Color="{StaticResource RibbonGroupHotInnerBorderColor1}" />
										<GradientStop  Offset="1" Color="{StaticResource RibbonGroupHotInnerBorderColor2}" />
									</LinearGradientBrush>
								</Setter.Value>
							</Setter>
							<Setter  Property="BorderBrush" TargetName="OuterBorder">
								<Setter.Value>
									<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
										<GradientStop Offset="0" Color="{StaticResource RibbonGroupHotOuterBorderColor1}" />
										<GradientStop Offset="1" Color="{StaticResource RibbonGroupHotOuterBorderColor2}" />
									</LinearGradientBrush>
								</Setter.Value>
							</Setter>
							<Setter  Property="Background" TargetName="OuterBorder">
								<Setter.Value>
									<LinearGradientBrush StartPoint="0,0" EndPoint="0,1" Opacity="1">
										<GradientStop Offset="0" Color="{StaticResource RibbonGroupBackground1}" />
										<GradientStop Offset="0.17" Color="{StaticResource RibbonGroupBackground2}" />
										<GradientStop Offset="0.17" Color="{StaticResource RibbonGroupBackground3}" />
										<GradientStop Offset="1" Color="{StaticResource RibbonGroupBackground4}" />
									</LinearGradientBrush>
								</Setter.Value>
							</Setter>
							<Setter  Property="Background" TargetName="Tray">
								<Setter.Value>
									<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
										<GradientStop Offset="0.5" Color="{StaticResource RibbonGroupHotTrayBackgroundColor1}" />
										<GradientStop Offset="1" Color="{StaticResource RibbonGroupHotTrayBackgroundColor2}" />
									</LinearGradientBrush>
								</Setter.Value>
							</Setter>

							<Setter TargetName="OuterBorder" Property="Margin" Value="0,0,0,1" />
							<Setter TargetName="InnerBorder" Property="Margin"  Value="1,1,1,2" />
							<Setter TargetName="InnerBorder" Property="Panel.ZIndex" Value="1" />
						</Trigger>
						
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="IsMouseOver" Value="False" />
								<Condition Property="IsPushed" Value="True" />
								<Condition Property="IsCollapsed" Value="True" />
							</MultiTrigger.Conditions>
						
							<Setter  Property="BorderBrush" TargetName="InnerBorder">
								<Setter.Value>
									<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
										<GradientStop  Offset="0" Color="{StaticResource RibbonGroupPushedInnerBorderColor1}" />
										<GradientStop  Offset="1" Color="{StaticResource RibbonGroupPushedInnerBorderColor2}" />
									</LinearGradientBrush>
								</Setter.Value>
							</Setter>
							<Setter  Property="BorderBrush" TargetName="OuterBorder">
								<Setter.Value>
									<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
										<GradientStop  Offset="0" Color="{StaticResource RibbonGroupPushedOuterBorderColor1}" />
										<GradientStop  Offset="1" Color="{StaticResource RibbonGroupPushedOuterBorderColor2}" />
									</LinearGradientBrush>
								</Setter.Value>
							</Setter>
							<Setter  Property="Background" TargetName="OuterBorder">
								<Setter.Value>
									<LinearGradientBrush StartPoint="0,0" EndPoint="0,1" Opacity="1">
										<GradientStop Offset="0" Color="{StaticResource RibbonGroupBackground1}" />
										<GradientStop Offset="0.17" Color="{StaticResource RibbonGroupBackground2}" />
										<GradientStop Offset="0.17" Color="{StaticResource RibbonGroupBackground3}" />
										<GradientStop Offset="1" Color="{StaticResource RibbonGroupBackground4}" />
									</LinearGradientBrush>
								</Setter.Value>
							</Setter>
							<Setter TargetName="OuterBorder" Property="Background"
									Value="{StaticResource RibbonGroupPushedBackgroundBrush}" />
						</MultiTrigger>

						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="IsMouseOver" Value="False" />
								<Condition Property="IsPushed" Value="False" />
								<Condition Property="IsCollapsed" Value="False" />
 							</MultiTrigger.Conditions>
							<Setter TargetName="Tray" Property="Visibility" Value="Visible" />
							<Setter  Property="BorderBrush" TargetName="InnerBorder">
								<Setter.Value>
									<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
										<GradientStop  Offset="0" Color="{StaticResource RibbonGroupNormalInnerBorderColor1}" />
										<GradientStop  Offset="1" Color="{StaticResource RibbonGroupNormalInnerBorderColor2}" />
									</LinearGradientBrush>
								</Setter.Value>
							</Setter>
							<Setter  Property="BorderBrush" TargetName="OuterBorder">
								<Setter.Value>
									<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
										<GradientStop  Offset="0" Color="{StaticResource RibbonGroupNormalOuterBorderColor1}" />
										<GradientStop  Offset="1" Color="{StaticResource RibbonGroupNormalOuterBorderColor2}" />
									</LinearGradientBrush>
								</Setter.Value>
							</Setter>
							<Setter TargetName="Tray" Property="Background">
								<Setter.Value>
									<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
										<GradientStop  Offset="0" Color="{StaticResource RibbonGroupNormalTrayBackgroundColor1}" />
										<GradientStop  Offset="1" Color="{StaticResource RibbonGroupNormalTrayBackgroundColor2}" />
									</LinearGradientBrush>
								</Setter.Value>
							</Setter>
							<Setter TargetName="OuterBorder" Property="Opacity" Value="0.5" />
						</MultiTrigger>

                    </ControlTemplate.Triggers>
					

				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
	

	<Style TargetType="telerikRibbonBar:RadButtonGroup"
           x:Key="{telerik:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikRibbonBar:RadButtonGroup}"
        >
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Background" Value="{StaticResource ButtonGroupBackgroundBrush}" />
