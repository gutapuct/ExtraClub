	<Style x:Key="ApplicationRibbonButton" TargetType="telerikRibbonBar:RadRibbonButton">
		<Setter Property="Padding" Value="5" />
		<Setter Property="BorderThickness" Value="1.5" />
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="HorizontalAlignment" Value="Stretch" />
		<Setter Property="HorizontalContentAlignment" Value="Left" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Size" Value="Large" />
		<!--<Setter Property="SplitText" Value="False" />-->
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBar:RadRibbonButton">
					<Grid>
						
						<Border x:Name="buttonSurface"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}"
								Background="{TemplateBinding Background}"
								CornerRadius="1">
							<Border x:Name="buttonSurfaceInner"
									BorderBrush="Transparent"
									BorderThickness="1"/>
						</Border>

						<Grid Margin="{TemplateBinding Padding}">
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
							</Grid.ColumnDefinitions>

							<ContentPresenter Content="" Grid.ColumnSpan="2" />

							<Image Name="Image" />

							<TextBlock
								Name="textRow1" Grid.Column="1" Margin="4,0,1,0"
								Text="{TemplateBinding Text}"
								TextAlignment="Left" VerticalAlignment="Center" />
						</Grid>

						<Rectangle x:Name="DisabledIndicator" Visibility="Collapsed"
								Fill="#99FFFFFF" />
					</Grid>

					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="true">
							<Setter Property="BorderBrush" TargetName="buttonSurface"
									Value="{StaticResource ApplicationRibbonButtonOverBorder}" />
							<Setter Property="Background" TargetName="buttonSurface"
									Value="{StaticResource ApplicationRibbonButtonOverBackground}" />
							<Setter Property="BorderBrush" TargetName="buttonSurfaceInner"
									Value="{StaticResource ApplicationRibbonButtonOverBorderInner}" />
						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter Property="BorderBrush" TargetName="buttonSurface"
									Value="{StaticResource ApplicationRibbonButtonOverBorder}" />
							<Setter Property="Background" TargetName="buttonSurface"
									Value="{StaticResource ApplicationRibbonButtonOverBackground}" />
							<Setter Property="BorderBrush" TargetName="buttonSurfaceInner"
									Value="{StaticResource ApplicationRibbonButtonOverBorderInner}" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
							<Setter TargetName="DisabledIndicator" Property="Visibility" Value="Visible" />
						</Trigger>
						<!--<Trigger Property="TextAlignmentState" Value="TextHide">
							<Setter TargetName="textRow1" Property="Visibility" Value="Collapsed" />
						</Trigger>-->
					</ControlTemplate.Triggers>
					

				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style x:Key="ApplicationRibbonDropDownButton" TargetType="telerikRibbonBar:RadRibbonDropDownButton">
		<Setter Property="Padding" Value="5" />
		<Setter Property="BorderThickness" Value="1.5" />
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="HorizontalAlignment" Value="Stretch" />
		<Setter Property="HorizontalContentAlignment" Value="Left" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Size" Value="Large" />
		<!--<Setter Property="SplitText" Value="False" />-->
		<Setter Property="AutoOpenDelay" Value="0:0:0.3" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBar:RadRibbonDropDownButton">
                    <Grid SnapsToDevicePixels="True"  >
						
						<Border x:Name="buttonSurface"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}"
								Background="{TemplateBinding Background}"
								CornerRadius="1">
							<Border x:Name="buttonSurfaceInner"
									BorderBrush="Transparent"
									BorderThickness="{TemplateBinding BorderThickness}"/>
						</Border>

						<Border x:Name="isOpenIndicator" Visibility="Collapsed"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{StaticResource ApplicationRibbonButtonOverBorder}"
								Background="{StaticResource ApplicationRibbonButtonOverBackground}"
								CornerRadius="1">
							<Border BorderBrush="{StaticResource ApplicationRibbonButtonOverBorderInner}"
									BorderThickness="{TemplateBinding BorderThickness}"/>
						</Border>

						<Border x:Name="mouseOverIndicator" CornerRadius="1"
								BorderThickness="{TemplateBinding BorderThickness}" />

						<Grid Margin="{TemplateBinding Padding}">
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="Auto" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="22" />
							</Grid.ColumnDefinitions>

							<ContentPresenter Grid.ColumnSpan="3"
									HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
									VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
									Content="{TemplateBinding Content}"
									ContentTemplate="{TemplateBinding ContentTemplate}" />

							<Image Name="Image" />

							<TextBlock
								Name="textRow1" Grid.Column="1" Margin="4,0,1,0"
								Text="{TemplateBinding Text}"
								TextAlignment="Left" VerticalAlignment="Center" />

							<Border Name="DropDownPanel" Grid.Column="2" VerticalAlignment="Stretch">
								<Path Name="DropDownIndicator" VerticalAlignment="Center" 
										HorizontalAlignment="Center"
										Data="M 0,0 L 3.5,3.5 L 0,7" Margin="2,0,0,0"
										Visibility="{TemplateBinding DropDownIndicatorVisibility}"
										Fill="{StaticResource RibbonButtonDropDownArrow}"
										Stroke="{StaticResource RibbonButtonDropDownArrowBorder}" />
							</Border>
						</Grid>
						
						<Popup x:Name="DropDownPopup" 
                            HorizontalOffset="2" VerticalOffset="-1"
							AllowsTransparency="True" StaysOpen="False" Focusable="false" Placement="Bottom"
							IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
						
						
								<!--PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}">-->
							<Grid>
								
								<Border x:Name="DropDownPopupBorder"
										BorderBrush="{StaticResource DropDownPopupBorderBrush}"
										Background="{StaticResource DropDownPopupBackgroundBrush}"
										BorderThickness="1"
										MinWidth="3" Width="{TemplateBinding DropDownWidth}"
										MinHeight="3" Height="{TemplateBinding DropDownHeight}">
									<ContentControl
										x:Name="DropDownPopupContent"
										Content="{TemplateBinding DropDownContent}"
										HorizontalAlignment="Stretch"
										VerticalAlignment="Stretch" />
								</Border>
							</Grid>
						
						</Popup>
						
						

						<Rectangle x:Name="DisabledIndicator" Visibility="Collapsed"
								Fill="#99FFFFFF" />
					</Grid>

					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="true">
							<Setter Property="BorderBrush" TargetName="buttonSurface"
									Value="{StaticResource ApplicationRibbonButtonOverBorder}" />
							<Setter Property="Background" TargetName="buttonSurface"
													Value="{StaticResource ApplicationRibbonButtonOverBackground}" />
							<Setter Property="BorderBrush" TargetName="buttonSurfaceInner"
									Value="{StaticResource ApplicationRibbonButtonOverBorderInner}" />
						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter Property="BorderBrush" TargetName="buttonSurface"
									Value="{StaticResource ApplicationRibbonButtonOverBorder}" />
							<Setter Property="Background" TargetName="buttonSurface"
									Value="{StaticResource ApplicationRibbonButtonOverBackground}" />
							<Setter Property="BorderBrush" TargetName="buttonSurfaceInner"
									Value="{StaticResource ApplicationRibbonButtonOverBorderInner}" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
							<Setter TargetName="DisabledIndicator" Property="Visibility" Value="Visible" />
						</Trigger>
						<!--<Trigger Property="TextAlignmentState" Value="TextHide">
							<Setter TargetName="textRow1" Property="Visibility" Value="Collapsed" />
						</Trigger>-->
						<Trigger Property="IsOpen" Value="True">
							<Setter TargetName="isOpenIndicator" Property="Visibility" Value="Visible" />
						</Trigger>
					</ControlTemplate.Triggers>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<ControlTemplate TargetType="telerik:RadButton" x:Key="AppMenuRadSplitButton_ButtonStyle">
		<Grid>
			

			<Border x:Name="backgroundDecorator"
					BorderThickness="{TemplateBinding BorderThickness}"
					BorderBrush="{TemplateBinding BorderBrush}"
