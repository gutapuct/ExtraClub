                                BorderBrush="{StaticResource ApplicationButtonType2010_InnerBorder_Selected}" />
                        </Border>
                        <Border x:Name="isOpenIndicator" Opacity="0" BorderThickness="1 1 1 0" CornerRadius="2 2 0 0"
                                Background="{StaticResource ApplicationButtonType2010_Background_Selected}"
                                BorderBrush="{StaticResource ApplicationButtonType2010_OuterBorder_Selected}" >
                            <Border  BorderThickness="1 1 1 0" CornerRadius="1 1 0 0"
                                BorderBrush="{StaticResource ApplicationButtonType2010_InnerBorder_Selected}" />
