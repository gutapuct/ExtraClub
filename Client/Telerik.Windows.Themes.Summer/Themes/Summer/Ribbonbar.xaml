            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonDropDownButton">
                    <Grid Width="46" Height="22">
                        

                       
                        <Border BorderThickness="1 1 1 0" CornerRadius="2 2 0 0"
                                 Background="{StaticResource ApplicationButtonType2010_Background}"
                                BorderBrush="{StaticResource ApplicationButtonType2010_OuterBorder}" >
