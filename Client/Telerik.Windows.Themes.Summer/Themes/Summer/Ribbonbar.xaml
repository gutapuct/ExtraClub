    <!--  MinimizeButton -->
    <Style x:Key="MinimizeButtonStyle" TargetType="telerik:RadToggleButton">
        <Setter Property="Margin" Value="1" />
        <Setter Property="Padding" Value="2" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadToggleButton">
                    <Grid Width="20">
                        

                        <telerikChromes:ButtonChrome telerik:StyleManager.Theme="{StaticResource Theme}"
                                Background="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Background}"
                                BorderBrush="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderBrush}"
                                BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness}"
                                RenderEnabled="{TemplateBinding IsEnabled}" RenderNormal="False"
                                RenderMouseOver="{TemplateBinding IsMouseOver}"
                                RenderPressed="{TemplateBinding IsPressed}" RenderFocused="{TemplateBinding IsFocused}"
                                RenderChecked="{TemplateBinding IsChecked}" CornerRadius="1" />
                        <Border BorderThickness="{TemplateBinding BorderThickness}">
                            <Grid VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}">
                                <Path x:Name="arrow" Fill="White" Stretch="None" Stroke="Black"
                                        Data="M0.25,4 L3.75,0.25 L4.75,0.25 L8.25,3.875 L8.3125,5.5 L7.25,6.627 L6.25,6.627 L4.25,4.627 L2.25,6.627 L1.25,6.627 L0.25,5.627 z"
                                        StrokeThickness="0.5" RenderTransformOrigin="0.5 0.5">
                                    <Path.RenderTransform>
                                        <RotateTransform x:Name="directionRotation" Angle="0" />
                                    </Path.RenderTransform>
                                </Path>
                                <ContentPresenter x:Name="Content" Margin="{TemplateBinding Padding}"
                                        Content="{TemplateBinding Content}"
                                        ContentTemplate="{TemplateBinding ContentTemplate}" />
                            </Grid>
                        </Border>
                    </Grid>

                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Opacity" TargetName="Content" Value="0.5" />
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="arrow" Property="LayoutTransform">
                                <Setter.Value>
                                    <TransformGroup>
                                        <RotateTransform Angle="180" />
                                    </TransformGroup>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                    </ControlTemplate.Triggers>
                    

                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- TabItem  Style -->
    <Style TargetType="telerikNavigation:RadTabItem" x:Key="RibbonRadTabItemStyle">
        <Setter Property="Padding" Value="0,1,0,1" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderThickness" Value="1,0,0,0" />
        <Setter Property="BorderBrush" Value="{StaticResource TabStripTabDividerBrush}" />
        <Setter Property="IsTabStop" Value="False" />
        
        <Setter Property="FocusVisualStyle">
            <Setter.Value>
                <Style TargetType="Control">
                    <Setter Property="Visibility" Value="Collapsed" />
                </Style>
            </Setter.Value>
        </Setter>
        
        <Setter Property="TopTemplate">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadTabItem">
                    <Grid>
                        
                        <Rectangle Margin="0 1 0 4" HorizontalAlignment="Left" Fill="{TemplateBinding BorderBrush}"
                                VerticalAlignment="Stretch" Width="1" />
                        <Grid x:Name="wrapper" Margin="4,1,3,1" 
                               >

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="3" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="3" />
                            </Grid.ColumnDefinitions>
                            <!--<Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" />-->

                            <Rectangle Grid.ColumnSpan="3" Margin="1,1,1,0" x:Name="rect">
                                <Rectangle.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop x:Name="BackgroundBrush1" Offset="0" />
                                        <GradientStop x:Name="BackgroundBrush2" Offset="1" />
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                            <Rectangle Grid.Column="0" Width="1" HorizontalAlignment="Left" Margin="-1,3,0,1"
                                    x:Name="LeftShadow" Visibility="Collapsed">
                                <Rectangle.Fill>
                                    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                        <GradientStop Offset="0" Color="Transparent" />
                                        <GradientStop Offset="0.6" Color="#08000000" />
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                            <Path Grid.Column="0" x:Name="LeftOuterBorder" Data="M -0.5,24 L 2.5,21 L 2.5,3 L 5.5,0"
                                    Margin="-2,0,0,0">
                                <Path.Stroke>
                                    <SolidColorBrush x:Name="LeftOuterBorderBrush" />
                                </Path.Stroke>
                            </Path>
                            <Path Grid.Column="0" x:Name="LeftInnerBorder" Data="M -0.5,24 L 2.5,21 L 2.5,3 L 4.5,1"
                                    Margin="-1,0,0,0">
                                <Path.Stroke>
                                    <SolidColorBrush x:Name="LeftInnerBorderBrush" />
                                </Path.Stroke>
                            </Path>

                            <Border Grid.Column="1" x:Name="Middle" BorderThickness="0,1,0,0">
                                <Border.BorderBrush>
                                    <SolidColorBrush x:Name="MiddleOuterBorderBrush" />
                                </Border.BorderBrush>
                            </Border>
                            <Rectangle Grid.Column="1" x:Name="MiddleInnerBorder" Height="1" VerticalAlignment="Top"
                                    Margin="0,1,0,0">
                                <Rectangle.Fill>
                                    <SolidColorBrush x:Name="MiddleInnerBorderBrush" />
                                </Rectangle.Fill>
                            </Rectangle>
                            <telerikNavigationPrimitives:TabItemContentPresenter Grid.Column="1" x:Name="HeaderElement"
                                    Content="{TemplateBinding Header}"
                                    ContentTemplate="{TemplateBinding HeaderTemplate}" IsTabStop="False"
                                    VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                    HorizontalAlignment="Center" HorizontalContentAlignment="Center"
                                    VerticalAlignment="Center" Margin="0,2,0,1" Foreground="#000000" />
                            <Path Grid.Column="2" x:Name="RightOuterBorder" Data="M 5.5,24 L 2.5,21 L 2.5,3 L -0.5,0"
                                    Margin="0,0,-2,0">
                                <Path.Stroke>
                                    <SolidColorBrush x:Name="RightOuterBorderBrush" />
                                </Path.Stroke>
                            </Path>
                            <Path Grid.Column="2" x:Name="RightInnerBorder" Data="M 4.5,24 L 1.5,21 L 1.5,3 L -0.5,1"
                                    Margin="0,0,-1,0">
                                <Path.Stroke>
                                    <SolidColorBrush x:Name="RightInnerBorderBrush" />
                                </Path.Stroke>
                            </Path>
                            <Rectangle Grid.Column="2" Width="1" HorizontalAlignment="Right" Margin="0,3,-1,1"
                                    x:Name="RightShadow1" Visibility="Collapsed">
                                <Rectangle.Fill>
                                    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                        <GradientStop Offset="0" Color="#11000000" />
                                        <GradientStop Offset="1" Color="#15000000" />
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                            <Rectangle Grid.Column="2" Width="1" HorizontalAlignment="Right" Margin="0,3,-2,1"
                                    x:Name="RightShadow2" Visibility="Collapsed">
                                <Rectangle.Fill>
                                    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                        <GradientStop Offset="0" Color="Transparent" />
                                        <GradientStop Offset="0.6" Color="#09000000" />
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                        </Grid>
                    </Grid>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="true">
                            <Setter Property="Stroke" TargetName="LeftOuterBorder">
                                <Setter.Value>
                                    <SolidColorBrush Color="Transparent" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="RightOuterBorder" Property="Stroke">
                                <Setter.Value>
                                    <SolidColorBrush Color="Transparent" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="Middle" Property="BorderBrush">
                                <Setter.Value>
                                    <SolidColorBrush Color="Transparent" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="LeftInnerBorder" Property="Stroke">
                                <Setter.Value>
                                    <SolidColorBrush Color="Transparent" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="RightInnerBorder" Property="Stroke">
                                <Setter.Value>
                                    <SolidColorBrush Color="Transparent" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="MiddleInnerBorder" Property="Fill">
                                <Setter.Value>
                                    <SolidColorBrush Color="Transparent" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="rect" Property="Fill">
                                <Setter.Value>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="Transparent" Offset="0" />
                                        <GradientStop Color="Transparent" Offset="1" />
                                    </LinearGradientBrush>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter Property="Stroke" TargetName="LeftOuterBorder">
                                <Setter.Value>
                                    <SolidColorBrush Color="{StaticResource TabStripTabHotOuterBorderColor}" />
                                </Setter.Value>
                            </Setter>

                            <Setter TargetName="RightOuterBorder" Property="Stroke">
                                <Setter.Value>
                                    <SolidColorBrush Color="{StaticResource TabStripTabHotOuterBorderColor}" />
                                </Setter.Value>
                            </Setter>

                            <Setter TargetName="Middle" Property="BorderBrush">
                                <Setter.Value>
                                    <SolidColorBrush Color="{StaticResource TabStripTabHotOuterBorderColor}" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="LeftInnerBorder" Property="Stroke">
                                <Setter.Value>
                                    <SolidColorBrush Color="{StaticResource TabStripTabHotInnerBorderColor}" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="RightInnerBorder" Property="Stroke">
                                <Setter.Value>
                                    <SolidColorBrush Color="{StaticResource TabStripTabHotInnerBorderColor}" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="MiddleInnerBorder" Property="Fill">
                                <Setter.Value>
                                    <SolidColorBrush Color="{StaticResource TabStripTabHotInnerBorderColor}" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="rect" Property="Fill">
                                <Setter.Value>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="{StaticResource TabStripTabHotBackgroundColor1}" Offset="0" />
                                        <GradientStop Color="{StaticResource TabStripTabHotBackgroundColor2}" Offset="1" />
                                    </LinearGradientBrush>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter Property="Stroke" TargetName="LeftOuterBorder">
                                <Setter.Value>
                                    <SolidColorBrush Color="{StaticResource TabStripTabSelectedOuterBorderColor}" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="RightOuterBorder" Property="Stroke">
                                <Setter.Value>
                                    <SolidColorBrush Color="{StaticResource TabStripTabSelectedOuterBorderColor}" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="Middle" Property="BorderBrush">
                                <Setter.Value>
                                    <SolidColorBrush Color="{StaticResource TabStripTabSelectedOuterBorderColor}" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="LeftInnerBorder" Property="Stroke">
                                <Setter.Value>
                                    <SolidColorBrush Color="{StaticResource TabStripTabSelectedLeftInnerBorderColor}" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="RightInnerBorder" Property="Stroke">
                                <Setter.Value>
                                    <SolidColorBrush Color="{StaticResource TabStripTabSelectedRightInnerBorderColor}" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="MiddleInnerBorder" Property="Fill">
                                <Setter.Value>
                                    <SolidColorBrush Color="{StaticResource TabStripTabSelectedMiddleInnerBorderColor}" />
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="rect" Property="Fill">
                                <Setter.Value>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="{StaticResource TabStripTabSelectedBackgroundColor1}"
                                                Offset="0" />
                                        <GradientStop Color="{StaticResource TabStripTabSelectedBackgroundColor2}"
                                                Offset="1" />
                                    </LinearGradientBrush>
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="wrapper" Property="Margin" Value="4,1,3,0" />
                            <Setter TargetName="LeftShadow" Property="Visibility" Value="Visible" />
                            <Setter TargetName="RightShadow1" Property="Visibility" Value="Visible" />
                            <Setter TargetName="RightShadow2" Property="Visibility" Value="Visible" />
                            <Setter TargetName="HeaderElement" Property="Margin" Value="0,1,0,1" />
                            <Setter TargetName="HeaderElement" Property="Foreground"
                                    Value="{StaticResource TabStripTabSelectedForegroundBrush}" />
                        </Trigger>

                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Opacity" Value="0.5" TargetName="HeaderElement" />
                        </Trigger>

                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- WindowTitle -->

    <Style x:Key="WindowTitleStyle" TargetType="telerikRibbonBarPrimitives:WindowTitle">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="FontSize" Value="11" />
        <Setter Property="IsHitTestVisible" Value="false" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="IsActive" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBarPrimitives:WindowTitle">
                    <StackPanel Orientation="Horizontal">
                        <TextBlock x:Name="Title" Text="{TemplateBinding Title}"
                                Foreground="{StaticResource RibbonTitlebarTitleBrush}" />
                        <TextBlock x:Name="Divider" Text=" - " Visibility="{TemplateBinding DividerVisibility}"
                                telerik:LocalizationManager.ResourceKey="RibbonBarWindowTitleDivider"
                                Foreground="{StaticResource RibbonTitlebarApplicationNameBrush}" />
                        <TextBlock x:Name="ApplicationName" Text="{TemplateBinding ApplicationName}"
                                Foreground="{StaticResource RibbonTitlebarApplicationNameBrush}" />
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="telerikRibbonBarPrimitives:WindowTitle" BasedOn="{StaticResource WindowTitleStyle}" />

    <!-- Empty ScrollViewer -->
    <Style x:Key="EmptyScrollViewerStyle" TargetType="ScrollViewer">
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Top" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ScrollViewer">
                    <Border CornerRadius="2" BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid Background="{TemplateBinding Background}">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <ScrollContentPresenter x:Name="ScrollContentPresenter" Cursor="{TemplateBinding Cursor}"
                                    Margin="{TemplateBinding Padding}"
                                    ContentTemplate="{TemplateBinding ContentTemplate}" />
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- Scroll Button:-->
    <Style x:Key="TabScrollButtonStyle" TargetType="RepeatButton">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="RepeatButton">
                    <Grid>
                        
                        <Border x:Name="OuterBorder"
                                Background="{Binding Background, RelativeSource={RelativeSource TemplatedParent}}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                BorderBrush="{Binding BorderBrush, RelativeSource={RelativeSource TemplatedParent}}"
                                CornerRadius="3 0 0 3">
                            <Border x:Name="InnerBorder" CornerRadius="2 0 0 2"
                                    BorderBrush="{StaticResource PageScrollViewerButtonInnerBorderBrush}"
                                    BorderThickness="1" Padding="{TemplateBinding Padding}">
                                <ContentPresenter Content="{TemplateBinding Content}"
                                        ContentTemplate="{TemplateBinding ContentTemplate}"
                                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                            </Border>
                        </Border>

                    </Grid>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="OuterBorder" Property="Background"
                                    Value="{StaticResource ButtonHotBackgroundBrush}" />
                            <Setter TargetName="OuterBorder" Property="BorderBrush"
                                    Value="{StaticResource ButtonPushedOuterBorderBrush}" />
                            <Setter TargetName="InnerBorder" Property="BorderBrush"
                                    Value="{StaticResource ButtonHotInnerBorderBrush}" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="OuterBorder" Property="Background"
                                    Value="{StaticResource ButtonPushedBackgroundBrush}" />
                            <Setter TargetName="OuterBorder" Property="BorderBrush"
                                    Value="{StaticResource ButtonPushedOuterBorderBrush}" />
                            <Setter TargetName="InnerBorder" Property="BorderBrush"
                                    Value="{StaticResource ButtonPushedInnerBorderBrush}" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="OuterBorder" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Content" Value="" />
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Path Stretch="Uniform" Height="5" Width="3"
                            Data="F1 M 110.692,342.252L 110.692,352.682L 104.594,347.467L 110.692,342.252 Z"
                            Fill="{StaticResource PageScrollViewerButtonForegroundBrush}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Padding" Value="2 2 3 2" />
        <Setter Property="Margin" Value="2" />
        
        
        <Setter Property="DockPanel.Dock" Value="Left" />
        
        <Setter Property="BorderBrush" Value="{StaticResource PageScrollViewerButtonOuterBorderBrush}" />
        <Setter Property="Background" Value="{StaticResource TabPageScrollViewerButtonBackgroundBrush}" />
        <Setter Property="IsTabStop" Value="False" />
    </Style>

    <!-- TabControl Top Template:-->
    <ControlTemplate x:Key="TabControlTopTemplate" TargetType="telerikNavigation:RadTabControl">
        <Grid>
            
            <Grid.RowDefinitions>
                <RowDefinition Height="auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <!--Content-->
            <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}"
                    Grid.Row="1">
                
                
                <ContentControl x:Name="ContentElement" Padding="{TemplateBinding Padding}"
                        Content="{TemplateBinding SelectedContent}"
                        ContentTemplate="{TemplateBinding SelectedContentTemplate}"
                        Foreground="{TemplateBinding Foreground}" IsTabStop="False" />
                
            </Border>

            <!--Header Background-->
            <Rectangle Visibility="{TemplateBinding BackgroundVisibility}" />

            <!--TabStrip-->
            
                
                <DockPanel>
                    
                    <!-- Items: -->

                    <RepeatButton x:Name="LeftScrollButtonElement" Style="{StaticResource TabScrollButtonStyle}"
                            Margin="4 4 2 7"
                              
                        
                        DockPanel.Dock="Left"
                        
                        Visibility="{Binding ComputedHorizontalScrollBarVisibility, ElementName=ScrollViewerElement}" />
                        <RepeatButton x:Name="RightScrollButtonElement" RenderTransformOrigin="0.5,0.5"
                                Style="{StaticResource TabScrollButtonStyle}" Margin="2 4 2 7"
                               
                            
                            DockPanel.Dock="Right"
                            
                            Visibility="{Binding ComputedHorizontalScrollBarVisibility, ElementName=ScrollViewerElement}" >
                            <RepeatButton.RenderTransform>
                                <ScaleTransform ScaleX="-1" />
                            </RepeatButton.RenderTransform>
                        </RepeatButton>

                        <ScrollViewer x:Name="ScrollViewerElement" Style="{StaticResource EmptyScrollViewerStyle}"
                                Margin="2 4 2 0" VerticalScrollBarVisibility="Disabled"
                                HorizontalScrollBarVisibility="{Binding (ScrollViewer.HorizontalScrollBarVisibility), RelativeSource={RelativeSource TemplatedParent}}">
                            <ItemsPresenter />
                        </ScrollViewer>
                        
            
            </DockPanel>
            

        </Grid>
        
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <!--<Setter Property="Visibility" Value="Visible" TargetName="DisableVisual" />-->
            </Trigger>
            <Trigger Property="DropDownDisplayMode" Value="Visible">
                <!--<Setter Property="Visibility" Value="Visible" TargetName="DropDownButtonElement" />-->
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="DropDownDisplayMode" Value="WhenNeeded" />
                    <Condition SourceName="ScrollViewerElement" Property="ComputedHorizontalScrollBarVisibility"
                            Value="Visible" />
                </MultiTrigger.Conditions>
                <!--<Setter Property="Visibility" Value="Visible"
                                TargetName="DropDownButtonElement" />-->
            </MultiTrigger>
        </ControlTemplate.Triggers>
        
    </ControlTemplate>

    <!-- RadTabControl -->
    <Style TargetType="telerikNavigation:RadTabControl" x:Key="customType">
        <Setter Property="MinWidth" Value="5" />
        <Setter Property="MinHeight" Value="0" />
        <Setter Property="MaxHeight" Value="26" />
        <Setter Property="Foreground" Value="#FF000000" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="IsEnabled" Value="True" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="BorderThickness" Value="1 0 1 1" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikNavigationPrimitives:TabStripPanel x:Name="TabStripPanel" Margin="-2 0 -2 0" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto" />
        <Setter Property="Template" Value="{StaticResource TabControlTopTemplate}" />
        <Setter Property="TopTemplate" Value="{StaticResource TabControlTopTemplate}" />
    </Style>

    <Style TargetType="telerikRibbonBar:QuickAccessToolBar" 
           x:Key="{telerik:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikRibbonBar:QuickAccessToolBar}"
         >
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Background" Value="{StaticResource QatBelowBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{StaticResource QatBelowOuterBorderBrush}" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="CustomizationMenuVisibility" Value="Visible" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:QuickAccessToolBar">
                    <Grid MinHeight="22" x:Name="RootElement">
                        
                        <Grid x:Name="BelowBackgroundGrid" Visibility="Collapsed">
                            <Border BorderThickness="1" CornerRadius="2 2 0 0"
                                    BorderBrush="{StaticResource QatBelowInnerBorderBrush}" Margin="1,1,0,0"
                                    Background="{TemplateBinding Background}" />
                            <Border BorderThickness="1" CornerRadius="2 2 0 0"
                                    BorderBrush="{TemplateBinding BorderBrush}" Margin="0,0,1,1" />
                        </Grid>

                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="13" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid x:Name="AboveBackgroundGridWrapper">
                                <Grid x:Name="AboveBackgroundGrid" Height="22">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="14" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="10" />
                                        <ColumnDefinition Width="5" />
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Column="1" BorderBrush="{StaticResource QatAboveHighlightBrush}"
                                            BorderThickness="0,1,0,1" />
                                    <Border Grid.Column="1" Background="{StaticResource QatAboveBackgroundBrush}"
                                            BorderBrush="{StaticResource QatAboveBorderBrush}" BorderThickness="0,1,0,1"
                                            Margin="0,1,0,1" />
                                    <Path Grid.Column="0" Data="M 14,0.5 L -0.5,0.5 A 28,28 30 0 1 12,23.5 L 14,23.5"
                                            Stroke="{StaticResource QatAboveHighlightBrush}" StrokeThickness="1" />
                                    <Path Grid.Column="0" Data="M 14,1.5 L 1,1.5 A 27,27 30 0 1 13,22.5 L 14,22.5"
                                            Fill="{StaticResource QatAboveBackgroundBrush}"
                                            Stroke="{StaticResource QatAboveBorderBrush}" StrokeThickness="1" />
                                    <Path Grid.Column="2" Data="M0,0.5 A9,10,180,1,1,0,21.5"
                                            Stroke="{StaticResource QatAboveHighlightBrush}" StrokeThickness="1" />
                                    <Path Grid.Column="2" Data="M 0,1.5 A 8,10 180 1 1 0,22.5"
                                            Fill="{StaticResource QatAboveBackgroundBrush}"
                                            Stroke="{StaticResource QatAboveBorderBrush}" StrokeThickness="1" />
                                </Grid>
                            </Grid>
                            <Grid HorizontalAlignment="Left" x:Name="PART_StripPanelGrid" Grid.Column="0"
                                    Margin="16,0,22,0">
                                <StackPanel x:Name="PART_StripPanel" Orientation="Horizontal" HorizontalAlignment="Left" />
                            </Grid>

                            <Popup Grid.Column="1" x:Name="PART_Overflow">

                                <Border Margin="0" MinWidth="55" MinHeight="33">
                                    <Border BorderBrush="{StaticResource OverflowBorderInnerBrush}"
                                            Background="{StaticResource OverflowContentBackgroundBrush}"
                                            BorderThickness="1" Padding="1,2,1,2">

                                        <StackPanel x:Name="PART_OverflowPanel" Orientation="Vertical"
                                                VerticalAlignment="Top" />
                                    </Border>
                                </Border>
                            </Popup>

                            <telerikRibbonBar:RadRibbonDropDownButton
                                    Visibility="{TemplateBinding CustomizationMenuVisibility}"
                                    telerik:StyleManager.Theme="{StaticResource Theme}"
                                    DropDownIndicatorVisibility="Collapsed" Width="13" Height="22"
                                    HorizontalAlignment="Left" Grid.Column="1" VerticalAlignment="Top" Padding="0,2,0,2">
                                <telerikRibbonBar:RadRibbonDropDownButton.DropDownContent>
                                    <StackPanel>
                                        <telerikRibbonBar:RadGroupHeader Content="Customize Quick Access Toolbar"
                                                telerik:LocalizationManager.ResourceKey="RibbonBarQATCustomize" />

                                        <telerikNavigation:RadContextMenu
                                                telerik:StyleManager.Theme="{StaticResource Theme}" BorderThickness="0">
                                            <telerikNavigation:RadMenuItem x:Name="TogglePositionMenuItem"
                                                    Header="Show Below the Ribbon."
                                                    telerik:LocalizationManager.ResourceKey="RibbonBarQATShowBelow" />
                                            <telerikNavigation:RadMenuItem IsSeparator="True" Padding="0" Margin="0" />
                                            <telerikNavigation:RadMenuItem x:Name="ToggleMinimizeMenuItem"
                                                    Header="Minimize the Ribbon."
                                                    telerik:LocalizationManager.ResourceKey="RibbonBarQATMinimize" />
                                        </telerikNavigation:RadContextMenu>

                                    </StackPanel>
                                </telerikRibbonBar:RadRibbonDropDownButton.DropDownContent>
                                <Path x:Name="MainPopupButtonPath" HorizontalAlignment="Center"
                                        VerticalAlignment="Center" Margin="4,7,2,0"
                                        Data="M 0,-2.5 L 0,-1.5 L 7,-1.5 L 7,-2.5 M 0,0 L 3.5,3.5 L 7,0"
                                        Fill="{StaticResource QatButtonArrow}"
                                        Stroke="{StaticResource QatButtonArrowHighlight}" />
                            </telerikRibbonBar:RadRibbonDropDownButton>

                        </Grid>
                    </Grid>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="Position" Value="BelowRibbon">
                            <Setter Property="Visibility" Value="Collapsed" TargetName="AboveBackgroundGrid" />
                            <Setter Property="Visibility" Value="Visible" TargetName="BelowBackgroundGrid" />
                            <Setter Property="Margin" Value="2,2,0,2" TargetName="PART_StripPanelGrid" />
                            <!--<Setter Property="Header" Value="Show Above the Ribbon." TargetName="TogglePositionMenuItem" />-->
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="TogglePositionMenuItem"
                                                Storyboard.TargetProperty="Header">
                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                    telerik:LocalizationManager.ResourceKey="RibbonBarQATShowAbove" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="TogglePositionMenuItem"
                                                Storyboard.TargetProperty="Header">
                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                    telerik:LocalizationManager.ResourceKey="RibbonBarQATShowBelow" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="ApplicationButtonType" Value="Office2010">
                            <Setter Property="Visibility" Value="Collapsed" TargetName="AboveBackgroundGrid" />
                            <Setter Property="Margin"     Value="2 0 0 0"   TargetName="PART_StripPanelGrid"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

   
    <Style TargetType="telerikRibbonBar:RadRibbonContextualGroupContainer">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonContextualGroupContainer">
                    <Grid>
                        <ItemsPresenter />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="telerikRibbonBar:RadRibbonContextualGroup" x:Key="ContextualTabsHeaderstyle">
        <Setter Property="Foreground" Value="{StaticResource ContextualTabsHeader_Foreground}" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonContextualGroup">
                    <Grid 
                         Margin="0 -1 0 -3"
                         >
                        <Border BorderThickness="1 0" BorderBrush="#737b87" Margin="-0.5 0 0 2" />
                        <Border Margin="1"
                                Background="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Background}">
                            <Border.OpacityMask>
                                <RadialGradientBrush Center="0.507,1.136" GradientOrigin="0.5,1.136" RadiusX="0.736"
                                        RadiusY="1.08">
                                    <GradientStop Offset="1" />
                                    <GradientStop Color="#B2FFFFFF" Offset="0.325" />
                                </RadialGradientBrush>
                            </Border.OpacityMask>
                        </Border>
                        <Border BorderThickness="0 0 0 1"
                                BorderBrush="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Background}"
                                Margin="0 0 0 1" VerticalAlignment="Bottom">
                            <Border.OpacityMask>
                                <LinearGradientBrush EndPoint="0.007,0.986" StartPoint="1.006,0.986">
                                    <GradientStop Offset="0" Color="#26000000" />
                                    <GradientStop Color="White" Offset="0.5" />
                                    <GradientStop Offset="1" Color="#26000000" />
                                </LinearGradientBrush>
                            </Border.OpacityMask>
                        </Border>
                        <ContentPresenter Margin="10 0 0 0" Content="{TemplateBinding Header}" ContentTemplate="{TemplateBinding HeaderTemplate}"
                                HorizontalAlignment="Left" VerticalAlignment="Center" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- RibbonBar -->
    <Style x:Key="RibbonBarStyle"  TargetType="telerikRibbonBar:RadRibbonBar">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="Background" Value="{StaticResource RibbonBackgroundBrush}" />
        <Setter Property="Foreground" Value="{StaticResource RibbonForegroundBrush}" />
        <Setter Property="ApplicationButtonVisibility" Value="Visible" />
        <Setter Property="TitleBarVisibility" Value="Visible" />
        <Setter Property="HelpButtonVisibility" Value="Collapsed" />
        <Setter Property="MinimizeButtonVisibility" Value="Collapsed" />
        <Setter Property="ApplicationName" Value="My Application" />
        <Setter Property="IsMinimizable" Value="True" />
        <Setter Property="IsMouseWheelTabScrollingEnabled" Value="True" />
        <Setter Property="QuickAccessToolBarPosition" Value="AboveRibbon" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="CollapseThresholdSize" Value="300, 250" />
        <Setter Property="FontSize" Value="10.8" />
        <Setter Property="HelpButtonImageSource" Value="/Telerik.Windows.Controls.RibbonBar;component/HelpIcon.png" />
        <Setter Property="TitleBarStyle" Value="{StaticResource WindowTitleStyle}" />
        <Setter Property="TitleBarBackground" Value="{StaticResource RibbonTitlebarBackgroundBrush}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadRibbonBar">
                    <Grid x:Name="RootElement" SnapsToDevicePixels="True"
                         >
                        <Grid.Resources>
                            <Style TargetType="telerikNavigation:RadTabItem" x:Key="ContextualTabsStyle">
                                <Setter Property="Padding" Value="0,1,0,1" />
                                <Setter Property="Margin" Value="0" />
                                <Setter Property="Background" Value="Pink" />
                                <Setter Property="BorderThickness" Value="1,0,0,0" />
                                <Setter Property="Foreground" Value="Pink" />
                                <Setter Property="IsTabStop" Value="False" />
                                <Setter Property="BorderBrush" Value="{StaticResource TabStripTabDividerBrush}" />
                                
                                <Setter Property="FocusVisualStyle">
                                    <Setter.Value>
                                        <Style TargetType="Control">
                                            <Setter Property="Visibility" Value="Collapsed" />
                                        </Style>
                                    </Setter.Value>
                                </Setter>
                                
                                <Setter Property="TopTemplate">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="telerikNavigation:RadTabItem">
                                            <Grid x:Name="wrapper" Margin="1,1,0,1">
                                                

                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="3" />
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="3" />
                                                </Grid.ColumnDefinitions>

                                                <Border BorderBrush="#848484" Grid.ColumnSpan="3" Opacity="0"
                                                        x:Name="MouseOverVisual" BorderThickness="1 1 1 0"
                                                        CornerRadius="3 3 0 0">
                                                    <Border CornerRadius="2 2 0 0" BorderBrush="#99FFFFFF"
                                                            BorderThickness="1 1 1 0">
                                                        <Border CornerRadius="1 1 0 0" Opacity="0.6"
                                                                Background="{TemplateBinding Background}">
                                                            <Border.OpacityMask>
                                                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                                                    <GradientStop Color="#00FFFFFF" Offset="0" />
                                                                    <GradientStop Color="#ffFFFFFF" Offset="1" />
                                                                    <GradientStop Color="#19FFFFFF" Offset="0.527" />
                                                                </LinearGradientBrush>
                                                            </Border.OpacityMask>
                                                        </Border>
                                                    </Border>
                                                </Border>

                                                <Border BorderBrush="#848484" Grid.ColumnSpan="3" Opacity="0"
                                                        x:Name="SelectionVisual" BorderThickness="1 1 1 0"
                                                        CornerRadius="3 3 0 0" Background="#FFFFFFFF">
                                                    <Border CornerRadius="2 2 0 0" x:Name="InnerBackground"
                                                            Background="{TemplateBinding Background}">
                                                        <Border.OpacityMask>
                                                            <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                                                <GradientStop Color="#19FFFFFF" Offset="0" />
                                                                <GradientStop Color="#59FFFFFF" Offset="0.173" />
                                                                <GradientStop Color="#7FFFFFFF" Offset="0.191" />
                                                                <GradientStop Color="Transparent" Offset="0.866" />
                                                            </LinearGradientBrush>
                                                        </Border.OpacityMask>
                                                    </Border>
                                                </Border>

                                                <telerikNavigationPrimitives:TabItemContentPresenter Grid.Column="1"
                                                        IsTabStop="False" x:Name="HeaderElement"
                                                        Content="{TemplateBinding Header}"
                                                        ContentTemplate="{TemplateBinding HeaderTemplate}"
                                                        VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                                        HorizontalAlignment="Center" HorizontalContentAlignment="Center"
                                                        VerticalAlignment="Center" Margin="0,2,0,1" Foreground="#000000" />

                                            </Grid>
                                            
                                            <ControlTemplate.Triggers>
                                                <Trigger Property="IsMouseOver" Value="True">
                                                    <Setter TargetName="MouseOverVisual" Property="Opacity" Value="1" />
                                                </Trigger>
                                                <Trigger Property="IsSelected" Value="True">
                                                    <Setter TargetName="SelectionVisual" Property="Opacity" Value="1" />
                                                </Trigger>
                                                <Trigger Property="IsEnabled" Value="False">
                                                    <Setter Property="Opacity" Value="0.5" TargetName="HeaderElement" />
                                                    <Setter TargetName="SelectionVisual" Property="Opacity" Value="0" />
                                                    <Setter TargetName="MouseOverVisual" Property="Opacity" Value="0" />
                                                </Trigger>

                                                <!--Horizontal Left-->
                                                <MultiTrigger>
                                                    <MultiTrigger.Conditions>
                                                        <Condition Property="IsMouseOver" Value="True" />
                                                        <Condition Property="IsSelected" Value="True" />
                                                    </MultiTrigger.Conditions>
                                                    <Setter TargetName="InnerBackground" Property="Background"
                                                            Value="#fbe0a1" />
                                                </MultiTrigger>
                                            </ControlTemplate.Triggers>
                                            
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </Grid.Resources>
                        
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" MaxHeight="28" />
                            <RowDefinition Height="Auto" MaxHeight="24" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>

                        <!-- Background Fill -->
                        <Rectangle Grid.Row="1" Grid.RowSpan="3" Grid.ColumnSpan="2" Fill="{TemplateBinding Background}" />
                        <!-- Titlebar background -->
                        <Border Grid.ColumnSpan="2" x:Name="titleBarBackground" BorderThickness="0,0,0,1"
                                Visibility="{TemplateBinding TitleBarVisibility}"
                                BorderBrush="{StaticResource RibbonTitlebarBorderBrush}"
                                Background="{TemplateBinding TitleBarBackground}" />
                        <!-- Tabstrip background -->
                        <Rectangle Grid.Row="1" Grid.ColumnSpan="2" VerticalAlignment="Top"
                                Visibility="{TemplateBinding TitleBarVisibility}" Height="1"
                                Fill="{StaticResource RibbonTabstripBorderBrush}" />

                        <Grid Grid.Column="1" x:Name="TitleBar_RibbonBar" Height="28"
                                Visibility="{TemplateBinding TitleBarVisibility}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <ContentPresenter x:Name="AboveToolBarHost" Margin="-7,0,0,1" VerticalAlignment="Bottom"
                                    HorizontalAlignment="Left" />
                            
                        </Grid>

                        <!-- Tabstrip and help button -->
                        <Grid Grid.Row="1" Grid.Column="1" Canvas.ZIndex="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <!--ItemContainerStyle="{StaticResource RibbonRadTabItemStyle}"-->
                            <telerikNavigation:RadTabControl BackgroundVisibility="Collapsed"
                                    ItemContainerStyle="{StaticResource RibbonRadTabItemStyle}" 
                                    Style="{StaticResource customType}" IsTabStop="False" x:Name="TabControl"
                                    Margin="10,-3,0,0" ScrollViewer.HorizontalScrollBarVisibility="Auto"
									 >

                                <telerikNavigation:RadTabControl.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <telerikRibbonBarPrimitives:TabStripPanel SeparatorOpacityThreshold="3"
                                                VariablePaddingLength="15" />
                                    </ItemsPanelTemplate>
                                </telerikNavigation:RadTabControl.ItemsPanel>
                            </telerikNavigation:RadTabControl>

                            <StackPanel Grid.Column="1" Orientation="Horizontal">

                                <telerikRibbonBar:RadRibbonToggleButton Style="{StaticResource MinimizeButtonStyle}"
                                        Visibility="{TemplateBinding MinimizeButtonVisibility}" Size="Small"
                                        x:Name="MinimizeButton" />

                                <telerikRibbonBar:RadRibbonButton telerik:StyleManager.Theme="{StaticResource Theme}"
                                        Visibility="{TemplateBinding HelpButtonVisibility}" x:Name="HelpButton"
                                        Size="Small" Height="21" SmallImage="{TemplateBinding HelpButtonImageSource}"
                                        VerticalAlignment="Center" />

                                <ContentPresenter Margin="0" Content="{TemplateBinding TabStripAdditionalContent}" />
                            </StackPanel>
                        </Grid>
                        
                        <DockPanel Grid.Column="0" Grid.ColumnSpan="2" MaxHeight="23"
                                Visibility="{TemplateBinding TitleBarVisibility}">
                            <ContentControl x:Name="windowButtonPlaceHolder" IsHitTestVisible="false"
                                    DockPanel.Dock="Right" Focusable="false"
                                    Width="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type Window}}, Path=CaptionButtonsReservedWidth}"
                                    IsTabStop="False" />
                            <ContentControl x:Name="windowIconPadding" Width="28" IsHitTestVisible="false"
                                    DockPanel.Dock="Left" Focusable="false" IsTabStop="False" />
                            <telerikRibbonBarPrimitives:RibbonTitlePanel x:Name="TitleHost">
                                <telerikRibbonBarPrimitives:RibbonTitlePanel.ContextualGroupPlaceholder>
                                    <ItemsControl x:Name="ContextPanel"
                                            ItemContainerStyle="{StaticResource ContextualTabsHeaderstyle}">
                                        <ItemsControl.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <telerikRibbonBarPrimitives:ContextualGroupPanel />
                                            </ItemsPanelTemplate>
                                        </ItemsControl.ItemsPanel>
                                    </ItemsControl>
                                </telerikRibbonBarPrimitives:RibbonTitlePanel.ContextualGroupPlaceholder>
                                <telerikRibbonBarPrimitives:RibbonTitlePanel.TitlePlaceholder>
                                    <telerikRibbonBarPrimitives:WindowTitle                                            
                                            Title="{TemplateBinding Title}"
                                            ApplicationName="{TemplateBinding ApplicationName}"
                                            Style="{TemplateBinding TitleBarStyle}" />
                                </telerikRibbonBarPrimitives:RibbonTitlePanel.TitlePlaceholder>
                                <telerikRibbonBarPrimitives:RibbonTitlePanel.QuickAccessToolBarPlaceholder>
                                    <StackPanel>
                                        <ContentPresenter Margin="5,1,0,0" x:Name="QatTopHost"
                                                Content="{TemplateBinding QuickAccessToolBar}" />
                                    </StackPanel>
                                </telerikRibbonBarPrimitives:RibbonTitlePanel.QuickAccessToolBarPlaceholder>
                            </telerikRibbonBarPrimitives:RibbonTitlePanel>
                        </DockPanel>
                        
                        <!-- Application button -->
                        <telerikRibbonBar:RadRibbonDropDownButton IsTabStop="False" x:Name="ApplicationButton"
                                Grid.RowSpan="2" DropDownIndicatorVisibility="Collapsed" IsBackstageOpen="{TemplateBinding IsBackstageOpen}"
                                SmallImage="{TemplateBinding ApplicationButtonImageSource}"
                                Content="{TemplateBinding ApplicationButtonContent}" IsAutoSize="False"
                                Style="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ApplicationButtonStyle, Mode=TwoWay}"
                                Visibility="{TemplateBinding ApplicationButtonVisibility}" />

                        <!-- Tab content -->
                        <Border Canvas.ZIndex="0" x:Name="TabContentHost" Grid.Row="2" Grid.ColumnSpan="2"
                                Background="{StaticResource RibbonLowerBackgroundBrush}" Visibility="Collapsed">
                            <Grid>
                                

                                <ContentPresenter x:Name="SelectedTabPresenter" Margin="1,-1,1,2" Height="91.0" />
                                <Rectangle VerticalAlignment="Bottom" Margin="4,0,4,1" Height="1" Fill="#0C000000" />
                            </Grid>
                        </Border>

                        <Popup x:Name="TabContentPopup" VerticalOffset="-2" 
                               AllowsTransparency="True"
                             >
                        </Popup>

                        

                            <!-- RadToolBar -->
                        <Border x:Name="MinimizedDivider" Height="2" Grid.Row="3" Grid.ColumnSpan="2"
                                VerticalAlignment="Top" BorderThickness="0,1,0,0"
                                BorderBrush="{StaticResource RibbonDividerDarkBrush}"
                                Background="{StaticResource RibbonDividerLightBrush}" />
                        <ContentPresenter x:Name="BelowToolBarHost" Grid.Row="3" Grid.ColumnSpan="2" />
                    </Grid>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="True">
                            <Setter Property="Visibility" Value="Visible" TargetName="TabContentHost" />
                            <Setter Property="Visibility" Value="Collapsed" TargetName="MinimizedDivider" />
                        </Trigger>
                        <Trigger Property="IsMinimized" Value="True">
                            <Setter Property="Visibility" Value="Collapsed" TargetName="TabContentHost" />
                            <Setter Property="Visibility" Value="Visible" TargetName="MinimizedDivider" />
                        </Trigger>
                        <Trigger Property="IsHostedInRibbonWindow" Value="true">
                            <Setter TargetName="titleBarBackground" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <Trigger Property="ApplicationButtonType" Value="Office2007">
                            <Setter TargetName="ApplicationButton" Property="Grid.Row" Value="0" />
                            <Setter TargetName="ApplicationButton" Property="Grid.RowSpan" Value="2" />
                        </Trigger>
                        <Trigger Property="ApplicationButtonType" Value="Office2010">
                            <Setter TargetName="ApplicationButton" Property="Grid.Row" Value="1" />
                            <Setter TargetName="ApplicationButton" Property="Grid.RowSpan" Value="1" />
                            <Setter TargetName="TitleBar_RibbonBar" Property="Grid.Column" Value="0" />
                            <Setter TargetName="TitleBar_RibbonBar" Property="Grid.ColumnSpan" Value="2" />
                            <Setter TargetName="AboveToolBarHost" Property="Margin" Value="0 0 0 1" />
                            <Setter TargetName="TabControl" Property="Margin" Value="0 -3 0 0" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    
    

    <Style TargetType="telerikRibbonBar:RadRibbonBar"
		x:Key="{telerik:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikRibbonBar:RadRibbonBar}"
         BasedOn="{StaticResource RibbonBarStyle}" >
        <Setter Property="ApplicationButtonStyleOffice2010" Value="{StaticResource ApplicationButtonStyleOffice2010}" />
        <Setter Property="ApplicationButtonStyleOffice2007" Value="{StaticResource ApplicationButtonStyleOffice2007}" />
        <Setter Property="ApplicationButtonType" Value="Office2007" />
    </Style>
    
    

	<Style TargetType="telerikRibbonBar:RadRibbonButton"
		
		x:Key="{telerik:ThemeResourceKey ThemeType=
		external:SummerThemeExternal , ElementType=telerikRibbonBar:RadRibbonButton}"
		
		>
		<Setter Property="Padding" Value="3" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="HorizontalAlignment" Value="Left" />
		<Setter Property="HorizontalContentAlignment" Value="Left" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBar:RadRibbonButton">
					<Grid >
						

						<Border x:Name="buttonSurface"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}"
								Background="{TemplateBinding Background}"
								CornerRadius="2">
							<Border x:Name="buttonSurfaceInner"
									BorderBrush="Transparent"
									BorderThickness="{TemplateBinding BorderThickness}" />
						</Border>

                        <Grid Margin="{TemplateBinding Padding}" x:Name="ContentGrid">
							<ContentPresenter
								Name="ContentPresenter"
								Content="{TemplateBinding Content}"
								ContentTemplate="{TemplateBinding ContentTemplate}"
								VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
								HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
								RecognizesAccessKey="True"
								 />

							<Grid>
								<Grid.RowDefinitions>
									<RowDefinition Height="Auto"/>
									<RowDefinition Height="*" MaxHeight="28"/>
								</Grid.RowDefinitions>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="*" />
								</Grid.ColumnDefinitions>

								<Image Name="Image"/>

								<TextBlock Name="textRow1" Grid.Row="1" Margin="0,2,0,0"
									Text="{TemplateBinding TextRow1}" TextAlignment="Center" />
								<TextBlock Name="textRow2" Grid.Row="1" Margin="0,14,0,0"
									Text="{TemplateBinding TextRow2}" TextAlignment="Center" />
							</Grid>
						</Grid>
					</Grid>

					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="True">
							<Setter Property="BorderBrush" TargetName="buttonSurface" Value="{StaticResource RadRibbonBarButtonOverBorder}"/>
							<Setter Property="Background" TargetName="buttonSurface" Value="{StaticResource RadRibbonBarButtonOverBackground}"/>
							<Setter Property="BorderBrush" TargetName="buttonSurfaceInner" Value="{StaticResource RadRibbonBarButtonOverBorderInner}"/>
						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter Property="BorderBrush" TargetName="buttonSurface" Value="{StaticResource RadRibbonBarButtonPressedBorder}"/>
							<Setter Property="Background" TargetName="buttonSurface" Value="{StaticResource RadRibbonBarButtonPressedBackground}"/>
							<Setter Property="BorderBrush" TargetName="buttonSurfaceInner" Value="{StaticResource RadRibbonBarButtonPressedBorderInner}"/>
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="ContentGrid" Property="Opacity" Value="0.5" />
						</Trigger>
						<Trigger Property="IsDefault" Value="True" />
						<Trigger Property="CurrentSize" Value="Large" />
						<Trigger Property="CurrentSize" Value="Medium">
							<Setter TargetName="textRow1" Property="Grid.Row" Value="0" />
							<Setter TargetName="textRow1" Property="Grid.Column" Value="1" />
							<Setter TargetName="textRow1" Property="Margin" Value="4,0,1,0" />
							<Setter TargetName="textRow1" Property="VerticalAlignment" Value="Center" />
							<Setter TargetName="textRow1" Property="TextAlignment" Value="Left" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="CurrentSize" Value="Small">
							<Setter TargetName="textRow1" Property="Visibility" Value="Collapsed" />
							<Setter TargetName="textRow2" Property="Visibility" Value="Collapsed" />
						</Trigger>
					</ControlTemplate.Triggers>
					
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	
	

	<!-- For  ComboBox, ... -->
	<Style TargetType="telerik:PickerToggleButton" x:Key="ToogleButtonStyle" >
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="BorderBrush" Value="{StaticResource ExpanderButtonNormal_Border}" />
		<Setter Property="Background" Value="{StaticResource ExpanderButtonNormal_Background}" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerik:PickerToggleButton">
					<Grid>
                        
						<Border x:Name="Chrome"
								BorderBrush="{TemplateBinding BorderBrush}"
								BorderThickness="1">
							<Border x:Name="ChromeInner"
									BorderBrush="{TemplateBinding Background}"
									BorderThickness="1"
									Background="{TemplateBinding Background}" />
						</Border>

						<Border x:Name="ChromeCheck" Visibility="Collapsed"
								BorderBrush="{StaticResource ExpandButtonPressed_OuterBorder}"
								BorderThickness="1">
							<Border BorderBrush="{StaticResource ExpanderButtonPressed_Border}"
									BorderThickness="1"
									Background="{StaticResource ExpanderButtonPressed_Background}" />
						</Border>

						<Path x:Name="Path"
								Data="M-0.7,0.45 L6.2,0.45 L2.814,5.509 L2.814,5.509 z" 
								Fill="{StaticResource RibbonGroupButtonArrow}" 
								Stretch="Fill" Width="5" Height="3"
								HorizontalAlignment="Right"
								Margin="0 0 4 0"
								VerticalAlignment="Center" />

						<ContentPresenter Content="{TemplateBinding Content}"
								ContentTemplate="{TemplateBinding ContentTemplate}" />

					</Grid>

					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="true">
							<Setter TargetName="Chrome"  Property="BorderBrush" 
									Value="{StaticResource ExpandButtonOver_OuterBorder}" />
							<Setter TargetName="ChromeInner" Property="BorderBrush"
									Value="{StaticResource ExpanderButtonOver_Border}" />
							<Setter TargetName="ChromeInner" Property="Background" 
									Value="{StaticResource ExpanderButtonOver_Background}" />
						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter TargetName="Chrome" Property="BorderBrush"
									Value="{StaticResource ExpandButtonPressed_OuterBorder}" />
							<Setter TargetName="ChromeInner" Property="BorderBrush"
									Value="{StaticResource ExpanderButtonPressed_Border}" />
							<Setter TargetName="ChromeInner" Property="Background"
									Value="{StaticResource ExpanderButtonPressed_Background}" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False" />
						<Trigger Property="IsChecked" Value="True">
							<Setter TargetName="ChromeCheck" Property="Visibility" Value="Collapsed"/>
						</Trigger>
					</ControlTemplate.Triggers>
					

				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<!-- NonEditableComboBoxTemplate -->
	<ControlTemplate x:Key="NonEditableComboBox" TargetType="telerikRibbonBar:RadRibbonComboBox">
		<Grid Name="MainGrid">
            

			<Border x:Name="ContentPresenterBorder" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition  Width="*"/>
						<ColumnDefinition Width="20" />
					</Grid.ColumnDefinitions>

					<telerik:PickerToggleButton Margin="-1" x:Name="PART_DropDownButton"
                                Grid.ColumnSpan="2"
								Style="{StaticResource ToogleButtonStyle}"
								IsChecked="{TemplateBinding IsDropDownOpen}"
								IsEnabled="{TemplateBinding IsEnabled}" />


					<ContentPresenter x:Name="ContentPresenterElement"
								HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
								Margin="{TemplateBinding Padding}"
								VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
								IsHitTestVisible="False"
								ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}"
								Content="{TemplateBinding SelectionBoxItem}" />
				</Grid>
			</Border>

			
			<Popup x:Name="PART_Popup" AllowsTransparency="True"
				   PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}" >
			
			
					
				<Border x:Name="DropDownBorder"
						HorizontalAlignment="Stretch"
						MinWidth="{TemplateBinding MinDropDownWidth}"
						MaxHeight="{TemplateBinding MaxDropDownHeight}">
					<Grid>
						<Border Background="{StaticResource RibbonPopupBackgroundBrush}" 
								BorderThickness="1"
                                BorderBrush="{StaticResource RibbonPopupOuterBorderBrush}" >
							<Border BorderBrush="{StaticResource RibbonPopupInnerBorderBrush}" BorderThickness="1">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <telerik:RadButton x:Name="PART_ClearButton" Grid.Row="0"
								            Margin="-1 -1 -1 0"
								            Visibility="{TemplateBinding ClearSelectionButtonVisibility}"
								            Content="{TemplateBinding ClearSelectionButtonContent}"
								            telerik:StyleManager.Theme="{StaticResource Theme}" />

                                    <ScrollViewer x:Name="PART_ScrollViewer" Padding="0 1" Grid.Row="1"
											VerticalScrollBarVisibility="Auto" BorderThickness="0"
                                            telerik:StyleManager.Theme="{StaticResource Theme}"
                                            
                                            
                                            CanContentScroll="True"
                                            >
                                        
                                        <ItemsPresenter />
                                    </ScrollViewer>
                                </Grid>
                            </Border>
						</Border>
					</Grid>
				</Border>
			
			</Popup>
			
			


			<Rectangle x:Name="DisabledVisual" Visibility="Collapsed"  
					Fill="{StaticResource DisabledBrush}" />
		</Grid>
		
		
		<ControlTemplate.Triggers>
			<Trigger Property="IsMouseOver" Value="True" />
			<Trigger Property="IsKeyboardFocusWithin" Value="True" />
			<Trigger Property="IsEnabled" Value="False">
				<Setter TargetName="DisabledVisual" Property="Visibility" Value="Visible" />
			</Trigger>
		</ControlTemplate.Triggers>
		
		
	</ControlTemplate>
	<!-- EditableComboBoxTemplate -->
	<ControlTemplate x:Key="EditableComboBox" TargetType="telerikRibbonBar:RadRibbonComboBox">
		<Grid>
            
			<Border x:Name="ContentPresenterBorder" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="*" />
						<ColumnDefinition Width="Auto" />
					</Grid.ColumnDefinitions>
					<telerik:PickerTextBox Grid.Column="0" Name="PART_EditableTextBox"
								Margin="{TemplateBinding Padding}"
								HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
								VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
								IsReadOnly="{TemplateBinding IsReadOnly}"/>

					<telerik:PickerToggleButton MinWidth="14" Margin="0,-1,-1,-1" x:Name="PART_DropDownButton" Grid.Column="1"
								Style="{StaticResource ToogleButtonStyle}"
								IsChecked="{TemplateBinding IsDropDownOpen}"
								IsEnabled="{TemplateBinding IsEnabled}" />

                    <TextBlock x:Name="Watermark" Text="{TemplateBinding EmptyText}" Opacity=".5" IsHitTestVisible="False" 
					   VerticalAlignment="Center" HorizontalAlignment="Left" Margin="5,0,0,0" Visibility="Collapsed" />
                </Grid>
			</Border>

			
			<Popup x:Name="PART_Popup" AllowsTransparency="True"
				   PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}" >
			
			
					
				<Border x:Name="DropDownBorder"
						HorizontalAlignment="Stretch"
						MinWidth="{TemplateBinding MinDropDownWidth}"
						MaxHeight="{TemplateBinding MaxDropDownHeight}">
					<Grid>
						<Border Background="{StaticResource RibbonPopupBackgroundBrush}" 
								BorderThickness="1"  
                                BorderBrush="{StaticResource RibbonPopupOuterBorderBrush}" >
							<Border BorderBrush="{StaticResource RibbonPopupInnerBorderBrush}" BorderThickness="1" >
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <telerik:RadButton x:Name="PART_ClearButton" Grid.Row="0"
								            Margin="-1 -1 -1 0"
								            Visibility="{TemplateBinding ClearSelectionButtonVisibility}"
								            Content="{TemplateBinding ClearSelectionButtonContent}"
								            telerik:StyleManager.Theme="{StaticResource Theme}" />

                                    <ScrollViewer x:Name="PART_ScrollViewer" Padding="0 1" Grid.Row="1"
											VerticalScrollBarVisibility="Auto" BorderThickness="0"	
                                            telerik:StyleManager.Theme="{StaticResource Theme}" 
                                            
                                            
                                            CanContentScroll="True"
                                             >

                                        <ItemsPresenter />
                                    </ScrollViewer>
                                </Grid>
                            </Border>
						</Border>
					</Grid>
				</Border>
			
			</Popup>
			
			

			<Rectangle x:Name="DisabledVisual" Visibility="Collapsed" Fill="{StaticResource DisabledBrush}" />
		</Grid>
		
		<ControlTemplate.Triggers>
			<Trigger Property="IsMouseOver" Value="True" >
				<Setter TargetName="ContentPresenterBorder" Property="Background" 
						Value="{StaticResource RibbonComboBoxHotBackgroundBrush}" />
				<Setter TargetName="ContentPresenterBorder" Property="BorderBrush"
						Value="{StaticResource RibbonComboBoxHotBorderBrush}" />
				<Setter TargetName="PART_DropDownButton" Property="IsEnabled" Value="True" />
				<Setter TargetName="PART_DropDownButton" Property="Background"
						Value="{StaticResource RibbonComboBoxButtonBackgroundBrush}" />
				<Setter TargetName="PART_DropDownButton" Property="BorderBrush"
						Value="{StaticResource RibbonComboBoxBorderBrush}" />
			</Trigger>
            <Trigger Property="IsWatermarkVisible" Value="True">
                <Setter TargetName="Watermark" Property="Visibility" Value="Visible" />
            </Trigger>
            <Trigger Property="IsKeyboardFocusWithin" Value="True" />
            <Trigger Property="IsEnabled" Value="False" >
                <Setter TargetName="DisabledVisual" Property="Visibility" Value="Visible" />
            </Trigger>
        </ControlTemplate.Triggers>
		
	</ControlTemplate>
	<!-- RadComboBox -->
	<Style TargetType="telerikRibbonBar:RadRibbonComboBox"
            x:Key="{telerik:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikRibbonBar:RadRibbonComboBox}"
        >
		<Setter Property="Background" Value="{StaticResource RibbonComboBoxBackgroundBrush}" />
		<Setter Property="BorderBrush" Value="{StaticResource RibbonComboBoxBorderBrush}" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="FontSize" Value="11" />
		<Setter Property="Foreground" Value="{StaticResource RibbonComboBoxForegroundBrush}" />
		<Setter Property="Padding" Value="3,2,3,2" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="ClearSelectionButtonContent" Value="Clear" />
        
        
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        
        <Setter Property="Template" Value="{StaticResource NonEditableComboBox}" />
		<Setter Property="EditableTemplate" Value="{StaticResource EditableComboBox}" />
		<Setter Property="NonEditableTemplate" Value="{StaticResource NonEditableComboBox}" />
		<Setter Property="MinHeight" Value="22" />
		<Setter Property="HorizontalContentAlignment" Value="Left" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
        
	</Style>
	<!-- RadComboBoxItem -->
	<Style TargetType="telerikRibbonBar:RadRibbonComboBoxItem"
            x:Key="{telerik:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikRibbonBar:RadRibbonComboBoxItem}"
        >
		<Setter Property="MinHeight" Value="17" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="Foreground" Value="#00156e" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="telerikRibbonBar:RadRibbonComboBoxItem">
					<Grid x:Name="LayoutRoot" MinHeight="{TemplateBinding MinHeight}">


						<Border x:Name="Bd" BorderBrush="{TemplateBinding BorderBrush}"
									BorderThickness="{TemplateBinding BorderThickness}"
									CornerRadius="{StaticResource ComboboxItemCornerRadius}" >

							<Border x:Name="InnerBd" BorderBrush="{TemplateBinding BorderBrush}"
									BorderThickness="1"
									Background="{TemplateBinding Background}" CornerRadius="{StaticResource ComboboxItemInnerCornerRadius}" >

								<ContentPresenter Margin="2,0,0,0" VerticalAlignment="Center" />
							</Border>
						</Border>

						<Rectangle x:Name="DisabledVisual" Visibility="Collapsed"
								Fill="{StaticResource DisabledBrush}" />
					</Grid>
					
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="True" >
							<Setter TargetName="Bd" Property="BorderBrush" 
									Value="{StaticResource ComboboxItemHotOuterBorderBrush}" />
							<Setter TargetName="InnerBd" Property="BorderBrush" 
									Value="{StaticResource ComboboxItemHotInnerBorderBrush}" />
