			</Setter.Value>
		</Setter>
	</Style>
	
	
	

    <Style TargetType="telerikRibbonBar:RadGallery"
        x:Key="{telerik:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikRibbonBar:RadGallery}"
        >
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="ItemWidth" Value="156"/>
        <Setter Property="ItemHeight" Value="26"/>
        <Setter Property="ViewportHeight" Value="184"/>
        <Setter Property="ViewportWidth" Value="157"/>        

        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                     <telerik:RadWrapPanel 
                        
                        
                        ItemWidth="{Binding RelativeSource={RelativeSource
						FindAncestor, AncestorType={x:Type telerikRibbonBar:RadRibbonGallery}},
						Path=ItemWidth}" ItemHeight="{Binding RelativeSource={RelativeSource
						FindAncestor, AncestorType={x:Type telerikRibbonBar:RadRibbonGallery}},
						Path=ItemHeight}"
                         />
                    
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadGallery">
                    <Grid >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <ScrollViewer x:Name="ScrollViewer"  
									  Grid.Row="0" IsTabStop="False" 
									  BorderThickness="0" Margin="1" 
									  Height="{TemplateBinding ViewportHeight}"
									  telerik:StyleManager.Theme="{StaticResource Theme}"
									  Padding="0">
                            <ItemsPresenter x:Name="presenter" Width="{TemplateBinding ViewportWidth}" HorizontalAlignment="Left" />
                        </ScrollViewer>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    
	

    <Style TargetType="telerikRibbonBar:RadGalleryItem"
            x:Key="{telerik:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikRibbonBar:RadGalleryItem}"
        >
        <Setter Property="Padding" Value="2" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonBar:RadGalleryItem">
                    <Grid SnapsToDevicePixels="True"  >
                        
                        <Border x:Name="buttonSurface" CornerRadius="1.5"
								BorderThickness="{TemplateBinding BorderThickness}"
								BorderBrush="{TemplateBinding BorderBrush}"
								Background="{TemplateBinding Background}" />
						
						<!-- Over -->
						<Border x:Name="mouseOverIndicator" CornerRadius="1.5" Opacity="0"
								BorderBrush="{StaticResource RadGalleryItemOver_border}"
