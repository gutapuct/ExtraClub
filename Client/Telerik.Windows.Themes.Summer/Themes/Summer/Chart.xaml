    <Style TargetType="telerikChart:RadChart"
        
        x:Key="{telerikControls:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikChart:RadChart}"
         >
        <Setter Property="Template" >
            <Setter.Value>
                <ControlTemplate TargetType="telerikChart:RadChart">
                    <Border BorderBrush="{TemplateBinding BorderBrush}"
                              BorderThickness="{TemplateBinding BorderThickness}">
                        <telerikCharting:ScalePanel MinFullWidth="{TemplateBinding MinFullWidth}" MinFullHeight="{TemplateBinding MinFullHeight}">
                            <Grid Background="{TemplateBinding Background}"
                                Margin="{TemplateBinding Padding}">
                                <Grid x:Name="PART_DefaultLayoutGrid" >
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <ContentPresenter x:Name="PART_DefaultChartTitleContainer" />
                                    <telerikControls:RadDockPanel LastChildFill="True" Grid.Row="1">
                                        <ContentPresenter x:Name="PART_DefaultChartLegendContainer" 
                                         telerikControls:RadDockPanel.Dock="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DefaultView.ChartLegendPosition}" />
                                        <ContentPresenter x:Name="PART_DefaultChartAreaContainer" />
                                    </telerikControls:RadDockPanel>
                                </Grid>
                                <ContentPresenter />
                            </Grid>
                        </telerikCharting:ScalePanel>
                    </Border>

                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Background" Value="{StaticResource ChartBackgroundBrush}" />
        <Setter Property="BorderBrush" Value="{StaticResource ChartBorderBrush}" />
        <Setter Property="BorderThickness" Value="{StaticResource ChartBorderThickness}" />
    </Style>

    <Style 
        
        x:Key="{telerikControls:ThemeResourceKey ThemeType=
        external:SummerThemeExternal , ElementType=telerikCharting:ChartArea}"
        
        TargetType="telerikCharting:ChartArea">
        <Setter Property="TransitionControlStyle">
            <Setter.Value>
                <Style TargetType="telerikControls:RadTransitionControl">
                    <Setter Property="Transition">
                        <Setter.Value>
                            <telerikEffects:LinearFadeTransition />
                        </Setter.Value>
                    </Setter>
                </Style>
            </Setter.Value>
        </Setter>
        <Setter Property="Template2D">
            <Setter.Value>
                <ControlTemplate TargetType="telerikCharting:ChartArea">
                    <Border Padding="{TemplateBinding Padding}"
                            Margin="{TemplateBinding Margin}"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="auto" />
                            </Grid.ColumnDefinitions>

                            <telerikCharting:AxisX2D x:Name="PART_AxisX" 
                                
                                Style="{TemplateBinding AxisXStyle}"  Grid.Row="2" Grid.Column="1" />

                                <telerikCharting:AxisY2D x:Name="PART_AxisY" 
                                
                                    Style="{TemplateBinding AxisYStyle}" Grid.Row="1" Grid.Column="0" />

                                    <telerikCharting:AdditionalAxes2DContainer x:Name="PART_AdditionalHorizontalAxesPanel"
                                                                               StackOrientation="Vertical"
                                                                               Grid.Row="0" Grid.Column="1"
                                                                                />
                                        <telerikCharting:AdditionalAxes2DContainer x:Name="PART_AdditionalVerticalAxesPanel"
                                                                                   StackOrientation="Horizontal"
                                                                                   Grid.Row="1" Grid.Column="2"
                                                                                    />

                                            <telerikControls:RadTransitionControl x:Name="PART_RadTransitionControl" 
                                                                                  Grid.Row="1" Grid.Column="1"
                                                                                  Style="{TemplateBinding TransitionControlStyle}">

                                                <telerikCharting:ClipPanel x:Name="PART_PlotAreaPanel" Style="{TemplateBinding PlotAreaStyle}">
                                                    <telerikCharting:HorizontalStripLines2D x:Name="PART_HorizontalStripLines"  />
                                                    <telerikCharting:VerticalStripLines2D x:Name="PART_VerticalStripLines"  />
                                                    <telerikCharting:AnnotationLayer x:Name="PART_AnnotationLayer"
                                                                                     ItemsSource="{TemplateBinding Annotations}" />
                                                    <telerikCharting:VerticalMinorGridLines2D x:Name="PART_VerticalMinorGridLines" />
                                                    <telerikCharting:HorizontalMinorGridLines2D x:Name="PART_HorizontalMinorGridLines" />
                                                    <telerikCharting:HorizontalGridLines2D x:Name="PART_HorizontalGridLines"  />
                                                    <telerikCharting:VerticalGridLines2D x:Name="PART_VerticalGridLines" />
                                                    <telerikCharting:AdditionalPlotAreaAxes2DContainer x:Name="PART_AdditionalPlotAreaHorizontalAxesPanel"
                                                                                                       StackOrientation="Vertical"
                                                                                                        />
                                                    <telerikCharting:AdditionalPlotAreaAxes2DContainer x:Name="PART_AdditionalPlotAreaVerticalAxesPanel"
                                                                                                       StackOrientation="Horizontal" 
                                                                                                        />
                                                    <telerikCharting:DragZoomLayerControl x:Name="PART_DragZoomLayer"
                                                                                          Style="{TemplateBinding DragZoomLayerControlStyle}">
                                                        <ItemsPresenter />
                                                    </telerikCharting:DragZoomLayerControl>
                                                    <telerikCharting:PlotAreaAxisY2D x:Name="PART_PlotAreaAxisY"
                                                                                     Style="{TemplateBinding PlotAreaAxisYStyle}" 
                                                                                      />
                                                    <telerikCharting:PlotAreaAxisX2D x:Name="PART_PlotAreaAxisX"
                                                                                     Style="{TemplateBinding PlotAreaAxisXStyle}"
                                                                                      />
                                                    <telerikCharting:LabelsPanel x:Name="PART_LabelsPanel" />
                                                </telerikCharting:ClipPanel>

                                            </telerikControls:RadTransitionControl>

                                            <telerikCharting:NoDataControl x:Name="PART_NoData" 
                                                Style="{TemplateBinding NoDataControlStyle}"  
                                                Grid.RowSpan="3" Grid.ColumnSpan="3" />
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        
        
        <Setter Property="Template3D">
            <Setter.Value>
                <ControlTemplate TargetType="telerikCharting:ChartArea">
                    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                        <ItemsPresenter />
                        <Grid x:Name="PART_Container"></Grid>
                        <telerikCharting:ViewportPanel x:Name="PART_CameraExtensionCanvas">
                            <Viewport3D x:Name="PART_ViewPort3D">
                                <Viewport3D.Camera>
                                    <PerspectiveCamera LookDirection="0.20,-0.23,-1" Position="-40,37.5,195" />
                                </Viewport3D.Camera>
                                <Viewport3D.Children>
                                    <ModelVisual3D>
                                        <ModelVisual3D.Content>
                                            <Model3DGroup>
                                                <AmbientLight Color="{StaticResource AmbientLight1Color}" />
                                                <DirectionalLight Direction="-0.5, -0.4, -1" Color="{StaticResource DirectionalLight1Color}" />
                                            </Model3DGroup>
                                        </ModelVisual3D.Content>
                                    </ModelVisual3D>
                                    <ContainerUIElement3D x:Name="PART_PlotArea" Transform="{StaticResource ChartAreaCenterTransform}">
                                        <telerikCharting:PlaneZX x:Name="PART_PlaneZX" 
                                           SecondaryTicksSource="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=AxisX.TickPoints}" 
                                            />
                                        <telerikCharting:PlaneXY x:Name="PART_PlaneXY" 
                                           PrimaryTicksSource="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=AxisX.TickPoints}" 
                                           SecondaryTicksSource="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=AxisY.TickPoints}" />
                                        <telerikCharting:PlaneYZ x:Name="PART_PlaneYZ" 
                                           PrimaryTicksSource="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=AxisY.TickPoints}" />
                                        <telerikCharting:ItemsPresenter3D x:Name="PART_ItemsPresenter3D" />
                                    </ContainerUIElement3D>
                                </Viewport3D.Children>
                            </Viewport3D>
                            <telerikCharting:LabelsControl x:Name="PART_LabelsControl"
                                                           ClipToBounds="True" >
                                <telerikCharting:LabelsControl.ItemTemplate>
                                    <DataTemplate>
                                        <telerikCharting:LabelLayer >
                                            <telerikCharting:LabelLayer.ItemTemplate>
                                                <DataTemplate >
                                                    <Border BorderBrush="{StaticResource LabelsBorderBrush}"
                                                        BorderThickness="1,1,1,1"
                                                        CornerRadius="3"
                                                        Background="{StaticResource LabelsBorderBackgroundBrush}"
                                                        LayoutTransform="{Binding Path=Transform}">
                                                        <TextBlock HorizontalAlignment="Center"
                                                           Text="{Binding Converter={StaticResource labelFormatConverter}}"
                                                           FontWeight="Bold"
                                                           Foreground="{StaticResource AxisLabel3DForeground}"
                                                           Margin="4,0,4,0" />
                                                    </Border>
                                                </DataTemplate>
                                            </telerikCharting:LabelLayer.ItemTemplate>
                                        </telerikCharting:LabelLayer>
                                    </DataTemplate>
                                </telerikCharting:LabelsControl.ItemTemplate>
                            </telerikCharting:LabelsControl>
                        </telerikCharting:ViewportPanel>
                        <telerikCharting:NoDataControl 
                            x:Name="PART_NoData" 
                            Style="{TemplateBinding NoDataControlStyle}" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        
        <Setter Property="ItemsPanel" >
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikCharting:ChartPanel />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Background" Value="{StaticResource ChartAreaBackground}" />
        <Setter Property="Padding" Value="5,10,10,5" />
    </Style>

    <Style 
        
